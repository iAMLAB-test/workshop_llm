            id:Pensioenwet-20250301-115-6,
            id:Pensioenwet-20250301-115-7,
            id:Pensioenwet-20250301-115-8,
            id:Pensioenwet-20250301-115-9 ;
        def:hasTitle "Het verantwoordingsorgaan"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115"@nl .

    id:Pensioenwet-20250301-115-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een ondernemingspensioenfonds of een bedrijfstakpensioenfonds met een paritair bestuur of een paritair gemengd dan wel omgekeerd gemengd bestuur stelt een verantwoordingsorgaan in. Een algemeen pensioenfonds met een paritair bestuur of een paritair gemengd dan wel omgekeerd gemengd bestuur stelt een verantwoordingsorgaan in voor elke collectiviteitkring."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 1"@nl .

    id:Pensioenwet-20250301-115-10 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=10&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het verantwoordingsorgaan heeft recht op overleg met het intern toezicht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 10"@nl .

    id:Pensioenwet-20250301-115-11 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=11&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur van het pensioenfonds en het verantwoordingsorgaan komen ten minste tweemaal per kalenderjaar in vergadering bijeen. Tijdens deze vergaderingen worden de aangelegenheden aan de orde gesteld waarover het bestuur van het pensioenfonds of het verantwoordingsorgaan overleg wenselijk acht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 11"@nl .

    id:Pensioenwet-20250301-115-12 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=12&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds verstrekt desgevraagd aan het verantwoordingsorgaan tijdig alle inlichtingen en gegevens, die deze voor de vervulling van zijn taak redelijkerwijze nodig heeft. De inlichtingen worden desgevraagd schriftelijk verstrekt."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 12"@nl .

    id:Pensioenwet-20250301-115-13 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=13&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien een leeftijdscohort voorafgaand aan het vaststellen van de risicohouding ondervertegenwoordigd is in het verantwoordingsorgaan, vraagt het pensioenfonds bij het uitvragen van de risicohouding de leden van dit leeftijdscohort actief zich aan te melden voor het verantwoordingsorgaan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 13"@nl .

    id:Pensioenwet-20250301-115-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In het verantwoordingsorgaan zijn de deelnemers en de pensioengerechtigden evenredig op basis van onderlinge getalsverhoudingen vertegenwoordigd. Bij een algemeen pensioenfonds wordt het voorgaande beoordeeld aan de hand van de onderlinge getalsverhoudingen binnen de collectiviteitkring waarvoor het verantwoordingsorgaan is ingesteld. De leden van het verantwoordingsorgaan vormen een zo evenwichtig mogelijke afspiegeling van de betreffende geleding. De werkgever kan vertegenwoordigd zijn in het verantwoordingsorgaan, indien de werkgever of de deelnemers en pensioengerechtigden dit wensen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 2"@nl .

    id:Pensioenwet-20250301-115-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0018831&artikel=1"^^xsd:string ;
        def:hasText "Indien een verantwoordingsorgaan van een algemeen pensioenfonds bij een collectiviteitkring hoort die bestaat uit meer dan een onderneming of groep, bedrijfstak of beroepspensioenregeling als bedoeld in artikel 1 van de Wet verplichte beroepspensioenregeling dan wel een combinatie hiervan, wordt iedere onderneming of groep, bedrijfstak dan wel beroepspensioenregeling door ten minste een deelnemer en een pensioengerechtigde vertegenwoordigd in het verantwoordingsorgaan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 3"@nl .

    id:Pensioenwet-20250301-115-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Op grond van door het bestuur van het pensioenfonds vast te stellen criteria kunnen naast de in het tweede lid bedoelde vertegenwoordigers ook één of meer vertegenwoordigers van gewezen deelnemers in het verantwoordingsorgaan zitting hebben."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 4"@nl .

    id:Pensioenwet-20250301-115-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In geval van verkiezing van leden van het verantwoordingsorgaan door deelnemers, gewezen deelnemers en pensioengerechtigden kunnen kandidaten worden voorgedragen door verenigingen en door individuele deelnemers, gewezen deelnemers en pensioengerechtigden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 5"@nl .

    id:Pensioenwet-20250301-115-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor zover geen verkiezing door de deelnemers, gewezen deelnemers en pensioengerechtigden plaatsvindt, maar de leden worden benoemd door verenigingen, zijn deze verenigingen evenredig aan hun ledenaantallen binnen hun geleding binnen het pensioenfonds vertegenwoordigd in het verantwoordingsorgaan. Bij een algemeen pensioenfonds wordt het voorgaande beoordeeld aan de hand van de ledenaantallen binnen de collectiviteitkring waarvoor het verantwoordingsorgaan is ingesteld."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 6"@nl .

    id:Pensioenwet-20250301-115-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een vereniging als bedoeld in het vijfde en zesde lid bezit volledige rechtsbevoegdheid; haar statutair doel omvat mede het behartigen van de belangen van haar leden als belanghebbenden bij een pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 7"@nl .

    id:Pensioenwet-20250301-115-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=8&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. op eigen initiatief van het pensioenfonds; of"@nl,
            "b. indien dit wordt verzocht door ten minste 1% van de deelnemers, gewezen deelnemers en pensioengerechtigden of door ten minste 500 deelnemers, gewezen deelnemers en pensioengerechtigden."@nl ;
        def:hasText "Het bestuur van het pensioenfonds gaat over tot verkiezing van de leden van het verantwoordingsorgaan die de deelnemers, gewezen deelnemers en pensioengerechtigden vertegenwoordigen:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 8"@nl .

    id:Pensioenwet-20250301-115-9 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115&lid=9&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een verantwoordingsorgaan van een algemeen pensioenfonds heeft uitsluitend de taken en bevoegdheden van het verantwoordingsorgaan voor zover ze betrekking hebben op de collectiviteitkring waarvoor het verantwoordingsorgaan is ingesteld. Het verantwoordingsorgaan stelt in overleg met het bestuur van het algemeen pensioenfonds een regeling vast ten aanzien van deze taken en bevoegdheden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115, lid 9"@nl .

    id:Pensioenwet-20250301-115a a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-115a-1,
            id:Pensioenwet-20250301-115a-2,
            id:Pensioenwet-20250301-115a-3,
            id:Pensioenwet-20250301-115a-4,
            id:Pensioenwet-20250301-115a-5,
            id:Pensioenwet-20250301-115a-6 ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115a"@nl .

    id:Pensioenwet-20250301-115a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur van het pensioenfonds legt verantwoording af aan het verantwoordingsorgaan over het beleid en de wijze waarop het is uitgevoerd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115a, lid 1"@nl .

    id:Pensioenwet-20250301-115a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115a&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het verantwoordingsorgaan heeft de bevoegdheid een oordeel te geven over het handelen van het bestuur aan de hand van het bestuursverslag, de jaarrekening en andere informatie, waaronder de bevindingen van het intern toezicht, over het door het bestuur uitgevoerde beleid, evenals over beleidskeuzes voor de toekomst waaronder de keuzes die van invloed zijn op de uitvoeringskosten. Dit oordeel wordt, samen met de reactie van het bestuur daarop, bekend gemaakt en in het bestuursverslag opgenomen. Het oordeel omvat in ieder geval een oordeel over de gemaakte uitvoeringskosten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115a, lid 2"@nl .

    id:Pensioenwet-20250301-115a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115a&lid=3&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het beleid inzake beloningen;"@nl,
            "b. de vorm en inrichting van het intern toezicht;"@nl,
            "c. de profielschets voor leden van de raad van toezicht;"@nl,
            "d. het vaststellen en wijzigen van een interne klachten- en geschillenprocedure;"@nl,
            "e. het vaststellen en wijzigen van het communicatie- en voorlichtingsbeleid;"@nl,
            "f. gehele of gedeeltelijke overdracht van de verplichtingen van het pensioenfonds of de overname van verplichtingen door het pensioenfonds;"@nl,
            "g. liquidatie, fusie of splitsing van het pensioenfonds;"@nl,
            "h. het sluiten, wijzigen of beëindigen van een uitvoeringsovereenkomst of het vaststellen, wijzigen of beëindigen van een uitvoeringsreglement; en"@nl,
            "i. het omzetten van het pensioenfonds in een andere rechtsvorm, bedoeld in artikel 18 van Boek 2 van het Burgerlijk Wetboek."@nl ;
        def:hasExtref "jci1.3:c:BWBR0003045&artikel=18"^^xsd:string ;
        def:hasText "Het pensioenfonds stelt het verantwoordingsorgaan in de gelegenheid advies uit te brengen over:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115a, lid 3"@nl .

    id:Pensioenwet-20250301-115a-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115a&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het verantwoordingsorgaan adviseert het bestuur naar aanleiding van de melding van disfunctioneren van het bestuur, bedoeld in artikel 104, vijfde lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115a, lid 4"@nl .

    id:Pensioenwet-20250301-115a-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115a&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het advies van het verantwoordingsorgaan wordt op een zodanig tijdstip gevraagd dat het van wezenlijke invloed kan zijn op de in het derde lid bedoelde besluiten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115a, lid 5"@nl .

    id:Pensioenwet-20250301-115a-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115a&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij het vragen van advies wordt aan het verantwoordingsorgaan een overzicht verstrekt van de beweegredenen voor het besluit en van de gevolgen die het besluit naar verwachting voor de deelnemers, gewezen deelnemers en pensioengerechtigden zal hebben."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115a, lid 6"@nl .

    id:Pensioenwet-20250301-115b a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-115b-1,
            id:Pensioenwet-20250301-115b-2,
            id:Pensioenwet-20250301-115b-3,
            id:Pensioenwet-20250301-115b-4,
            id:Pensioenwet-20250301-115b-5,
            id:Pensioenwet-20250301-115b-6 ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115b"@nl .

    id:Pensioenwet-20250301-115b-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115b&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een ondernemingspensioenfonds of een bedrijfstakpensioenfonds met een onafhankelijk bestuur of een onafhankelijk gemengd bestuur stelt een belanghebbendenorgaan in. Een algemeen pensioenfonds met een onafhankelijk of onafhankelijk gemengd bestuur stelt een belanghebbendenorgaan in voor elke collectiviteitkring. In een algemeen pensioenfonds met meerdere belanghebbendenorganen kunnen belanghebbendenorganen worden samengevoegd indien de betrokken belanghebbendenorganen hiermee instemmen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115b, lid 1"@nl .

    id:Pensioenwet-20250301-115b-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115b&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor de samenstelling van het belanghebbendenorgaan zijn de artikelen 100, eerste tot en met vijfde lid, en 102 van overeenkomstige toepassing. Bij een belanghebbendenorgaan voor een collectiviteitkring waarvan de pensioenregeling is beëindigd kan worden afgezien van vertegenwoordiging door de werkgever, indien de betrokken belanghebbenden in het belanghebbendenorgaan daarmee instemmen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115b, lid 2"@nl .

    id:Pensioenwet-20250301-115b-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115b&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een belanghebbendenorgaan van een algemeen pensioenfonds heeft uitsluitend de taken en bevoegdheden van het belanghebbendenorgaan voor zover ze betrekking hebben op de collectiviteitkring waarvoor het belanghebbendenorgaan is ingesteld. Indien belanghebbendenorganen zijn samengevoegd tot één belanghebbendenorgaan heeft dit belanghebbendenorgaan de taken en bevoegdheden van de afzonderlijke belanghebbendenorganen. Het belanghebbendenorgaan stelt in overleg met het bestuur van het algemeen pensioenfonds een regeling vast ten aanzien van deze taken en bevoegdheden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115b, lid 3"@nl .

    id:Pensioenwet-20250301-115b-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115b&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het belanghebbendenorgaan heeft recht op overleg met het intern toezicht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115b, lid 4"@nl .

    id:Pensioenwet-20250301-115b-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115b&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur van het pensioenfonds en het belanghebbendenorgaan komen ten minste tweemaal per kalenderjaar in vergadering bijeen. Tijdens deze vergaderingen worden de aangelegenheden aan de orde gesteld waarover het bestuur van het pensioenfonds of het belanghebbendenorgaan overleg wenselijk acht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115b, lid 5"@nl .

    id:Pensioenwet-20250301-115b-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115b&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds verstrekt desgevraagd aan het belanghebbendenorgaan tijdig alle inlichtingen en gegevens, die deze voor de vervulling van zijn taak redelijkerwijze nodig heeft. De inlichtingen worden desgevraagd schriftelijk verstrekt."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115b, lid 6"@nl .

    id:Pensioenwet-20250301-115c a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-115c-1,
            id:Pensioenwet-20250301-115c-10,
            id:Pensioenwet-20250301-115c-2,
            id:Pensioenwet-20250301-115c-3,
            id:Pensioenwet-20250301-115c-4,
            id:Pensioenwet-20250301-115c-5,
            id:Pensioenwet-20250301-115c-6,
            id:Pensioenwet-20250301-115c-7,
            id:Pensioenwet-20250301-115c-8,
            id:Pensioenwet-20250301-115c-9 ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c"@nl .

    id:Pensioenwet-20250301-115c-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het belanghebbendenorgaan adviseert het pensioenfonds desgevraagd of uit eigen beweging over aangelegenheden die het pensioenfonds betreffen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 1"@nl .

    id:Pensioenwet-20250301-115c-10 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=10&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het ontbreken van de goedkeuring van het belanghebbendenorgaan op een besluit als bedoeld in het achtste of negende lid tast de vertegenwoordigingsbevoegdheid van bestuur of bestuurders niet aan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 10"@nl .

    id:Pensioenwet-20250301-115c-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het nemen van maatregelen van algemene strekking;"@nl,
            "b. wijziging van de statuten en reglementen van het pensioenfonds;"@nl,
            "c. vaststelling van het bestuursverslag, de jaarrekening en de actuariële en bedrijfstechnische nota, bedoeld in artikel 145;"@nl,
            "d. het sluiten, wijzigen of beëindigen van een uitvoeringsovereenkomst of het vaststellen, wijzigen of beëindigen van een uitvoeringsreglement;"@nl,
            "e. een overeenkomst van uitbesteding;"@nl,
            "f. het beleid inzake beloningen;"@nl,
            "g. de vorm en inrichting van het intern toezicht;"@nl,
            "h. de profielschets voor leden van de raad van toezicht;"@nl,
            "i. het vaststellen en wijzigen van een interne klachten- en geschillenprocedure; en"@nl,
            "j. het vaststellen en wijzigen van het communicatie- en voorlichtingsbeleid."@nl ;
        def:hasText "Het pensioenfonds stelt het belanghebbendenorgaan in ieder geval in de gelegenheid advies uit te brengen over elk voorgenomen besluit van het pensioenfonds met betrekking tot:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 2"@nl .

    id:Pensioenwet-20250301-115c-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het advies van het belanghebbendenorgaan wordt op een zodanig tijdstip gevraagd dat het van wezenlijke invloed kan zijn op de in het tweede lid bedoelde besluiten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 3"@nl .

    id:Pensioenwet-20250301-115c-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het belanghebbendenorgaan adviseert het bestuur naar aanleiding van de melding van disfunctioneren van het bestuur, bedoeld in artikel 104, vijfde lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 4"@nl .

    id:Pensioenwet-20250301-115c-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij het vragen van advies wordt aan het belanghebbendenorgaan een overzicht verstrekt van de beweegredenen voor het besluit en van de gevolgen die het besluit naar verwachting voor de deelnemers, gewezen deelnemers en pensioengerechtigden zal hebben."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 5"@nl .

    id:Pensioenwet-20250301-115c-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur van het pensioenfonds legt verantwoording af aan het belanghebbendenorgaan over het beleid en de wijze waarop het is uitgevoerd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 6"@nl .

    id:Pensioenwet-20250301-115c-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het belanghebbendenorgaan heeft de bevoegdheid een oordeel te geven over het handelen van het bestuur aan de hand van het bestuursverslag, de jaarrekening en andere informatie, waaronder de bevindingen van het intern toezicht, over het door het bestuur uitgevoerde beleid, evenals over beleidskeuzes voor de toekomst waaronder de keuzes die van invloed zijn op de uitvoeringskosten. Dit oordeel wordt, samen met de reactie van het bestuur daarop, bekend gemaakt en in het bestuursverslag opgenomen. Het oordeel omvat in ieder geval een oordeel over de gemaakte uitvoeringskosten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 7"@nl .

    id:Pensioenwet-20250301-115c-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Besluiten van het bestuur kunnen bij of krachtens de statuten worden onderworpen aan de goedkeuring van het belanghebbendenorgaan, voor zover uit de wet niet anders voortvloeit. De statuten voorzien in een regeling voor geschillen over goedkeuring van besluiten door het belanghebbendenorgaan. Bij het vragen van goedkeuring is het vijfde lid van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 8"@nl .

    id:Pensioenwet-20250301-115c-9 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115c&lid=9&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. gehele of gedeeltelijke overdracht van de verplichtingen van het pensioenfonds of de overname van verplichtingen door het pensioenfonds;"@nl,
            "b. liquidatie, fusie of splitsing van het pensioenfonds;"@nl,
            "c. het omzetten van het pensioenfonds in een andere rechtsvorm, bedoeld in artikel 18 van Boek 2 van het Burgerlijk Wetboek;"@nl,
            "d. wijziging van de collectiviteitkring;"@nl,
            "e. het strategische beleggingsbeleid;"@nl,
            "f. de solidariteitsreserve of risicodelingsreserve;"@nl,
            "g. de toedelingsregels;"@nl,
            "h. het vaststellen en wijzigen van het toeslagbeleid;"@nl,
            "i. vaststelling van een herstelplan als bedoeld in artikel 138 of artikel 139;"@nl,
            "j. het terugstorten van premie, bedoeld in artikel 129; en"@nl,
            "k. vermindering van de verworven pensioenaanspraken en pensioenrechten indien toepassing wordt gegeven aan artikel 134."@nl ;
        def:hasExtref "jci1.3:c:BWBR0003045&artikel=18"^^xsd:string ;
        def:hasText "Het bestuur heeft in ieder geval goedkeuring nodig van het belanghebbendenorgaan voor elk voorgenomen besluit met betrekking tot:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115c, lid 9"@nl .

    id:Pensioenwet-20250301-115d a def:LawArticle ;
        def:hasDescription " In de statuten van het pensioenfonds kunnen aan het verantwoordingsorgaan of het belanghebbendenorgaan verdere bevoegdheden dan de in deze wet genoemde worden toegekend."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115d"@nl .

    id:Pensioenwet-20250301-115e a def:LawArticle ;
        def:hasDescription " Het pensioenfonds deelt het verantwoordingsorgaan of het belanghebbendenorgaan zo spoedig mogelijk schriftelijk mee, of het een advies niet of niet geheel volgt, waarbij tevens wordt meegedeeld waarom van het advies of van een daarin vervat minderheidsadvies wordt afgeweken."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115e"@nl .

    id:Pensioenwet-20250301-115f a def:LawArticle ;
        def:hasDescription " Een pensioenfonds informeert het verantwoordingsorgaan of het belanghebbendenorgaan onverwijld schriftelijk over:"@nl ;
        def:hasSection id:Pensioenwet-20250301-115f-1,
            id:Pensioenwet-20250301-115f-2,
            id:Pensioenwet-20250301-115f-3 ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115f"@nl .

    id:Pensioenwet-20250301-115f-1 a def:LawArticleSection ;
        def:hasText "a. de verplichting tot opstelling van een herstelplan als bedoeld in artikel 138 of artikel 139;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115f, Deel 1"@nl .

    id:Pensioenwet-20250301-115f-2 a def:LawArticleSection ;
        def:hasText "b. de aanstelling van een bewindvoerder als bedoeld in artikel 173; en"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115f, Deel 2"@nl .

    id:Pensioenwet-20250301-115f-3 a def:LawArticleSection ;
        def:hasText "c. de beëindiging van de situatie, bedoeld in artikel 172, waarin de bevoegdheiduitoefening van alle of bepaalde organen van een pensioenfonds is gebonden aan toestemming van een of meer door de toezichthouder aangewezen personen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115f, Deel 3"@nl .

    id:Pensioenwet-20250301-115g a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-115g-1,
            id:Pensioenwet-20250301-115g-2 ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115g"@nl .

    id:Pensioenwet-20250301-115g-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115g&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds staat de leden van het verantwoordingsorgaan of het belanghebbendenorgaan het gebruik toe van de voorzieningen waarover het kan beschikken, voor zover dat redelijkerwijs voor de vervulling van hun taken nodig is."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115g, lid 1"@nl .

    id:Pensioenwet-20250301-115g-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.2&artikel=115g&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002747&artikel=10"^^xsd:string,
            "jci1.3:c:BWBR0002747&artikel=16"^^xsd:string,
            "jci1.3:c:BWBR0002747&artikel=17"^^xsd:string,
            "jci1.3:c:BWBR0002747&artikel=18"^^xsd:string,
            "jci1.3:c:BWBR0002747&artikel=22"^^xsd:string ;
        def:hasText "Ten aanzien van de rechten en bevoegdheden van het verantwoordingsorgaan of het belanghebbendenorgaan in verhouding tot het bestuur van het pensioenfonds zijn de artikelen 10, 16, 17, 18 en 22 van de Wet op de ondernemingsraden van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115g, lid 2"@nl .

    id:Pensioenwet-20250301-115h a def:LawArticle ;
        def:hasDescription " Een pensioenfonds is gehouden om op verzoek van werknemers of een werknemersvereniging mee te werken aan de verstrekking van informatie aan de werknemers van het fonds over het voornemen tot oprichting, of over het bestaan, van een werknemersvereniging. Een pensioenfonds is tevens gehouden om op verzoek van pensioengerechtigden of een vereniging van pensioengerechtigden mee te werken aan de verstrekking van informatie aan de pensioengerechtigden van het fonds over het voornemen tot oprichting, of over het bestaan, van een vereniging van pensioengerechtigden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 115h"@nl .

    id:Pensioenwet-20250301-116 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-116-1,
            id:Pensioenwet-20250301-116-2 ;
        def:hasTitle "Verbod van nevenactiviteiten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 116"@nl .

    id:Pensioenwet-20250301-116-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=116&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds verricht slechts activiteiten in verband met pensioen en werkzaamheden die daarmee verband houden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 116, lid 1"@nl .

    id:Pensioenwet-20250301-116-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=116&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen regels worden gesteld ter zake van activiteiten die door pensioenfondsen kunnen worden verricht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 116, lid 2"@nl .

    id:Pensioenwet-20250301-117 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-117-1,
            id:Pensioenwet-20250301-117-2,
            id:Pensioenwet-20250301-117-3,
            id:Pensioenwet-20250301-117-4,
            id:Pensioenwet-20250301-117-5,
            id:Pensioenwet-20250301-117-6,
            id:Pensioenwet-20250301-117-7 ;
        def:hasTitle "Uitvoering vrijwillige pensioenregeling"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117"@nl .

    id:Pensioenwet-20250301-117-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=117&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds kan uitsluitend een vrijwillige pensioenregeling uitvoeren indien dit een aanvulling op een door datzelfde pensioenfonds uitgevoerde basispensioenregeling betreft."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117, lid 1"@nl .

    id:Pensioenwet-20250301-117-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=117&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds kan een fiscaal bovenmatige pensioenregeling uitsluitend uitvoeren als vrijwillige pensioenregeling."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117, lid 2"@nl .

    id:Pensioenwet-20250301-117-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=117&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002471&hoofdstuk=IIB"^^xsd:string,
            "jci1.3:c:BWBR0002471&hoofdstuk=VIII"^^xsd:string ;
        def:hasText "Onder een fiscaal bovenmatige pensioenregeling als bedoeld in het tweede lid wordt verstaan: een pensioenregeling die niet blijft binnen de begrenzingen die zijn opgenomen in de hoofdstukken IIB en VIII van de Wet op de loonbelasting 1964."@nl ;
        def:referencesTo id:WetOpLoonbelasting1964-20250101 ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117, lid 3"@nl .

    id:Pensioenwet-20250301-117-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=117&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002471"^^xsd:string,
            "jci1.3:c:BWBR0011353&artikel=3.18"^^xsd:string,
            "jci1.3:c:BWBR0011353&artikel=3.95"^^xsd:string ;
        def:hasText "Voor de toepassing van het derde lid zijn de artikelen 3.18, vierde en vijfde lid, en 3.95, eerste lid, tweede volzin, van de Wet inkomstenbelasting 2001 van overeenkomstige toepassing voor zover aan de pensioenregeling wordt deelgenomen door deelnemers die niet worden aangemerkt als werknemer in de zin van de Wet op de loonbelasting 1964."@nl ;
        def:referencesTo id:WetOpLoonbelasting1964-20250101 ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117, lid 4"@nl .

    id:Pensioenwet-20250301-117-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=117&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij een algemeen pensioenfonds kan de vrijwillige pensioenregeling die in aanvulling op de basispensioenregeling wordt uitgevoerd deel uitmaken van een ander afgescheiden vermogen dan deze basispensioenregeling."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117, lid 5"@nl .

    id:Pensioenwet-20250301-117-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=117&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur kunnen nadere regels worden gesteld met betrekking tot het bepaalde in het vijfde lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117, lid 6"@nl .

    id:Pensioenwet-20250301-117-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=117&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De voordracht voor een krachtens het zesde lid vast te stellen algemene maatregel van bestuur wordt niet eerder gedaan dan vier weken nadat het ontwerp aan beide kamers der Staten-Generaal is overgelegd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117, lid 7"@nl .

    id:Pensioenwet-20250301-117a a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-117a-1,
            id:Pensioenwet-20250301-117a-2 ;
        def:hasTitle "Uitvoering nettopensioen door pensioenfonds"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117a"@nl .

    id:Pensioenwet-20250301-117a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=117a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur worden regels gesteld over de uitvoering van een nettopensioen door een pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117a, lid 1"@nl .

    id:Pensioenwet-20250301-117a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=117a&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De voordracht voor een krachtens het eerste lid vast te stellen algemene maatregel van bestuur wordt niet eerder gedaan dan vier weken nadat het ontwerp aan beide kamers der Staten-Generaal is overgelegd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 117a, lid 2"@nl .

    id:Pensioenwet-20250301-118 a def:LawArticle ;
        def:hasDescription " Vervallen"@nl ;
        def:hasTitle "Eisen uitkeringsovereenkomsten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 118"@nl .

    id:Pensioenwet-20250301-119 a def:LawArticle ;
        def:hasDescription " Vervallen"@nl ;
        def:hasTitle "Eisen kapitaalovereenkomsten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 119"@nl .

    id:Pensioenwet-20250301-12 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-12-1,
            id:Pensioenwet-20250301-12-2 ;
        def:hasTitle "Betalingsvoorbehoud"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 12"@nl .

    id:Pensioenwet-20250301-12-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=12&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De werkgever kan zich bij het sluiten of bij een wijziging van de pensioenovereenkomst het recht voorbehouden de premiebetaling, voorzover deze betrekking heeft op de bijdrage van de werkgever, te verminderen of beëindigen in geval van een ingrijpende wijziging van omstandigheden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 12, lid 1"@nl .

    id:Pensioenwet-20250301-12-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=12&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Andere bedingen dan die bedoeld in het eerste lid, waarin een voorbehoud wordt gemaakt inzake premiebetaling, zijn nietig."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 12, lid 2"@nl .

    id:Pensioenwet-20250301-120 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-120-1,
            id:Pensioenwet-20250301-120-2,
            id:Pensioenwet-20250301-120-3,
            id:Pensioenwet-20250301-120-4 ;
        def:hasTitle "Eisen premieovereenkomsten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 120"@nl .

    id:Pensioenwet-20250301-120-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=120&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor een basispensioenregeling in de vorm van een premieovereenkomst welke wordt uitgevoerd door een pensioenfonds geldt dat de werkgeverspremie ten minste 10 procent van de premie van de basispensioenregeling bedraagt."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 120, lid 1"@nl .

    id:Pensioenwet-20250301-120-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=120&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. dat de werkgever ten minste 10 procent van de premie van de vrijwillige pensioenregeling bijdraagt; of"@nl,
            "b. dat op de vrijwillige pensioenregeling in beginsel dezelfde regels en voorwaarden van toepassing zijn als op de basispensioenregeling en de basispensioenregeling een solidaire premieovereenkomst betreft of een flexibele premieovereenkomst met een risicodelingsreserve."@nl ;
        def:hasText "Voor een vrijwillige pensioenregeling in de vorm van een premieovereenkomst welke wordt uitgevoerd door een pensioenfonds geldt:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 120, lid 2"@nl .

    id:Pensioenwet-20250301-120-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=120&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien de vrijwillige pensioenregeling voldoet aan de voorwaarde, bedoeld in het tweede lid, onderdeel a, deelt het pensioenfonds dit schriftelijk mee aan de deelnemers en de toezichthouder."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 120, lid 3"@nl .

    id:Pensioenwet-20250301-120-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=120&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het eerste lid is niet van toepassing op een verplichtgesteld bedrijfstakpensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 120, lid 4"@nl .

    id:Pensioenwet-20250301-121 a def:LawArticle ;
        def:hasDescription " Een bedrijfstakpensioenfonds kan een uitvoeringsovereenkomst sluiten met een werkgever die niet onder de werkingssfeer van het bedrijfstakpensioenfonds valt, maar die zich op vrijwillige basis bij het bedrijfstakpensioenfonds wil aansluiten, indien:"@nl ;
        def:hasSection id:Pensioenwet-20250301-121-1,
            id:Pensioenwet-20250301-121-2,
            id:Pensioenwet-20250301-121-3 ;
        def:hasTitle "Vrijwillige aansluiting bij een bedrijfstakpensioenfonds"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 121"@nl .

    id:Pensioenwet-20250301-121-1 a def:LawArticleSection ;
        def:hasText "a. de loonontwikkeling bij deze werkgever ten minste gelijk is aan die in een bedrijfstak waarin het bedrijfstakpensioenfonds werkzaam is en de werkgever deelneemt in de sociale fondsen van dezelfde bedrijfstak;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 121, Deel 1"@nl .

    id:Pensioenwet-20250301-121-2 a def:LawArticleSection ;
        def:hasText "b. er sprake is van een groepsverhouding tussen de werkgever die zich vrijwillig wil aansluiten en een andere werkgever die onder de werkingssfeer van het bedrijfstakpensioenfonds valt; of"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 121, Deel 2"@nl .

    id:Pensioenwet-20250301-121-3 a def:LawArticleSection ;
        def:hasText "c. dit aansluitend gebeurt aan een periode waarin de werkgever wel onder de werkingssfeer van het bedrijfstakpensioenfonds viel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 121, Deel 3"@nl .

    id:Pensioenwet-20250301-121a a def:LawArticle ;
        def:hasDescription """
    Artikel 121 is van overeenkomstige toepassing bij uitbreiding van de werkingssfeer van een verplichtgesteld bedrijfstakpensioenfonds met een bedrijfstak of deel van een bedrijfstak waarbij deelneming aan het bedrijfstakpensioenfonds voor in die bedrijfstak werkzame personen niet verplicht is gesteld."""@nl ;
        def:hasTitle "Uitbreiding werkingssfeer verplichtgesteld bedrijfstakpensioenfonds"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 121a"@nl .

    id:Pensioenwet-20250301-122 a def:LawArticle ;
        def:hasDescription " Indien een onderneming niet langer deel uitmaakt van een groep waaraan een ondernemingspensioenfonds verbonden is, of een groep die valt onder de werkingssfeer van een bedrijfstakpensioenfonds, kan dit pensioenfonds blijven optreden als pensioenuitvoerder voor deze onderneming, tenzij de onderneming onder de werkingssfeer van een verplichtgesteld bedrijfstakpensioenfonds valt."@nl ;
        def:hasTitle "Beëindiging verbondenheid met groep"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 122"@nl .

    id:Pensioenwet-20250301-123 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-123-1,
            id:Pensioenwet-20250301-123-2,
            id:Pensioenwet-20250301-123-3,
            id:Pensioenwet-20250301-123-4,
            id:Pensioenwet-20250301-123-5 ;
        def:hasTitle "Uitvoeren van meerdere pensioenregelingen en rangregeling"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 123"@nl .

    id:Pensioenwet-20250301-123-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=123&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien een ondernemingspensioenfonds of een bedrijfstakpensioenfonds meerdere pensioenregelingen uitvoert vormen deze pensioenregelingen financieel een geheel, tenzij toepassing wordt gegeven aan artikel 150l, zevende lid. Een algemeen pensioenfonds houdt een afgescheiden vermogen aan voor iedere collectiviteitkring, waarbij ten aanzien van een collectiviteitkring toepassing kan worden gegeven aan artikel 150l, zevende lid. "@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 123, lid 1"@nl .

    id:Pensioenwet-20250301-123-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=123&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De werkingssfeer van de collectiviteitkring, bedoeld in het eerste lid, wordt vastgelegd in de statuten door omschrijving van de pensioenregelingen en vermelding van de uitvoeringsovereenkomsten of uitvoeringsreglementen die onderdeel uitmaken van de collectiviteitkring."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 123, lid 2"@nl .

    id:Pensioenwet-20250301-123-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=123&lid=3&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. kosten die verband houden met de uitvoering van de pensioenregeling die volgens de uitvoeringsovereenkomst of het uitvoeringsreglement ten laste kunnen worden gebracht van het vermogen; en"@nl,
            "b. pensioenaanspraken en pensioenrechten van deelnemers, gewezen deelnemers, andere aanspraakgerechtigden en pensioengerechtigden bij dat vermogen."@nl ;
        def:hasText "Het vermogen voor een collectiviteitkring is een afgescheiden vermogen dat, onverminderd artikel 129 en het vijfde lid, uitsluitend dient tot voldoening van vorderingen die voortvloeien uit:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 123, lid 3"@nl .

    id:Pensioenwet-20250301-123-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=123&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien het afgescheiden vermogen bij vereffening ontoereikend is voor voldoening van de vorderingen, dient het vermogen ter voldoening van de vorderingen in de volgorde van het derde lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 123, lid 4"@nl .

    id:Pensioenwet-20250301-123-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=123&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0001860"^^xsd:string ;
        def:hasText "In geval van een faillietverklaring van een algemeen pensioenfonds worden de boedelschulden, overeenkomstig de bepalingen van de Faillissementswet, al naar gelang de aard van de betrokken boedelschuld hetzij omgeslagen over ieder deel van de boedel, hetzij uitsluitend van een bepaalde bate van de boedel afgetrokken."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 123, lid 5"@nl .

    id:Pensioenwet-20250301-124 a def:LawArticle ;
        def:hasDescription " Een pensioenfonds kan de mogelijkheid bieden tot verhoging van de pensioenaanspraken indien het deel van de pensioenaanspraken dat voortvloeit uit deze inkoop overeenkomstig de pensioenaanspraken op grond van de basispensioenregeling wordt behandeld."@nl ;
        def:hasTitle "Inkoop van pensioenopbouw"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 124"@nl .

    id:Pensioenwet-20250301-124a a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-124a-1,
            id:Pensioenwet-20250301-124a-2,
            id:Pensioenwet-20250301-124a-3 ;
        def:hasTitle "Aanstelling van een pensioenbewaarder"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 124a"@nl .

    id:Pensioenwet-20250301-124a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=124a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds draagt de eigendom van een pensioenvermogen slechts over aan een pensioenbewaarder nadat hij met deze een overeenkomst inzake het beheer en de bewaring van het pensioenvermogen heeft gesloten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 124a, lid 1"@nl .

    id:Pensioenwet-20250301-124a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=124a&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds treft maatregelen opdat de pensioenbewaarder slechts met zijn medewerking over de bestanddelen van het pensioenvermogen zal beschikken."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 124a, lid 2"@nl .

    id:Pensioenwet-20250301-124a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=124a&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur worden regels gesteld met betrekking tot de inhoud van de overeenkomst, bedoeld in het eerste lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 124a, lid 3"@nl .

    id:Pensioenwet-20250301-125 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-125-1,
            id:Pensioenwet-20250301-125-2 ;
        def:hasTitle "Vergunning en kennisgeving grensoverschrijdende activiteit"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 125"@nl .

    id:Pensioenwet-20250301-125-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=125&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. zonder een daartoe door de toezichthouder verleende vergunning; en"@nl,
            "b. zonder de toezichthouder van het voornemen daartoe in kennis te hebben gesteld, op de wijze, bedoeld in artikel 194, en met inachtneming van artikel 196."@nl ;
        def:hasText "Het is een pensioenfonds verboden bijdragen te ontvangen van een bijdragende onderneming die zetel heeft in een andere lidstaat dan Nederland:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 125, lid 1"@nl .

    id:Pensioenwet-20250301-125-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.3&artikel=125&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor de toepassing van het eerste lid, artikel 138, vierde lid, artikel 140, tweede lid, en de paragrafen 7.4.2 en 7.4.3 wordt onder het ontvangen van bijdragen verstaan het ontvangen van bijdragen voor de uitvoering van een pensioenregeling die afkomstig is uit een andere lidstaat dan de lidstaat waar het pensioenfonds gevestigd is."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 125, lid 2"@nl .

    id:Pensioenwet-20250301-125a a def:LawArticle ;
        def:hasDescription " Indien een algemeen pensioenfonds afgescheiden vermogens aanhoudt, wordt hetgeen bij of krachtens de artikelen 126, 129 tot en met 142, 143, voor zover het de haalbaarheidstoets betreft, 145, 147, 149 en 150 is bepaald, toegepast per afgescheiden vermogen."@nl ;
        def:hasTitle "Financieel toetsingskader bij afgescheiden vermogens"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 125a"@nl .

    id:Pensioenwet-20250301-125b a def:LawArticle ;
        def:hasDescription " Vervallen"@nl ;
        def:hasTitle "Inbreng gehele vermogen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 125b"@nl .

    id:Pensioenwet-20250301-126 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-126-1,
            id:Pensioenwet-20250301-126-2,
            id:Pensioenwet-20250301-126-3 ;
        def:hasTitle "Vaststelling technische voorzieningen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 126"@nl .

    id:Pensioenwet-20250301-126-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=126&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds stelt toereikende technische voorzieningen vast met betrekking tot het geheel van pensioenverplichtingen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 126, lid 1"@nl .

    id:Pensioenwet-20250301-126-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=126&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. de technische voorzieningen worden berekend op basis van marktwaardering;"@nl,
            "b. de voor de berekening van de technische voorzieningen gebruikte grondslagen inzake overlijden of arbeidsongeschiktheid en levensverwachting worden gebaseerd op prudente beginselen; en"@nl,
            "c. de methode en de grondslag van de berekening van de technische voorzieningen blijven van boekjaar tot boekjaar ongewijzigd, tenzij wijzigingen daarin gerechtvaardigd zijn als gevolg van een verandering van de juridische, demografische of economische omstandigheden die aan de hypothesen ten grondslag liggen."@nl ;
        def:hasText "De berekening wordt uitgevoerd met inachtneming van de volgende beginselen:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 126, lid 2"@nl .

    id:Pensioenwet-20250301-126-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=126&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden regels gesteld over de wijze van berekening van het minimum bedrag van de technische voorzieningen, de daarbij in acht te nemen voorzichtigheidsmarges en kunnen regels worden gesteld over de frequentie waarmee de technische voorzieningen worden berekend."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 126, lid 3"@nl .

    id:Pensioenwet-20250301-127 a def:LawArticle ;
        def:hasDescription " Ouderdomspensioen wordt gefinancierd op basis van kapitaaldekking."@nl ;
        def:hasTitle "Financiering ouderdomspensioen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 127"@nl .

    id:Pensioenwet-20250301-128 a def:LawArticle ;
        def:hasDescription " Vervallen"@nl ;
        def:hasTitle "Hoogte kostendekkende premie"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 128"@nl .

    id:Pensioenwet-20250301-129 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-129-1,
            id:Pensioenwet-20250301-129-2 ;
        def:hasTitle "Terugstorting"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 129"@nl .

    id:Pensioenwet-20250301-129-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=129&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. gezien de beleidsdekkingsgraad ten aanzien van de pensioenverplichtingen wordt voldaan aan de artikelen 126, 132 en 133;"@nl,
            "b. de voorwaardelijke toeslagen zowel met betrekking tot de voorgaande tien jaar zijn verleend als ook in de toekomst kunnen worden verleend; en"@nl,
            "c. de korting op de pensioenaanspraken en pensioenrechten op grond van artikel 134 in de voorgaande tien jaar gecompenseerd is."@nl ;
        def:hasText "Een pensioenfonds kan uitsluitend terugstorten indien:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 129, lid 1"@nl .

    id:Pensioenwet-20250301-129-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=129&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen nadere regels worden gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 129, lid 2"@nl .

    id:Pensioenwet-20250301-13 a def:LawArticle ;
        def:hasDescription " Vervallen"@nl ;
        def:hasTitle "Verlening van toeslagen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 13"@nl .

    id:Pensioenwet-20250301-130 a def:LawArticle ;
        def:hasDescription " Vervallen"@nl ;
        def:hasTitle "Vermelding premie in jaarrekening en bestuursverslag"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 130"@nl .

    id:Pensioenwet-20250301-130a a def:LawArticle ;
        def:hasDescription " Vervallen"@nl ;
        def:hasTitle "Vermelding premiecomponenten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 130a"@nl .

    id:Pensioenwet-20250301-131 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-131-1,
            id:Pensioenwet-20250301-131-2 ;
        def:hasTitle "Minimaal vereist eigen vermogen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 131"@nl .

    id:Pensioenwet-20250301-131-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=131&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een pensioenfonds tot volledige overdracht of onderbrenging is overgegaan; en"@nl,
            "b. de toezichthouder heeft ingestemd met het feit dat het pensioenfonds daarom niet beschikt over een minimaal vereist eigen vermogen."@nl ;
        def:hasText "Een pensioenfonds beschikt over een minimaal vereist eigen vermogen, tenzij:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 131, lid 1"@nl .

    id:Pensioenwet-20250301-131-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=131&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden de omvang en de samenstelling van het minimaal vereist eigen vermogen bepaald."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 131, lid 2"@nl .

    id:Pensioenwet-20250301-132 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-132-1,
            id:Pensioenwet-20250301-132-2,
            id:Pensioenwet-20250301-132-3 ;
        def:hasTitle "Vereist eigen vermogen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 132"@nl .

    id:Pensioenwet-20250301-132-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=132&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds beschikt over een vereist eigen vermogen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 132, lid 1"@nl .

    id:Pensioenwet-20250301-132-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=132&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds stelt het vereist eigen vermogen zodanig vast dat met een zekerheid van 971/2 procent wordt voorkomen dat het pensioenfonds binnen een periode van één jaar over minder waarden beschikt dan de hoogte van de technische voorzieningen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 132, lid 2"@nl .

    id:Pensioenwet-20250301-132-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=132&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld inzake de berekening en de samenstelling van het vereist eigen vermogen, bedoeld in het eerste lid, en het bepaalde in het tweede lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 132, lid 3"@nl .

    id:Pensioenwet-20250301-133 a def:LawArticle ;
        def:hasDescription " De technische voorzieningen en de aan het pensioenfonds verstrekte leningen worden volledig door waarden gedekt."@nl ;
        def:hasTitle "Dekking door waarden"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 133"@nl .

    id:Pensioenwet-20250301-133a a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-133a-1,
            id:Pensioenwet-20250301-133a-2 ;
        def:hasTitle "Beleidsdekkingsgraad"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 133a"@nl .

    id:Pensioenwet-20250301-133a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=133a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De beleidsdekkingsgraad van een pensioenfonds is de gemiddelde dekkingsgraad van de 12 maanden voorafgaand aan het moment van vaststelling."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 133a, lid 1"@nl .

    id:Pensioenwet-20250301-133a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=133a&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot de beleidsdekkingsgraad en het moment van vaststelling van de beleidsdekkingsgraad."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 133a, lid 2"@nl .

    id:Pensioenwet-20250301-133b a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-133b-1,
            id:Pensioenwet-20250301-133b-2 ;
        def:hasTitle "Melding reële dekkingsgraad aan toezichthouder"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 133b"@nl .

    id:Pensioenwet-20250301-133b-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=133b&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds is verplicht om jaarlijks de hoogte van de reële dekkingsgraad van het pensioenfonds aan de toezichthouder te melden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 133b, lid 1"@nl .

    id:Pensioenwet-20250301-133b-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=133b&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij regeling van Onze Minister worden regels gesteld over de reële dekkingsgraad."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 133b, lid 2"@nl .

    id:Pensioenwet-20250301-134-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=134&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het pensioenfonds gezien de beleidsdekkingsgraad niet voldoet aan de bij of krachtens artikel 131 gestelde eisen ten aanzien van het minimaal vereist eigen vermogen of de bij of krachtens artikel 132 gestelde eisen ten aanzien van het vereist eigen vermogen;"@nl,
            "b. het pensioenfonds niet in staat is binnen een redelijke termijn te voldoen aan artikel 131 of artikel 132 zonder dat de belangen van deelnemers, gewezen deelnemers, pensioengerechtigden, andere aanspraakgerechtigden of de werkgever onevenredig worden geschaad; en"@nl,
            "c. alle overige beschikbare sturingsmiddelen, met uitzondering van het beleggingsbeleid, zijn ingezet in het herstelplan, bedoeld in artikel 138 of artikel 139."@nl ;
        def:hasText "Een pensioenfonds kan verworven pensioenaanspraken en pensioenrechten uitsluitend verminderen indien:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 134, lid 1"@nl .

    id:Pensioenwet-20250301-134-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=134&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds informeert de deelnemers, gewezen deelnemers, pensioengerechtigden en de werkgever onverwijld over het besluit tot vermindering van pensioenaanspraken en pensioenrechten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 134, lid 2"@nl .

    id:Pensioenwet-20250301-134-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=134&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De vermindering, bedoeld in het eerste lid, kan op zijn vroegst drie maanden nadat de pensioengerechtigden hierover zijn geïnformeerd en een maand nadat de deelnemers, gewezen deelnemers, werkgever en toezichthouder hierover zijn geïnformeerd, worden gerealiseerd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 134, lid 3"@nl .

    id:Pensioenwet-20250301-134-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=134&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen nadere regels worden gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 134, lid 4"@nl .

    id:Pensioenwet-20250301-135 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-135-1,
            id:Pensioenwet-20250301-135-2,
            id:Pensioenwet-20250301-135-3,
            id:Pensioenwet-20250301-135-4 ;
        def:hasTitle "Eisen ten aanzien van beleggingen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 135"@nl .

    id:Pensioenwet-20250301-135-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=135&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. de waarden worden belegd in het belang van aanspraak- en pensioengerechtigden;"@nl,
            "b. beleggingen in de bijdragende onderneming worden beperkt tot ten hoogste 5% van de portefeuille als geheel, en ingeval de bijdragende onderneming tot een groep behoort, worden beleggingen in de ondernemingen die tot dezelfde groep als de bijdragende onderneming behoren, beperkt tot ten hoogste 10% van de portefeuille. Wanneer een groep van ondernemingen aan het pensioenfonds premies betaalt, geschieden beleggingen in deze bijdragende ondernemingen prudent, waarbij rekening wordt gehouden met de noodzaak van een behoorlijke diversificatie;"@nl,
            "c. de beleggingen worden gewaardeerd op basis van marktwaardering; en"@nl,
            "d. de blootstelling aan beleggingsrisico is niet hoger dan 150%."@nl ;
        def:hasText "Een pensioenfonds voert een beleggingsbeleid dat in overeenstemming is met de prudent-person regel en met name gebaseerd is op de volgende uitgangspunten:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 135, lid 1"@nl .

    id:Pensioenwet-20250301-135-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=135&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden ter waarborging van het prudente beleggingsbeleid nadere regels gesteld."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 135, lid 2"@nl .

    id:Pensioenwet-20250301-135-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=135&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De eisen die zijn opgenomen in het eerste lid, aanhef en onderdeel b, en de regels die op grond van het tweede lid worden gesteld ten aanzien van de diversificatie van waarden zijn niet van toepassing op beleggingen in staatsobligaties."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 135, lid 3"@nl .

    id:Pensioenwet-20250301-135-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=135&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds vermeldt in zijn bestuursverslag op welke wijze in het beleggingsbeleid rekening wordt gehouden met milieu en klimaat, mensenrechten en sociale verhoudingen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 135, lid 4"@nl .

    id:Pensioenwet-20250301-136 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-136-1,
            id:Pensioenwet-20250301-136-2 ;
        def:hasTitle "Leningen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 136"@nl .

    id:Pensioenwet-20250301-136-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=136&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds gaat geen leningen aan, tenzij de lening tijdelijk wordt aangegaan voor liquiditeitsdoelstellingen en treedt niet namens derde partijen op als garant."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 136, lid 1"@nl .

    id:Pensioenwet-20250301-136-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=136&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen regels worden gesteld ten aanzien van het eerste lid met betrekking tot de tijdelijkheid van de lening en de liquiditeitsdoelstellingen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 136, lid 2"@nl .

    id:Pensioenwet-20250301-137 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-137-1,
            id:Pensioenwet-20250301-137-2,
            id:Pensioenwet-20250301-137-3,
            id:Pensioenwet-20250301-137-4,
            id:Pensioenwet-20250301-137-5 ;
        def:hasTitle "Voorwaardelijke toeslagverlening"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 137"@nl .

    id:Pensioenwet-20250301-137-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=137&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor zover van toepassing stelt een pensioenfonds beleid vast met betrekking tot de voorwaardelijke toeslagverlening."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 137, lid 1"@nl .

    id:Pensioenwet-20250301-137-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=137&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. bij een beleidsdekkingsgraad onder een bij algemene maatregel van bestuur te bepalen niveau wordt geen toeslag verleend;"@nl,
            "b. er wordt niet meer toeslag verleend dan naar verwachting in de toekomst te realiseren is; en"@nl,
            "c. incidentele toeslagverlening om in het verleden niet toegekende toeslag of in het verleden doorgevoerde vermindering van pensioenaanspraken en pensioenrechten te compenseren kan worden verleend indien die toeslagverlening geen gevolgen heeft voor de toeslagverlening in de toekomst overeenkomstig onderdeel b, de beleidsdekkingsgraad het niveau van het vereist eigen vermogen, bedoeld in artikel 132, behoudt en in enig jaar ten hoogste een vijfde van het vermogen dat voor deze toeslagverlening beschikbaar is, wordt aangewend."@nl ;
        def:hasText "Voor een pensioenfonds geldt bij de voorwaardelijke toeslagverlening het volgende:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 137, lid 2"@nl .

    id:Pensioenwet-20250301-137-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=137&lid=3&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een pensioenfonds volledig verzekerd is bij een verzekeraar;"@nl,
            "b. de werkgever een onvoorwaardelijke verplichting heeft tot het verstrekken van bijdragen aan een pensioenfonds zodanig dat dit pensioenfonds steeds voldoet aan de bij of krachtens artikel 131 gestelde eisen ten aanzien van het minimaal vereist eigen vermogen en daarbij sprake is van onvoorwaardelijke toeslagverlening voor deelnemers conform minimaal de groeivoet van het prijsindexcijfer; of"@nl,
            "c. anderszins sprake is van bij of krachtens algemene maatregel van bestuur te bepalen bijzondere omstandigheden."@nl ;
        def:hasText "Het tweede lid is niet van toepassing indien:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 137, lid 3"@nl .

    id:Pensioenwet-20250301-137-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=137&lid=4&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het creëren van technische voorzieningen;"@nl,
            "b. het creëren van eigen vermogen boven het vereist eigen vermogen ten behoeve van de toeslagverlening;"@nl,
            "c. het putten uit het eigen vermogen boven het vereist eigen vermogen ten behoeve van de toeslagverlening;"@nl,
            "d. het hanteren van een opslag op de premie; of"@nl,
            "e. overrendement."@nl ;
        def:hasText "Pensioenfondsen kunnen de voorwaardelijke toeslagverlening financieren door:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 137, lid 4"@nl .

    id:Pensioenwet-20250301-137-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=137&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 137, lid 5"@nl .

    id:Pensioenwet-20250301-138 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-138-1,
            id:Pensioenwet-20250301-138-2,
            id:Pensioenwet-20250301-138-3,
            id:Pensioenwet-20250301-138-4,
            id:Pensioenwet-20250301-138-5,
            id:Pensioenwet-20250301-138-6,
            id:Pensioenwet-20250301-138-7,
            id:Pensioenwet-20250301-138-8 ;
        def:hasTitle "Herstelplan"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 138"@nl .

    id:Pensioenwet-20250301-138-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=138&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Wanneer de beleidsdekkingsgraad van een pensioenfonds per het einde van een kalenderkwartaal is komen te liggen onder de bij of krachtens artikel 132 gestelde eisen ten aanzien van het vereist eigen vermogen, meldt het pensioenfonds dit onverwijld aan de toezichthouder. Wanneer de beleidsdekkingsgraad van een pensioenfonds, waarvan het minimaal vereist eigen vermogen hoger is dan het vereist eigen vermogen, per het einde van een kalenderkwartaal is komen te liggen onder de bij of krachtens artikel 131 gestelde eisen ten aanzien van het minimaal vereist eigen vermogen, meldt het pensioenfonds dit onverwijld aan de toezichthouder."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 138, lid 1"@nl .

    id:Pensioenwet-20250301-138-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=138&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In de in de eerste zin van het eerste lid bedoelde situatie dient het pensioenfonds binnen drie maanden of zoveel eerder als de toezichthouder bepaalt ter instemming bij de toezichthouder een concreet en haalbaar herstelplan in, tenzij het pensioenfonds gezien de beleidsdekkingsgraad op dat moment weer voldoet aan artikel 132. In het herstelplan werkt het pensioenfonds uit hoe het uiterlijk binnen tien jaar zal voldoen aan artikel 132."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 138, lid 2"@nl .

    id:Pensioenwet-20250301-138-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=138&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In afwijking van het tweede lid kan de toezichthouder, in verband met de specifieke situatie van het pensioenfonds en in het belang van de aanspraak- en pensioengerechtigden, bepalen dat voor het herstelplan een kortere termijn geldt dan tien jaar."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 138, lid 3"@nl .

    id:Pensioenwet-20250301-138-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=138&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds dat bijdragen ontvangt van een in een andere lidstaat gevestigde bijdragende onderneming en een beleidsdekkingsgraad heeft die ligt onder de bij of krachtens artikel 131 gestelde eisen ten aanzien van het minimaal vereist eigen vermogen werkt in het herstelplan tevens uit hoe het binnen een jaar zal voldoen aan artikel 131."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 138, lid 4"@nl .

    id:Pensioenwet-20250301-138-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=138&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het herstelplan vertoont een in beginsel tijdsevenredig herstel. In afwijking hiervan kan het herstelplan een meer dan tijdsevenredig herstel vertonen in de eerste helft van de looptijd van het herstelplan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 138, lid 5"@nl .

    id:Pensioenwet-20250301-138-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=138&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het herstelplan gaat uiterlijk zes maanden nadat de beleidsdekkingsgraad is komen te liggen onder de bij of krachtens artikel 132 gestelde eisen in. Het pensioenfonds handelt onverwijld overeenkomstig het herstelplan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 138, lid 6"@nl .

    id:Pensioenwet-20250301-138-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=138&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds stelt jaarlijks de beleidsdekkingsgraad vast op het moment waarop een situatie als bedoeld in het eerste lid is vastgesteld. Zo nodig in afwijking van de vorige zin kan het pensioenfonds de beleidsdekkingsgraad jaarlijks vaststellen per het einde van het vierde kalenderkwartaal, door de eerste vaststelling van de beleidsdekkingsgraad na het moment waarop een situatie als bedoeld in het eerste lid, is vastgesteld te vervroegen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 138, lid 7"@nl .

    id:Pensioenwet-20250301-138-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=138&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel die onder meer betrekking hebben op de periode tot het herstelplan ingaat."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 138, lid 8"@nl .

    id:Pensioenwet-20250301-139 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-139-1,
            id:Pensioenwet-20250301-139-2,
            id:Pensioenwet-20250301-139-3,
            id:Pensioenwet-20250301-139-4 ;
        def:hasTitle "Actualisatie herstelplan"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 139"@nl .

    id:Pensioenwet-20250301-139-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=139&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Wanneer de beleidsdekkingsgraad die op grond van artikel 138, zevende lid, is vastgesteld ligt onder de bij of krachtens artikel 132 gestelde eisen ten aanzien van het vereist eigen vermogen, dient het pensioenfonds binnen drie maanden of zoveel eerder als de toezichthouder bepaalt ter instemming bij de toezichthouder een concreet en haalbaar geactualiseerd herstelplan in."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 139, lid 1"@nl .

    id:Pensioenwet-20250301-139-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=139&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In het in het eerste lid bedoelde herstelplan werkt het pensioenfonds uit hoe het in dezelfde termijn als de termijn die het pensioenfonds bij het herstelplan op grond van artikel 138, tweede lid, heeft gebruikt, zal voldoen aan artikel 132. Het pensioenfonds kan, rekening houdend met de specifieke situatie van het pensioenfonds en in het belang van aanspraak- en pensioengerechtigden, een andere termijn hanteren voor het geactualiseerd herstelplan indien de toezichthouder daarmee instemt."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 139, lid 2"@nl .

    id:Pensioenwet-20250301-139-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=139&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText """
      Artikel 138, vierde tot en met achtste lid, is van toepassing op de geactualiseerde herstelplannen. Op het moment dat het geactualiseerde herstelplan ingaat vervangt dit een eerder vastgesteld herstelplan of geactualiseerd herstelplan."""@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 139, lid 3"@nl .

    id:Pensioenwet-20250301-139-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=139&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen nadere regels worden gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 139, lid 4"@nl .

    id:Pensioenwet-20250301-14 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-14-1,
            id:Pensioenwet-20250301-14-2,
            id:Pensioenwet-20250301-14-3,
            id:Pensioenwet-20250301-14-4 ;
        def:hasTitle "Beperking onderscheid naar leeftijd bij verwerving en aanbod"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 14"@nl .

    id:Pensioenwet-20250301-14-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=14&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002221&artikel=7a"^^xsd:string ;
        def:hasText "Het verwerven van ouderdomspensioenaanspraken op basis van een pensioenovereenkomst begint uiterlijk op de 18-jarige leeftijd van de werknemer of op de latere datum van indiensttreding, tenzij het een ouderdomspensioen betreft dat uitsluitend voorziet in een uitkering tot het bereiken van de pensioengerechtigde leeftijd, bedoeld in artikel 7a, eerste lid, van de Algemene Ouderdomswet, of tot het bereiken van de pensioenleeftijd voor het levenslange ouderdomspensioen."@nl ;
        def:referencesTo id:AlgemeneOuderdomswet-20250101-7a ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 14, lid 1"@nl .

    id:Pensioenwet-20250301-14-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=14&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het in het eerste lid genoemde tijdstip waarop de verwerving begint kan, indien in de pensioenovereenkomst is voorzien in een drempelperiode, met betrekking tot ouderdompensioen worden uitgesteld met ten hoogste twee maanden. Wachttijden zijn niet toegestaan voor het ouderdomspensioen. Wachttijden of drempelperioden zijn niet toegestaan voor het nabestaandenpensioen en het arbeidsongeschiktheidspensioen. Indien een werknemer bij de beëindiging van zijn arbeidsovereenkomst naar burgerlijk recht of publiekrechtelijke aanstelling nog niet in de pensioenregeling is opgenomen, maar er door of namens hem een bijdrage is betaald, dan wordt de som van de bijdragen aan hem terugbetaald of wordt, in geval de werknemer het beleggingsrisico draagt, de som van de gestorte bijdragen of de met deze bijdragen gerealiseerde beleggingswaarde aan hem terugbetaald."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 14, lid 2"@nl .

    id:Pensioenwet-20250301-14-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=14&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In geval een werkgever die nog geen enkele pensioenovereenkomst heeft gesloten of die alleen een pensioenovereenkomst heeft gesloten met werknemers die tot een bepaalde groep behoren, over gaat tot het sluiten van een of meer pensioenovereenkomsten, geldt ten aanzien van zijn werknemers die bij het sluiten van de pensioenovereenkomst ouder zijn dan 18 jaar, niet de eis dat de verwerving van pensioen op 18-jarige leeftijd moet zijn begonnen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 14, lid 3"@nl .

    id:Pensioenwet-20250301-14-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=14&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Elk beding in strijd met het eerste lid en tweede lid is nietig."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 14, lid 4"@nl .

    id:Pensioenwet-20250301-140 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-140-1,
            id:Pensioenwet-20250301-140-2,
            id:Pensioenwet-20250301-140-3,
            id:Pensioenwet-20250301-140-4 ;
        def:hasTitle "Maatregel minimaal vereist eigen vermogen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 140"@nl .

    id:Pensioenwet-20250301-140-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=140&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien de beleidsdekkingsgraad in een situatie als bedoeld in artikel 138, eerste lid, of vastgesteld op grond van artikel 138, zevende lid, zes maal opeenvolgend ligt onder de bij of krachtens artikel 131 gestelde eisen ten aanzien van het minimaal vereist eigen vermogen en de dekkingsgraad bij die laatste vaststelling ook onder dat niveau ligt neemt het pensioenfonds binnen zes maanden maatregelen waardoor de dekkingsgraad van het pensioenfonds direct voldoet aan artikel 131. Voor zover het bij de maatregelen vermindering van pensioenaanspraken en pensioenrechten betreft worden deze direct in de technische voorzieningen verwerkt en ofwel direct doorgevoerd, ofwel in beginsel evenredig gespreid in de tijd gedurende maximaal de termijn die wordt gebruikt voor het herstelplan dat is opgesteld na constatering van de situatie, bedoeld in de eerste zin."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 140, lid 1"@nl .

    id:Pensioenwet-20250301-140-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=140&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor een pensioenfonds dat bijdragen ontvangt van een in een andere lidstaat gevestigde bijdragende onderneming geldt in plaats van het in het eerste lid genoemde aantal van zes een aantal van twee."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 140, lid 2"@nl .

    id:Pensioenwet-20250301-140-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=140&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien een pensioenfonds in de situatie, bedoeld in het eerste lid, maatregelen heeft genomen waardoor de dekkingsgraad direct voldoet aan artikel 131, begint voor de volgende toepassing van het eerste lid een nieuwe termijn. Indien een pensioenfonds in de situatie, bedoeld in het eerste lid, geen maatregelen heeft genomen omdat bij de laatste vaststelling van de beleidsdekkingsgraad de dekkingsgraad niet onder het minimaal vereist eigen vermogen lag, is het eerste lid van overeenkomstige toepassing bij iedere volgende vaststelling van de beleidsdekkingsgraad op grond van artikel 138, zevende lid, totdat de beleidsdekkingsgraad ligt op of boven het niveau van het minimaal vereist eigen vermogen of de beleidsdekkingsgraad en de dekkingsgraad liggen onder dat niveau."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 140, lid 3"@nl .

    id:Pensioenwet-20250301-140-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=140&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen nadere regels worden gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 140, lid 4"@nl .

    id:Pensioenwet-20250301-141 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-141-1,
            id:Pensioenwet-20250301-141-2,
            id:Pensioenwet-20250301-141-3 ;
        def:hasTitle "Mogelijkheid tot ontheffing"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 141"@nl .

    id:Pensioenwet-20250301-141-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=141&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder kan, rekening houdend met de specifieke situatie van het pensioenfonds en in het belang van de aanspraak- en pensioengerechtigden, op aanvraag van een pensioenfonds geheel of gedeeltelijk, ontheffing verlenen van het bij of krachtens de artikelen 131, 132, 134, eerste lid, onderdeel a, 137, 138, 139 en 140 bepaalde, indien het pensioenfonds aantoont dat daaraan redelijkerwijs niet kan worden voldaan en dat de doeleinden die deze artikelen beogen te bereiken anderszins worden bereikt."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 141, lid 1"@nl .

    id:Pensioenwet-20250301-141-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=141&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In afwijking van het eerste lid kan de toezichthouder geen ontheffing verlenen van artikel 138, vierde lid, en artikel 140, tweede lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 141, lid 2"@nl .

    id:Pensioenwet-20250301-141-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=141&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen regels worden gesteld waaraan de houder van een ontheffing dient te voldoen en met betrekking tot het verlenen van de ontheffing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 141, lid 3"@nl .

    id:Pensioenwet-20250301-142 a def:LawArticle ;
        def:hasDescription " Bij regeling van Onze Minister, na overleg met de toezichthouder, kan vrijstelling worden verleend van de in artikel 138 genoemde termijn van tien jaar en het in artikel 140 genoemde aantal van zes, indien er sprake is van een uitzonderlijke economische situatie waardoor een groot aantal pensioenfondsen niet kan voldoen aan de bij of krachtens deze wet gestelde vereisten inzake het vereist eigen vermogen en het minimaal vereist eigen vermogen."@nl ;
        def:hasTitle "Vrijstelling bij uitzonderlijke situatie"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 142"@nl .

    id:Pensioenwet-20250301-143 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-143-1,
            id:Pensioenwet-20250301-143-2 ;
        def:hasTitle "Beheerste en integere bedrijfsvoering"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143"@nl .

    id:Pensioenwet-20250301-143-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=143&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds richt zijn organisatie zodanig in dat deze een beheerste en integere bedrijfsvoering waarborgt."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143, lid 1"@nl .

    id:Pensioenwet-20250301-143-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=143&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het beheersen van bedrijfsprocessen en bedrijfsrisico’s;"@nl,
            "b. integriteit;"@nl,
            "c. de soliditeit van het pensioenfonds, waaronder wordt verstaan:"@nl,
            "d. het beheersen van de financiële positie over de lange termijn door periodiek een haalbaarheidstoets te maken."@nl ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden regels gesteld met betrekking tot het eerste lid. De regels hebben in ieder geval betrekking op:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143, lid 2"@nl .

    id:Pensioenwet-20250301-143a a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-143a-1,
            id:Pensioenwet-20250301-143a-2,
            id:Pensioenwet-20250301-143a-3,
            id:Pensioenwet-20250301-143a-4,
            id:Pensioenwet-20250301-143a-5 ;
        def:hasTitle "Sleutelfuncties"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143a"@nl .

    id:Pensioenwet-20250301-143a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=143a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds beschikt over een risicobeheerfunctie, interne auditfunctie en actuariële functie."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143a, lid 1"@nl .

    id:Pensioenwet-20250301-143a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=143a&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De houders van de risicobeheerfunctie, interne auditfunctie of actuariële functie rapporteren materiële bevindingen en aanbevelingen op het gebied dat onder hun verantwoordelijkheid valt aan het bestuur van het pensioenfonds. Indien de houder van de risicobeheerfunctie, interne auditfunctie of actuariële functie tevens bestuurder is van het pensioenfonds worden de materiële bevindingen en aanbevelingen ook gerapporteerd aan de raad van toezicht of de visitatiecommissie."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143a, lid 2"@nl .

    id:Pensioenwet-20250301-143a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=143a&lid=3&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een substantieel risico dat het pensioenfonds niet aan een bij of krachtens de wet gesteld vereiste van significante betekenis zal voldoen en dit ernstige gevolgen kan hebben voor de belangen van deelnemers, gewezen deelnemers, andere aanspraakgerechtigden of pensioengerechtigden; of"@nl,
            "b. een significante inbreuk op de voor het pensioenfonds en haar activiteiten geldende bij of krachtens de wet gestelde vereisten."@nl ;
        def:hasText "De houders van de risicobeheerfunctie, interne auditfunctie of actuariële functie melden het de toezichthouder zo spoedig mogelijk indien het bestuur van het pensioenfonds niet tijdig passende corrigerende maatregelen treft, nadat het bestuur op grond van het tweede lid op de hoogte is gesteld van:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143a, lid 3"@nl .

    id:Pensioenwet-20250301-143a-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=143a&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds draagt er zorg voor dat de houder van de risicobeheerfunctie, interne auditfunctie of actuariële functie die op grond van het derde lid te goeder trouw en naar behoren een melding heeft gedaan bij de toezichthouder als gevolg van deze melding niet wordt benadeeld."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143a, lid 4"@nl .

    id:Pensioenwet-20250301-143a-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=143a&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143a, lid 5"@nl .

    id:Pensioenwet-20250301-143b a def:LawArticle ;
        def:hasDescription " De toezichthouder kan een pensioenfonds de verplichting opleggen om een stresstest uit te voeren."@nl ;
        def:hasTitle "Stresstest"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 143b"@nl .

    id:Pensioenwet-20250301-144-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=144&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het minimale percentage van het gemiddelde loon- of prijsindexcijfer;"@nl,
            "b. het maximaal te hanteren gemiddelde rendement op vastrentende waarden;"@nl,
            "c. de maximaal te hanteren risicopremies op onder andere aandelen en onroerend goed; en"@nl,
            "d. een uniforme set met economische scenario’s."@nl ;
        def:hasText "Bij algemene maatregel van bestuur worden, ten behoeve van de berekeningen noodzakelijk voor de uitvoering van een pensioenregeling, regels gesteld over:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 144, lid 1"@nl .

    id:Pensioenwet-20250301-144-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=144&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De in het eerste lid bedoelde regels worden uiterlijk iedere vijf jaren getoetst, rekening houdend met financieel-economische ontwikkelingen in het verleden en realistische inzichten ten aanzien van toekomstige financieel-economische verwachtingen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 144, lid 2"@nl .

    id:Pensioenwet-20250301-144-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=144&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0008159&artikel=6"^^xsd:string ;
        def:hasText "Ten behoeve van de toetsing, bedoeld in het tweede lid, stelt Onze Minister een commissie in met toepassing van artikel 6 van de Kaderwet adviescolleges. Onze Minister kan tevens het oordeel van de commissie vragen over de toepassing van de rentetermijnstructuur bij rentes met een lange looptijd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 144, lid 3"@nl .

    id:Pensioenwet-20250301-144-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=144&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De voordracht voor een krachtens het eerste lid vast te stellen algemene maatregel van bestuur wordt niet eerder gedaan dan vier weken nadat het ontwerp aan beide kamers der Staten-Generaal is overgelegd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 144, lid 4"@nl .

    id:Pensioenwet-20250301-145 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-145-1,
            id:Pensioenwet-20250301-145-2,
            id:Pensioenwet-20250301-145-3,
            id:Pensioenwet-20250301-145-4,
            id:Pensioenwet-20250301-145-5 ;
        def:hasTitle "Actuariële en bedrijfstechnische nota"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 145"@nl .

    id:Pensioenwet-20250301-145-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=145&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds stelt een actuariële en bedrijfstechnische nota vast waarin in elk geval een omschrijving is opgenomen van de wijze waarop uitvoering wordt gegeven aan het bij of krachtens de artikelen 25, 126 tot en met 137 en 143 bepaalde en de wijze waarop uitvoering wordt gegeven aan de regels ten aanzien van de solidariteitsreserve of de risicodelingsreserve, de toedelingsregels, het projectierendement of de vaste daling en de risicohouding. De actuariële en bedrijfstechnische nota bevat voorts een verklaring inzake beleggingsbeginselen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 145, lid 1"@nl .

    id:Pensioenwet-20250301-145-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=145&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een beschrijving van de wijze waarop het pensioenfonds voorkomt dat de voor pensioen bestemde vermogens, de solidariteitsreserve of de risicodelingsreserve negatief kunnen worden;"@nl,
            "b. een financieel crisisplan waarin het pensioenfonds beschrijft welke maatregelen ingezet kunnen worden in de situatie dat niet aan de vereisten gesteld bij of krachtens artikel 132 zal worden voldaan en welke maatregelen ingezet kunnen worden in de situatie, bedoeld in artikel 140, eerste lid;"@nl,
            "c. het niveau van de beleidsdekkingsgraad vanaf welke premiekortingen en terugstortingen als bedoeld in artikel 129 zijn toegestaan; en"@nl,
            "d. een beschrijving van de overige sturingsmiddelen."@nl ;
        def:hasText "De actuariële en bedrijfstechnische nota van een pensioenfonds bevat verder:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 145, lid 2"@nl .

    id:Pensioenwet-20250301-145-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=145&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden regels gesteld ten aanzien van de actuariële en bedrijfstechnische nota."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 145, lid 3"@nl .

    id:Pensioenwet-20250301-145-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=145&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voorzover risico’s zijn overgedragen, verzekerd of ondergebracht kan de omschrijving, bedoeld in het eerste lid, beperkt blijven tot een verwijzing naar hetgeen daarover in de betreffende overeenkomsten is opgenomen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 145, lid 4"@nl .

    id:Pensioenwet-20250301-145-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=145&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De verklaring inzake beleggingsbeginselen wordt om de drie jaren en voorts onverwijld na iedere belangrijke wijziging van het beleggingsbeleid herzien."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 145, lid 5"@nl .

    id:Pensioenwet-20250301-146 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-146-1,
            id:Pensioenwet-20250301-146-2 ;
        def:hasTitle "Jaarrekening en bestuursverslag"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 146"@nl .

    id:Pensioenwet-20250301-146-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=146&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0003045&artikel=360"^^xsd:string,
            "jci1.3:c:BWBR0003045&artikel=390"^^xsd:string,
            "jci1.3:c:BWBR0003045&artikel=396"^^xsd:string,
            "jci1.3:c:BWBR0003045&artikel=397"^^xsd:string,
            "jci1.3:c:BWBR0003045&titeldeel=9"^^xsd:string ;
        def:hasText "Een pensioenfonds met zetel in Nederland stelt binnen zes maanden na afloop van het boekjaar de jaarrekening en het bestuursverslag overeenkomstig titel 9, Boek 2 van het Burgerlijk Wetboek vast, met dien verstande dat artikel 390 van genoemd wetboek niet van toepassing is en dat de in artikel 360, derde lid, 396 en 397 van genoemd wetboek geformuleerde uitzonderingen niet van toepassing zijn."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 146, lid 1"@nl .

    id:Pensioenwet-20250301-146-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=146&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een algemeen pensioenfonds beschrijft ieder afgescheiden vermogen afzonderlijk in de jaarrekening en het bestuursverslag."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 146, lid 2"@nl .

    id:Pensioenwet-20250301-147 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-147-1,
            id:Pensioenwet-20250301-147-2,
            id:Pensioenwet-20250301-147-3,
            id:Pensioenwet-20250301-147-4,
            id:Pensioenwet-20250301-147-5,
            id:Pensioenwet-20250301-147-6 ;
        def:hasTitle "Staten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 147"@nl .

    id:Pensioenwet-20250301-147-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=147&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds doet het boekjaar gelijk lopen met het kalenderjaar."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 147, lid 1"@nl .

    id:Pensioenwet-20250301-147-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=147&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds verstrekt periodiek binnen de daartoe vastgestelde termijnen staten aan de toezichthouder die de toezichthouder nodig heeft voor de juiste uitoefening van zijn taak, bedoeld in artikel 151."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 147, lid 2"@nl .

    id:Pensioenwet-20250301-147-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=147&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De actuariële staten zijn gewaarmerkt door een bevoegd actuaris. Bij de actuariële staten hoort een actuarieel verslag voorzien van een verklaring van een actuaris."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 147, lid 3"@nl .

    id:Pensioenwet-20250301-147-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=147&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Met zijn verklaring bedoeld in het derde lid bevestigt de actuaris dat hij zich ervan heeft overtuigd dat voldaan is aan de artikelen 126 tot en met 140, de correcte toepassing van de toedelingsregels en de regels ten aanzien van de risicohouding. Hij is bevoegd zijn verklaring nader toe te lichten of op enig punt een voorbehoud te maken."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 147, lid 4"@nl .

    id:Pensioenwet-20250301-147-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=147&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De staten zijn periodiek voorzien van een verklaring omtrent de getrouwheid, afgegeven door een accountant. Ten bewijze dat de staten door hem zijn onderzocht, waarmerkt de accountant de staten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 147, lid 5"@nl .

    id:Pensioenwet-20250301-147-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=147&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur wordt bepaald welke staten worden verstrekt en worden regels gesteld met betrekking tot de inhoud en de modellen van de staten en de wijze, de periodiciteit en de termijnen van de verstrekking."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 147, lid 6"@nl .

    id:Pensioenwet-20250301-148 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-148-1,
            id:Pensioenwet-20250301-148-2,
            id:Pensioenwet-20250301-148-3 ;
        def:hasTitle "Onafhankelijkheid actuaris"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 148"@nl .

    id:Pensioenwet-20250301-148-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=148&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De bevoegde actuaris die het actuarieel verslag waarmerkt, is onafhankelijk van het pensioenfonds en verricht geen andere werkzaamheden voor het pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 148, lid 1"@nl .

    id:Pensioenwet-20250301-148-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=148&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het is de waarmerkende actuaris niet toegestaan de werkzaamheden, bedoeld in artikel 147, vierde lid, uit te oefenen voor een pensioenfonds wanneer een andere actuaris of andere deskundige die behoort tot dezelfde organisatie als de waarmerkende actuaris, andere werkzaamheden verricht voor hetzelfde pensioenfonds, tenzij de organisatie van de waarmerkende actuaris een door de toezichthouder goedgekeurde gedragscode heeft over de onafhankelijkheid van de waarmerkende actuaris."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 148, lid 2"@nl .

    id:Pensioenwet-20250301-148-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6&artikel=148&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In afwijking van het eerste lid kan de waarmerkende actuaris andere werkzaamheden voor het pensioenfonds verrichten, voor zover het gaat om werkzaamheden van de actuariële functie."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 148, lid 3"@nl .

    id:Pensioenwet-20250301-148a a def:LawArticle ;
        def:hasDescription " Een pensioenfonds kan niet overgaan tot verzekering bij een verzekeraar indien de verzekering gebaseerd is op een kapitaalcontract. Een kapitaalcontract is een overeenkomst tussen een pensioenfonds en een verzekeraar waarbij geldt dat:"@nl ;
        def:hasSection id:Pensioenwet-20250301-148a-1,
            id:Pensioenwet-20250301-148a-2,
            id:Pensioenwet-20250301-148a-3 ;
        def:hasTitle "Verbod kapitaalcontracten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 148a"@nl .

    id:Pensioenwet-20250301-148a-1 a def:LawArticleSection ;
        def:hasText "a. risico’s van het pensioenfonds gedurende de contractsperiode worden verzekerd;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 148a, Deel 1"@nl .

    id:Pensioenwet-20250301-148a-2 a def:LawArticleSection ;
        def:hasText "b. de daarbij behorende pensioenaanspraken en pensioenrechten na afloop van de contractsperiode niet op verzoek van het pensioenfonds premievrij bij de verzekeraar achter kunnen worden gelaten; en"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 148a, Deel 2"@nl .

    id:Pensioenwet-20250301-148a-3 a def:LawArticleSection ;
        def:hasText "c. de waarden die behoren bij het ouderdomspensioen of nabestaandenpensioen op kapitaalbasis gedurende de contractsperiode geheel of gedeeltelijk in juridisch eigendom van de verzekeraar zijn."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 148a, Deel 3"@nl .

    id:Pensioenwet-20250301-149 a def:LawArticle ;
        def:hasDescription " De toezichthouder kan een pensioenfonds de verplichting opleggen om binnen een door de toezichthouder te stellen termijn over te gaan tot verzekering bij een verzekeraar, overdracht aan een verzekeraar of onderbrenging bij een pensioenfonds indien dit naar het oordeel van de toezichthouder noodzakelijk is in verband met:"@nl ;
        def:hasSection id:Pensioenwet-20250301-149-1,
            id:Pensioenwet-20250301-149-2 ;
        def:hasTitle "Verplichting tot overdracht, verzekering of onderbrenging"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 149"@nl .

    id:Pensioenwet-20250301-149-1 a def:LawArticleSection ;
        def:hasText "a. de actuariële en bedrijfstechnische opzet van het pensioenfonds; of"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 149, Deel 1"@nl .

    id:Pensioenwet-20250301-149-2 a def:LawArticleSection ;
        def:hasText "b. de deskundigheid en betrouwbaarheid van het bestuur."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 149, Deel 2"@nl .

    id:Pensioenwet-20250301-15 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-15-1,
            id:Pensioenwet-20250301-15-2 ;
        def:hasTitle "Nadere eisen ouderdomspensioen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 15"@nl .

    id:Pensioenwet-20250301-15-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=15&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002221&artikel=7a"^^xsd:string ;
        def:hasText "Indien een pensioenovereenkomst voorziet in een ouderdomspensioen, wordt in de overeenkomst bepaald dat dat pensioen levenslang wordt uitgekeerd aan de gepensioneerde, tenzij het ouderdomspensioen uitsluitend voorziet in een uitkering tot het bereiken van de de pensioengerechtigde leeftijd, bedoeld in artikel 7a, eerste lid, van de Algemene Ouderdomswet, of tot het bereiken van de pensioenleeftijd voor het levenslange ouderdomspensioen."@nl ;
        def:referencesTo id:AlgemeneOuderdomswet-20250101-7a ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 15, lid 1"@nl .

    id:Pensioenwet-20250301-15-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=15&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Elk beding in strijd met het eerste lid is nietig."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 15, lid 2"@nl .

    id:Pensioenwet-20250301-150 a def:LawArticle ;
        def:hasDescription " Wanneer een pensioenregeling eindigt en er geen sprake meer is van verwerving van pensioen tijdens een periode waarin de beleidsdekkingsgraad van het pensioenfonds ligt onder de bij of krachtens artikel 131 gestelde eisen ten aanzien van het minimaal vereist eigen vermogen:"@nl ;
        def:hasSection id:Pensioenwet-20250301-150-1,
            id:Pensioenwet-20250301-150-2,
            id:Pensioenwet-20250301-150-3 ;
        def:hasTitle "Overdracht, verzekering of onderbrenging bij eindigen pensioenregeling"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150"@nl .

    id:Pensioenwet-20250301-150-1 a def:LawArticleSection ;
        def:hasText "a. stelt het pensioenfonds de toezichthouder hiervan op de hoogte;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150, Deel 1"@nl .

    id:Pensioenwet-20250301-150-2 a def:LawArticleSection ;
        def:hasText "b. gaat het pensioenfonds binnen een door de toezichthouder te stellen termijn over tot het verzekeren bij een verzekeraar, overdragen aan een verzekeraar of onderbrengen bij een pensioenfonds van de pensioenverplichtingen op basis van een procedure welke ter kennis en instemming van de toezichthouder wordt gebracht; en"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150, Deel 2"@nl .

    id:Pensioenwet-20250301-150-3 a def:LawArticleSection ;
        def:hasText "c. stelt het pensioenfonds een algemeen overzicht van de procedure, bedoeld in onderdeel b, beschikbaar voor de deelnemers, gewezen deelnemers en pensioengerechtigden of de vertegenwoordigers van de genoemde personen in overeenstemming met het vertrouwelijkheidbeginsel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150, Deel 3"@nl .

    id:Pensioenwet-20250301-150a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6a&artikel=150a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur kan bij wijze van experiment, met het oog op het onderzoeken van de effecten van de mogelijkheid om een pensioenregeling aan zelfstandigen aan te bieden ten einde het sparen voor de oude dag onder zelfstandigen te stimuleren, worden afgeweken van het bepaalde bij of krachtens de artikelen 17, 65, 70, 117 en 120 van deze wet. Bij toepassing van de eerste zin wordt bij algemene maatregel van bestuur geregeld op welke wijze en gedurende welke periode van welke artikelen van de wet wordt afgeweken."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150a, lid 1"@nl .

    id:Pensioenwet-20250301-150a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6a&artikel=150a&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een experiment als bedoeld in het eerste lid duurt ten hoogste vijf jaar. Indien, voor een experiment is afgelopen, een voorstel van wet is ingediend bij de Staten-Generaal om het experiment om te zetten in een structurele wettelijke regeling, kan het experiment worden verlengd tot het tijdstip waarop het voorstel van wet in werking treedt dan wel tot het tijdstip waarop duidelijk is dat het voorstel van wet niet in werking treedt. Het eerste lid, tweede zin, is van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150a, lid 2"@nl .

    id:Pensioenwet-20250301-150a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6a&artikel=150a&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Onverminderd het eerste lid is deze wet met uitzondering van de artikelen 7 en 9 van overeenkomstige toepassing op de zelfstandige die deelneemt in een pensioenregeling die wordt uitgevoerd in het kader van een experiment als bedoeld in het eerste lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150a, lid 3"@nl .

    id:Pensioenwet-20250301-150a-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6a&artikel=150a&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij de uitvoering van een experiment als bedoeld in het eerste lid kan de pensioenuitvoerder persoonsgegevens van gewezen deelnemers uitsluitend voor het in het eerste lid omschreven doel verwerken."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150a, lid 4"@nl .

    id:Pensioenwet-20250301-150a-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6a&artikel=150a&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens de algemene maatregel van bestuur, bedoeld in het eerste lid, kunnen regels worden gesteld met betrekking tot de uitvoering van een experiment en voorzieningen worden getroffen voor zich gedurende een experiment voordoende onvoorziene gevallen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150a, lid 5"@nl .

    id:Pensioenwet-20250301-150a-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6a&artikel=150a&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Onze Minister zendt uiterlijk negen maanden voor het einde van de werkingsduur van een algemene maatregel van bestuur als bedoeld in het eerste lid aan de Staten-Generaal een verslag over de doeltreffendheid en de effecten van het experiment in de praktijk alsmede een standpunt inzake de voortzetting ervan anders dan als experiment."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150a, lid 6"@nl .

    id:Pensioenwet-20250301-150a-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6a&artikel=150a&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De voordracht voor een krachtens dit artikel vast te stellen algemene maatregel van bestuur wordt niet eerder gedaan dan vier weken nadat het ontwerp aan beide Kamers der Staten-Generaal is overgelegd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150a, lid 7"@nl .

    id:Pensioenwet-20250301-150b a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150b-1,
            id:Pensioenwet-20250301-150b-2,
            id:Pensioenwet-20250301-150b-3,
            id:Pensioenwet-20250301-150b-4 ;
        def:hasTitle "Toepassingsgebied en vaststelling scenario’s"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150b"@nl .

    id:Pensioenwet-20250301-150b-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.1&artikel=150b&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. op de dag voorafgaand aan het tijdstip van inwerkingtreding van de Wet toekomst pensioenen een pensioenovereenkomst gold;"@nl,
            "b. de pensioenovereenkomst wordt gewijzigd met als doel om te voldoen aan het nieuwe kader zoals geïntroduceerd met de Wet toekomst pensioenen; en"@nl,
            "c. er geen gebruik wordt gemaakt van het overgangsrecht, bedoeld in artikel 220e."@nl ;
        def:hasExtref "jci1.3:c:BWBR0048328"^^xsd:string ;
        def:hasText "Dit hoofdstuk is van toepassing, indien:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150b, lid 1"@nl .

    id:Pensioenwet-20250301-150b-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.1&artikel=150b&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor de toepassing van dit hoofdstuk wordt onder werkgever mede verstaan de partijen die de wijziging van de pensioenregeling overeenkomen of zijn overeengekomen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150b, lid 2"@nl .

    id:Pensioenwet-20250301-150b-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.1&artikel=150b&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur worden regels gesteld over de risico-neutrale economische scenario’s die worden gebruikt bij de berekening van de transitie-effecten, bedoeld in artikel 150e, vijfde lid, en de collectieve waardeoverdracht indien gebruik wordt gemaakt van de vba-methode, bedoeld in artikel 150n. Voordat de voordracht van de algemene maatregel van bestuur wordt gedaan vraagt Onze Minister het oordeel van de commissie, bedoeld in artikel 144, derde lid. De voordracht voor deze algemene maatregel van bestuur wordt niet eerder gedaan dan vier weken nadat het ontwerp aan beide Kamers der Staten-Generaal is overgelegd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150b, lid 3"@nl .

    id:Pensioenwet-20250301-150b-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.1&artikel=150b&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText """
      Artikel 144, eerste lid, is van toepassing op de berekeningen, bedoeld in dit hoofdstuk met uitzondering van de berekeningen waarvoor de risico-neutrale scenario’s worden gebruikt, bedoeld in het derde lid."""@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150b, lid 4"@nl .

    id:Pensioenwet-20250301-150c a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150c-1,
            id:Pensioenwet-20250301-150c-2,
            id:Pensioenwet-20250301-150c-3 ;
        def:hasTitle "Mijlpalen in transitieperiode"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150c"@nl .

    id:Pensioenwet-20250301-150c-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.2&artikel=150c&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. de werkgever zendt de gewijzigde pensioenovereenkomst en het transitieplan aan het pensioenfonds;"@nl,
            "b. het pensioenfonds dient het implementatieplan met inbegrip van het communicatieplan in bij de toezichthouder."@nl ;
        def:hasText "Ten behoeve van de wijziging van de pensioenovereenkomst en de uitvoering daarvan door een pensioenfonds wordt voldaan aan de volgende mijlpalen:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150c, lid 1"@nl .

    id:Pensioenwet-20250301-150c-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.2&artikel=150c&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. de werkgever zendt de gewijzigde pensioenovereenkomst en het transitieplan aan de verzekeraar of de premiepensioeninstelling;"@nl,
            "b. de verzekeraar of premiepensioeninstelling dient het implementatieplan met inbegrip van het communicatieplan in bij de toezichthouder."@nl ;
        def:hasText "Ten behoeve van de wijziging van de pensioenovereenkomst en de uitvoering daarvan door een verzekeraar of premiepensioeninstelling wordt voldaan aan de volgende mijlpalen:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150c, lid 2"@nl .

    id:Pensioenwet-20250301-150c-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.2&artikel=150c&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld voor de tijdstippen waarin voldaan is aan de mijlpalen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150c, lid 3"@nl .

    id:Pensioenwet-20250301-150d a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150d-1,
            id:Pensioenwet-20250301-150d-2,
            id:Pensioenwet-20250301-150d-3,
            id:Pensioenwet-20250301-150d-4 ;
        def:hasTitle "Transitieplan"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150d"@nl .

    id:Pensioenwet-20250301-150d-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150d&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In het transitieplan legt de werkgever de keuzes, overwegingen en berekeningen die ten grondslag liggen aan de wijziging van de pensioenovereenkomst en de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten schriftelijk vast, alsmede de verantwoording waarom sprake is van een evenwichtige transitie."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150d, lid 1"@nl .

    id:Pensioenwet-20250301-150d-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150d&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het karakter van de gewijzigde pensioenovereenkomst, bedoeld in artikel 10, tweede lid;"@nl,
            "b. de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten;"@nl,
            "c. de effecten van de wijziging van de pensioenovereenkomst en de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten voor deelnemers, gewezen deelnemers, gewezen partners en pensioengerechtigden berekend op de wijze vastgelegd in artikel 150e;"@nl,
            "d. de gemaakte afspraken over compensatie;"@nl,
            "e. indien afspraken zijn gemaakt over compensatie in de vorm van het toekennen van extra pensioenaanspraken aan deelnemers, het financieringsplan voor deze compensatie waarbij inzichtelijk wordt gemaakt in welke mate elke bron ingezet zal worden; en"@nl,
            "f. voor zover van toepassing, de gemaakte afspraken over de initiële vulling van de solidariteitsreserve of risicodelingsreserve."@nl ;
        def:hasText "Het transitieplan bevat in ieder geval de volgende onderdelen:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150d, lid 2"@nl .

    id:Pensioenwet-20250301-150d-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150d&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De werkgever zendt het transitieplan binnen twee weken na de afronding aan de pensioenuitvoerder. De pensioenuitvoerder stelt het transitieplan op zijn website beschikbaar voor de deelnemer, gewezen deelnemer, gewezen partner en pensioengerechtigde."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150d, lid 3"@nl .

    id:Pensioenwet-20250301-150d-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150d&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden regels gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150d, lid 4"@nl .

    id:Pensioenwet-20250301-150e a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150e-1,
            id:Pensioenwet-20250301-150e-2,
            id:Pensioenwet-20250301-150e-3,
            id:Pensioenwet-20250301-150e-4,
            id:Pensioenwet-20250301-150e-5,
            id:Pensioenwet-20250301-150e-6,
            id:Pensioenwet-20250301-150e-7,
            id:Pensioenwet-20250301-150e-8 ;
        def:hasTitle "Berekenen transitie-effecten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150e"@nl .

    id:Pensioenwet-20250301-150e-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150e&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het netto profijt van het ongewijzigd voortzetten van de pensioenovereenkomst af te zetten tegen het netto profijt van het wijzigen van de pensioenovereenkomst en de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten; en"@nl,
            "b. de pensioenverwachting bij ongewijzigd voortzetten van de pensioenovereenkomst te vergelijken met de pensioenverwachting bij wijziging van de pensioenovereenkomst en de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten, waarbij de pensioenverwachting, voor zover het ouderdomspensioen betreft, wordt weergegeven op basis van een pessimistisch scenario, een verwacht scenario en een optimistisch scenario."@nl ;
        def:hasText "De transitie-effecten van de wijziging van de pensioenovereenkomst en de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten worden in ieder geval berekend door:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150e, lid 1"@nl .

    id:Pensioenwet-20250301-150e-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150e&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het netto profijt is het verschil tussen de marktwaarde van de te verwachten pensioenuitkeringen en de marktwaarde van de toekomstige premie-inleg."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150e, lid 2"@nl .

    id:Pensioenwet-20250301-150e-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150e&lid=3&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. de pensioenovereenkomst voor de wijziging het karakter heeft van een premieovereenkomst of een kapitaalovereenkomst als bedoeld in artikel 1, zoals dat artikel luidde op de dag voorafgaand aan het tijdstip van inwerkingtreding van de Wet toekomst pensioenen;"@nl,
            "b. de pensioenovereenkomst na de wijziging het karakter heeft van een flexibele premieovereenkomst zonder risicodelingsreserve of een premie-uitkeringsovereenkomst; en"@nl,
            "c. geen gebruik wordt gemaakt van het overgangsrecht, bedoeld in artikel 220e."@nl ;
        def:hasExtref "jci1.3:c:BWBR0048328"^^xsd:string ;
        def:hasText "In afwijking van het eerste lid, onderdeel a, worden de effecten van de wijziging van de pensioenovereenkomst en de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten berekend door het bruto profijt van het ongewijzigd voortzetten van de pensioenovereenkomst af te zetten tegen het bruto profijt van het wijzigen van de pensioenovereenkomst en de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten, indien:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150e, lid 3"@nl .

    id:Pensioenwet-20250301-150e-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150e&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bruto profijt is de contante waarde van de toekomstige premie-inleg."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150e, lid 4"@nl .

    id:Pensioenwet-20250301-150e-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150e&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De pensioenuitvoerder stelt op de website een kwalitatieve toelichting op de transitie-effecten beschikbaar en verstrekt de deelnemer, gewezen deelnemer, gewezen partner en pensioengerechtigde op verzoek een berekening van de transitie-effecten per leeftijdscohort."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150e, lid 5"@nl .

    id:Pensioenwet-20250301-150e-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150e&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150e, lid 6"@nl .

    id:Pensioenwet-20250301-150e-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150e&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur kunnen kwantitatieve voorwaarden worden gesteld met betrekking tot de transitie-effecten, bedoeld in het eerste en derde lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150e, lid 7"@nl .

    id:Pensioenwet-20250301-150e-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150e&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De voordracht voor een krachtens het zevende lid vast te stellen algemene maatregel van bestuur wordt niet eerder gedaan dan vier weken nadat het ontwerp aan beide kamers der Staten-Generaal is overgelegd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150e, lid 8"@nl .

    id:Pensioenwet-20250301-150f-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150f&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. de werknemer heeft recht op compensatie als voor het leeftijdscohort waartoe de werknemer behoort compensatie is overeengekomen in de pensioenovereenkomst, waarbij niet is vereist dat de werknemer bij aanvang van de compensatieperiode reeds werkzaam was bij de werkgever;"@nl,
            "b. de compensatie wordt tijdsevenredig aan de werknemer toegekend over de compensatieperiode, die aanvangt op de ingangsdatum van de gewijzigde pensioenovereenkomst en uiterlijk 31 december 2036 eindigt; en"@nl,
            "c. de compensatie is gefinancierd op het moment dat de compensatie onvoorwaardelijk wordt toegekend."@nl ;
        def:hasText "Indien een pensioenovereenkomst afspraken bevat over compensatie in de vorm van het toekennen van extra pensioenaanspraken, wordt voldaan aan de volgende voorwaarden:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150f, lid 1"@nl .

    id:Pensioenwet-20250301-150f-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150f&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In aanvulling op de artikelen 38, eerste lid, en 39, eerste lid, verstrekt de pensioenuitvoerder voor zover van toepassing de deelnemer jaarlijks en bij beëindiging van de deelneming informatie over de compensatieperiode en de mogelijke effecten voor compensatie bij beëindiging van de deelneming en het aangaan van een nieuwe pensioenovereenkomst. De pensioenuitvoerder stelt deze informatie tevens op zijn website beschikbaar voor de deelnemer."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150f, lid 2"@nl .

    id:Pensioenwet-20250301-150f-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150f&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het eerste lid is van overeenkomstige toepassing op de gewezen werknemer die op grond van de pensioenovereenkomst recht heeft op premievrije voortzetting."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150f, lid 3"@nl .

    id:Pensioenwet-20250301-150g a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150g-1,
            id:Pensioenwet-20250301-150g-2,
            id:Pensioenwet-20250301-150g-3 ;
        def:hasTitle "Hoorrecht vereniging van gewezen deelnemers of pensioengerechtigden bij uitvoering door pensioenfonds"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150g"@nl .

    id:Pensioenwet-20250301-150g-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150g&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien de pensioenovereenkomst is ondergebracht bij een pensioenfonds stelt de werkgever, voor zover van toepassing, een vereniging van gewezen deelnemers die aantoont een substantieel gedeelte van alle gewezen deelnemers van het pensioenfonds te vertegenwoordigen of een vereniging van pensioengerechtigden die aantoont een substantieel gedeelte van alle pensioengerechtigden van het pensioenfonds te vertegenwoordigen in de gelegenheid een oordeel uit te spreken over het transitieplan. Het oordeel wordt op een zodanig tijdstip gevraagd dat het van invloed kan zijn op de inhoud van het transitieplan en de daarin verantwoorde keuzes. Indien een oordeel gegeven wordt, geeft de werkgever aan wat met het oordeel gedaan is."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150g, lid 1"@nl .

    id:Pensioenwet-20250301-150g-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150g&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Onverminderd artikel 115h is een pensioenfonds gehouden om op verzoek van gewezen deelnemers of een vereniging van gewezen deelnemers mee te werken aan de verstrekking van informatie aan de gewezen deelnemers van het fonds over het voornemen tot oprichting, of over het bestaan, van een vereniging van gewezen deelnemers."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150g, lid 2"@nl .

    id:Pensioenwet-20250301-150g-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150g&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel over in ieder geval de voorwaarde dat een vereniging van gewezen deelnemers dan wel vereniging van pensioengerechtigden een substantieel gedeelte van de gewezen deelnemers respectievelijk pensioengerechtigden dient te vertegenwoordigen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150g, lid 3"@nl .

    id:Pensioenwet-20250301-150h a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150h-1,
            id:Pensioenwet-20250301-150h-2,
            id:Pensioenwet-20250301-150h-3,
            id:Pensioenwet-20250301-150h-4 ;
        def:hasTitle "Transitiecommissie"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150h"@nl .

    id:Pensioenwet-20250301-150h-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150h&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Er is een onafhankelijke transitiecommissie."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150h, lid 1"@nl .

    id:Pensioenwet-20250301-150h-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150h&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De transitiecommissie heeft tot taak het bemiddelen tussen partijen die een pensioenovereenkomst sluiten, indien deze partijen daartoe gezamenlijk een verzoek doen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150h, lid 2"@nl .

    id:Pensioenwet-20250301-150h-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150h&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De transitiecommissie heeft tot taak het adviseren van partijen die een pensioenovereenkomst sluiten, indien deze partijen daartoe gezamenlijk een verzoek doen en zij overeenkomen zich te binden aan het advies van de transitiecommissie."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150h, lid 3"@nl .

    id:Pensioenwet-20250301-150h-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.3&artikel=150h&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel worden nadere regels gesteld met betrekking tot dit artikel, waarbij onder meer wordt geregeld voor welk tijdstippen een verzoek wordt gedaan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150h, lid 4"@nl .

    id:Pensioenwet-20250301-150i a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150i-1,
            id:Pensioenwet-20250301-150i-2,
            id:Pensioenwet-20250301-150i-3,
            id:Pensioenwet-20250301-150i-4,
            id:Pensioenwet-20250301-150i-5,
            id:Pensioenwet-20250301-150i-6 ;
        def:hasTitle "Implementatieplan"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150i"@nl .

    id:Pensioenwet-20250301-150i-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150i&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In het implementatieplan legt de pensioenuitvoerder schriftelijk vast op welke wijze voorbereidingen worden getroffen voor de uitvoering van de gewijzigde pensioenovereenkomst en invulling zal worden gegeven aan de uitvoering van de gewijzigde pensioenovereenkomst, alsmede de wijze waarop zal worden omgegaan met opgebouwde pensioenaanspraken en pensioenrechten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150i, lid 1"@nl .

    id:Pensioenwet-20250301-150i-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150i&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. de technische uitvoerbaarheid van de pensioenovereenkomst;"@nl,
            "b. de kosten die verband houden met de uitvoering van de pensioenovereenkomst;"@nl,
            "c. de risico’s die verband houden met de uitvoering van de pensioenovereenkomst;"@nl,
            "d. de risicobeheersmaatregelen die worden getroffen in verband met de uitvoering van de pensioenovereenkomst;"@nl,
            "e. de wijze waarop zal worden omgegaan met opgebouwde pensioenaanspraken en pensioenrechten;"@nl,
            "f. de wijze waarop uitvoering zal worden gegeven aan de pensioenovereenkomst met inachtneming van de toepasselijke wet- en regelgeving, waaronder de gelijkebehandelingswetgeving;"@nl,
            "g. een communicatieplan; en"@nl,
            "h. indien van toepassing, de effecten van het toepassen van het financieel toetsingskader tijdens de transitie en van toeslagverlening tussen 1 juli 2022 en de aanvang van de transitie op grond van de verwachting dat zal worden overgegaan tot een collectieve waardeoverdracht als bedoeld in artikel 150m."@nl ;
        def:hasText "Het implementatieplan bevat in ieder geval de volgende onderdelen:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150i, lid 2"@nl .

    id:Pensioenwet-20250301-150i-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150i&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien het implementatieplan wordt opgesteld door een pensioenfonds legt het pensioenfonds tevens vast, voor zover van toepassing, op welke wijze voorbereidingen worden getroffen voor en invulling zal worden gegeven aan de uitvoering van een besluit als bedoeld in artikel 150n."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150i, lid 3"@nl .

    id:Pensioenwet-20250301-150i-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150i&lid=4&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. bij uitvoering door een pensioenfonds: behoren tot hetzelfde financieel geheel; en"@nl,
            "b. bij uitvoering door verzekeraars of premiepensioeninstellingen: dezelfde karakteristieken hebben en gebaseerd zijn op hetzelfde producttype."@nl ;
        def:hasText "De pensioenuitvoerder stelt per pensioenregeling een implementatieplan op. In afwijking van de eerste zin kan een implementatieplan zich uitstrekken tot meerdere pensioenregelingen, voor zover de pensioenregelingen:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150i, lid 4"@nl .

    id:Pensioenwet-20250301-150i-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150i&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De pensioenuitvoerder dient het implementatieplan binnen twee weken na de afronding in bij de toezichthouder. De pensioenuitvoerder stelt het implementatieplan op zijn website beschikbaar voor de deelnemer, gewezen deelnemer, gewezen partner en pensioengerechtigde."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150i, lid 5"@nl .

    id:Pensioenwet-20250301-150i-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150i&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150i, lid 6"@nl .

    id:Pensioenwet-20250301-150j a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150j-1,
            id:Pensioenwet-20250301-150j-2,
            id:Pensioenwet-20250301-150j-3,
            id:Pensioenwet-20250301-150j-4 ;
        def:hasTitle "Communicatieplan"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150j"@nl .

    id:Pensioenwet-20250301-150j-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150j&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In het communicatieplan, bedoeld in artikel 150i, tweede lid, onderdeel g, legt de pensioenuitvoerder schriftelijk vast dat en op welke wijze de informatieverstrekking aan deelnemers, gewezen deelnemers, gewezen partners en pensioengerechtigden wordt ingevuld, en gaat daarbij ten minste in op de voor deelnemers, gewezen deelnemers, gewezen partners en pensioengerechtigden relevante gevolgen van de wijziging van de pensioenovereenkomst en de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150j, lid 1"@nl .

    id:Pensioenwet-20250301-150j-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150j&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. de hoogte van het reglementair te bereiken pensioen, de opgebouwde pensioenaanspraken of het pensioenrecht met en zonder wijziging van de pensioenovereenkomst, waarbij bij de informatie over de hoogte met wijziging van de pensioenovereenkomst rekening is gehouden met de effecten van de wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten. Voor zover de informatie betrekking heeft op het reglementair te bereiken ouderdomspensioen voor deelnemers of de opgebouwde aanspraken op ouderdomspensioen voor gewezen deelnemers, wordt deze informatie tevens weergegeven op basis van een pessimistisch scenario, een verwacht scenario en een optimistisch scenario;"@nl,
            "b. de mate waarin het verwacht scenario voor het reglementair te bereiken ouderdomspensioen voor deelnemers of de opgebouwde aanspraken op ouderdomspensioen voor gewezen deelnemers zich na ingang kan ontwikkelen op basis van een pessimistisch scenario, een verwacht scenario en een optimistisch scenario; en"@nl,
            "c. indien afspraken zijn gemaakt over compensatie in de vorm van het toekennen van extra pensioenaanspraken aan deelnemers, de inhoud van deze afspraken en het financieringsplan voor de compensatie waarbij inzichtelijk wordt gemaakt in welke mate welke bron ingezet zal worden."@nl ;
        def:hasText "Het communicatieplan bevat in ieder geval de wijze waarop deelnemers, gewezen deelnemers, gewezen partners en pensioengerechtigden, voor zover van toepassing, worden geïnformeerd over:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150j, lid 2"@nl .

    id:Pensioenwet-20250301-150j-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150j&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De pensioenuitvoerder verstrekt de informatie, bedoeld in het tweede lid, en de overige relevante informatie bij de transitie, aan deelnemers, gewezen deelnemers, gewezen partners en pensioengerechtigden voorafgaand aan het tijdstip, bedoeld in artikel 220i, tweede lid, op basis van een zo nauwkeurig mogelijke prognose en verstrekt na dit tijdstip de informatie, bedoeld in het tweede lid, zoals die definitief is vastgesteld, waarbij de pensioenuitvoerder de verschillen tussen de prognose en de vastgestelde informatie toelicht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150j, lid 3"@nl .

    id:Pensioenwet-20250301-150j-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.4&artikel=150j&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150j, lid 4"@nl .

    id:Pensioenwet-20250301-150k a def:LawArticle ;
        def:hasDescription " In aanvulling op artikel 25 wordt in de uitvoeringsovereenkomst, voor zover overeengekomen, een regeling opgenomen met betrekking tot de toekenning en financiering van de compensatie aan deelnemers door het toekennen van extra pensioenaanspraken."@nl ;
        def:hasTitle "Afspraken compensatie uitvoeringsovereenkomst"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150k"@nl .

    id:Pensioenwet-20250301-150l a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150l-1,
            id:Pensioenwet-20250301-150l-2,
            id:Pensioenwet-20250301-150l-3,
            id:Pensioenwet-20250301-150l-4,
            id:Pensioenwet-20250301-150l-5,
            id:Pensioenwet-20250301-150l-6,
            id:Pensioenwet-20250301-150l-7,
            id:Pensioenwet-20250301-150l-8 ;
        def:hasTitle "Standaard invaarpad"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150l"@nl .

    id:Pensioenwet-20250301-150l-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150l&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De wijze waarop wordt omgegaan met opgebouwde pensioenaanspraken en pensioenrechten als bedoeld in artikel 150d, tweede lid, onderdeel b, houdt voor pensioenfondsen in dat na de collectieve wijziging van de pensioenovereenkomsten door collectieve waardeoverdracht de waarde van de pensioenaanspraken en pensioenrechten wordt aangewend bij het pensioenfonds overeenkomstig de gewijzigde pensioenovereenkomsten, tenzij dit onevenredig ongunstig zou zijn voor deelnemers, gewezen deelnemers, andere aanspraakgerechtigden, pensioengerechtigden of de werkgever."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150l, lid 1"@nl .

    id:Pensioenwet-20250301-150l-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150l&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het besluit van de werkgever om het pensioenfonds niet te verzoeken over te gaan tot collectieve waardeoverdracht moet evenwichtig zijn en alle relevante belangen moeten in acht zijn genomen. De onderbouwing van dit besluit wordt opgenomen in het transitieplan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150l, lid 2"@nl .

    id:Pensioenwet-20250301-150l-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150l&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds meldt de werkgever of de analyse en de onderbouwing voor het afzien van collectieve waardeoverdracht wordt gedeeld en informeert het verantwoordingsorgaan, het belanghebbendenorgaan of de raad van toezicht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150l, lid 3"@nl .

    id:Pensioenwet-20250301-150l-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150l&lid=4&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. sprake is van strijd met wettelijke voorschriften;"@nl,
            "b. de effecten van de voorgenomen wijzigingen ten aanzien van het pensioen als geheel tot onevenwichtig nadeel zou leiden voor deelnemers, gewezen deelnemers, andere aanspraakgerechtigden of pensioengerechtigden; of"@nl,
            "c. de waardeoverdracht niet uitvoerbaar is binnen de grenzen van een beheerste en integere bedrijfsvoering."@nl ;
        def:hasText "Indien de werkgever een verzoek tot collectieve waardeoverdracht doet, wijst het pensioenfonds het verzoek van de werkgever alleen af indien:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150l, lid 4"@nl .

    id:Pensioenwet-20250301-150l-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150l&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij de collectieve waardeoverdracht, bedoeld in dit artikel, is, in afwijking van de artikelen 20 en 83, artikel 150m van toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150l, lid 5"@nl .

    id:Pensioenwet-20250301-150l-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150l&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0048328"^^xsd:string ;
        def:hasText "Indien de wijziging van de pensioenovereenkomsten betekent dat een pensioenfonds overgaat van uitvoering van een pensioenovereenkomst die het karakter heeft van een uitkeringsovereenkomst als bedoeld in artikel 1, zoals dat artikel luidde op de dag voorafgaand aan het tijdstip van inwerkingtreding van de Wet toekomst pensioenen, naar uitvoering van een flexibele premieovereenkomst, legt het pensioenfonds binnen een jaar na de collectieve waardeoverdracht, bedoeld in artikel 150m, gepensioneerden de keuze voor tussen een vastgestelde of variabele uitkering. In afwijking van artikel 70, derde lid, is artikel 80 van overeenkomstige toepassing. De artikelen 44a, 47a, 47b, 47c en 63b zijn van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150l, lid 6"@nl .

    id:Pensioenwet-20250301-150l-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150l&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor zover een pensioenfonds niet overgaat tot een collectieve waardeoverdracht als bedoeld in artikel 150m, worden de pensioenaanspraken en pensioenrechten die voortvloeien uit de pensioenovereenkomsten zoals die luidden voor de wijziging van deze pensioenovereenkomsten aangemerkt als een te onderscheiden financieel geheel. Artikel 125a is van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150l, lid 7"@nl .

    id:Pensioenwet-20250301-150l-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150l&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen regels worden gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150l, lid 8"@nl .

    id:Pensioenwet-20250301-150m a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150m-1,
            id:Pensioenwet-20250301-150m-10,
            id:Pensioenwet-20250301-150m-2,
            id:Pensioenwet-20250301-150m-3,
            id:Pensioenwet-20250301-150m-4,
            id:Pensioenwet-20250301-150m-5,
            id:Pensioenwet-20250301-150m-6,
            id:Pensioenwet-20250301-150m-7,
            id:Pensioenwet-20250301-150m-8,
            id:Pensioenwet-20250301-150m-9 ;
        def:hasTitle "Interne collectieve waardeoverdracht pensioenfondsen bij transitie"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m"@nl .

    id:Pensioenwet-20250301-150m-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds is op verzoek van de werkgever bevoegd tot collectieve waardeoverdracht indien de waardeoverdracht ertoe strekt in verband met een collectieve wijziging van de pensioenovereenkomsten de waarde van pensioenaanspraken of pensioenrechten aan te wenden bij het pensioenfonds overeenkomstig die gewijzigde pensioenovereenkomsten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 1"@nl .

    id:Pensioenwet-20250301-150m-10 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=10&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 10"@nl .

    id:Pensioenwet-20250301-150m-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het voornemen tot waardeoverdracht wordt door het pensioenfonds, onverwijld nadat het besluit tot waardeoverdracht is genomen maar uiterlijk zes maanden voor de beoogde datum van waardeoverdracht, elektronisch gemeld aan de toezichthouder. De toezichthouder kan binnen die periode een verbod tot waardeoverdracht opleggen. De toezichthouder kan de termijn van zes maanden gemotiveerd verlengen met maximaal twee maal drie maanden. De toezichthouder richt een proces in voor de melding van de interne collectieve waardeoverdracht en schrijft voor welke stukken daarvoor benodigd zijn."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 2"@nl .

    id:Pensioenwet-20250301-150m-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het intern toezicht bij een pensioenfonds houdt toezicht op de voorgenomen interne collectieve waardeoverdracht met inbegrip van het aanwenden van het vermogen en legt hierover verantwoording af in het bestuursverslag."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 3"@nl .

    id:Pensioenwet-20250301-150m-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds stelt het verantwoordingsorgaan in de gelegenheid advies uit te brengen over de voorgenomen collectieve waardeoverdracht met inbegrip van het aanwenden van het vermogen van het pensioenfonds bij de collectieve waardeoverdracht. Ten aanzien van dit advies zijn de artikelen 115a, vijfde en zesde lid, 115e en 217 van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 4"@nl .

    id:Pensioenwet-20250301-150m-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien het verantwoordingsorgaan of een geleding binnen het verantwoordingsorgaan negatief adviseert over de voorgenomen collectieve waardeoverdracht, informeert het pensioenfonds de werkgever die het verzoek tot collectieve waardeoverdracht heeft gedaan hierover en vraagt de werkgever het verzoek tot waardeoverdracht te heroverwegen met inachtneming van het advies van het verantwoordingsorgaan of een geleding van het verantwoordingsorgaan. De werkgever onderbouwt het resultaat van de heroverweging."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 5"@nl .

    id:Pensioenwet-20250301-150m-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds heeft de goedkeuring nodig van het belanghebbendenorgaan voor elk voorgenomen besluit met betrekking tot de collectieve waardeoverdracht met inbegrip van het aanwenden van het vermogen van het pensioenfonds bij de collectieve waardeoverdracht. Artikel 115c, vijfde, achtste, negende en tiende lid, is van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 6"@nl .

    id:Pensioenwet-20250301-150m-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds heeft de goedkeuring nodig van de raad van toezicht voor elk voorgenomen besluit met betrekking tot de collectieve waardeoverdracht met inbegrip van het aanwenden van het vermogen bij de collectieve waardeoverdracht. Artikel 104, vierde, achtste en negende lid, is van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 7"@nl .

    id:Pensioenwet-20250301-150m-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij de collectieve waardeoverdracht wordt de overdrachtswaarde door het pensioenfonds zodanig vastgesteld dat de voor mannen en vrouwen te verwerven pensioenrechten gelijk zijn waarbij aan het vereiste van collectieve actuariële gelijkwaardigheid op basis van dezelfde grondslagen wordt voldaan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 8"@nl .

    id:Pensioenwet-20250301-150m-9 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=6b&paragraaf=6b.5&artikel=150m&lid=9&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Elk beding strijdig met dit artikel is nietig."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 150m, lid 9"@nl .

    id:Pensioenwet-20250301-150n a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-150n-1,
            id:Pensioenwet-20250301-150n-10,

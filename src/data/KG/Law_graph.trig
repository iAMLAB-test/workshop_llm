    id:AlgemeneOuderdomswet-20250101-6a a def:LawArticle ;
        def:hasDescription " Zo nodig in afwijking van artikel 6 en de daarop berustende bepalingen: "@nl ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-6a-1,
            id:AlgemeneOuderdomswet-20250101-6a-2 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 6a"@nl .

    id:AlgemeneOuderdomswet-20250101-6a-1 a def:LawArticleSection ;
        def:hasText "a. wordt als verzekerde aangemerkt de persoon van wie de verzekering op grond van deze wet voortvloeit uit de toepassing van bepalingen van een verdrag of van een besluit van een volkenrechtelijke organisatie; "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 6a, Deel 1"@nl .

    id:AlgemeneOuderdomswet-20250101-6a-2 a def:LawArticleSection ;
        def:hasText "b. wordt niet als verzekerde aangemerkt de persoon op wie op grond van een verdrag of een besluit van een volkenrechtelijke organisatie de wetgeving van een andere mogendheid van toepassing is. "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 6a, Deel 2"@nl .

    id:AlgemeneOuderdomswet-20250101-7-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=7&lid=1&o=a&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasCases "a. de pensioengerechtigde leeftijd heeft bereikt, en "@nl,
            "b. ingevolge deze wet minimaal één kalenderjaar verzekerd is geweest in het tijdvak, aanvangende met de dag waarop de aanvangsleeftijd is bereikt en eindigende met de dag voorafgaande aan de dag waarop de pensioengerechtigde leeftijd is bereikt. "@nl ;
        def:hasText "Recht op ouderdomspensioen overeenkomstig de bepalingen van deze wet heeft degene, die "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 7, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-7-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=7&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Bij ministeriële regeling worden regels gesteld omtrent de herleiding van gedeelten van kalenderjaren tot gehele kalenderjaren."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 7, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-70 a def:LawArticle ;
        def:hasDescription " Hetgeen nog ter uitvoering van deze wet nodig is, wordt geregeld bij ministeriële regeling. "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 70"@nl .

    id:AlgemeneOuderdomswet-20250101-71 a def:LawArticle ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-71-1,
            id:AlgemeneOuderdomswet-20250101-71-2 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 71"@nl .

    id:AlgemeneOuderdomswet-20250101-71-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=X&artikel=71&lid=1&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Indien in een pensioenregeling van een pensioenfonds of van een werkgever bepalingen zijn of worden opgenomen krachtens welke op enigerlei wijze geheel of gedeeltelijk met het ouderdomspensioen ingevolge deze wet rekening wordt gehouden, dient bij de toepassing van deze bepalingen in acht te worden genomen, dat een verhoging van het ouderdomspensioen, welke plaatsvindt na de datum van beëindiging van de actieve deelneming aan de pensioenregeling, buiten beschouwing blijft. "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 71, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-71-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=X&artikel=71&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText " Het eerste lid is niet van toepassing op pensioenregelingen waarin bepalingen zijn opgenomen volgens welke het ouderdomspensioen ingevolge deze wet en de premievrije aanspraken of het pensioen van die regeling tezamen na beëindiging van de actieve deelneming jaarlijks ten minste worden verhoogd met het percentage van de in het eerste lid bedoelde verhoging van het ouderdomspensioen of het percentage van de loon- of prijsontwikkeling. "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 71, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-72 a def:LawArticle ;
        def:hasDescription " Vervallen"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 72"@nl .


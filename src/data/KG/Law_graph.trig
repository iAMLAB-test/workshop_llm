    id:Pensioenwet-20250301-16 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-16-1,
            id:Pensioenwet-20250301-16-2,
            id:Pensioenwet-20250301-16-3 ;
        def:hasTitle "Nadere eisen nabestaandenpensioen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 16"@nl .

    id:Pensioenwet-20250301-16-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=16&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een nabestaandenpensioen bij overlijden voor pensioendatum betreft een nabestaandenpensioen op risicobasis en de hoogte is diensttijdonafhankelijk;"@nl,
            "b. een partnerpensioen bij overlijden op of na pensioendatum betreft een partnerpensioen op opbouwbasis;"@nl,
            "c. indien sprake is van een partnerpensioen, voorziet de pensioenovereenkomst voor alle partnerrelaties in partnerpensioen en wordt geen onderscheid gemaakt al naar gelang het type partnerrelatie; en"@nl,
            "d. indien sprake is van een wezenpensioen wordt in de pensioenovereenkomst bepaald dat het wezenpensioen wordt uitgekeerd tot het kind 25 jaar wordt."@nl ;
        def:hasText "Indien een pensioenovereenkomst voorziet in een nabestaandenpensioen wordt voldaan aan de volgende voorwaarden:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 16, lid 1"@nl .

    id:Pensioenwet-20250301-16-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=16&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor de toepassing van deze wet kan een werknemer of gewezen werknemer op enig moment slechts één partner hebben. Indien de werknemer of gewezen werknemer op dat moment meer dan één partner zou hebben, wordt alleen de partner uit de oudste relatie als partner in de zin van deze wet aangemerkt."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 16, lid 2"@nl .

    id:Pensioenwet-20250301-16-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=16&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Elk beding in strijd met dit artikel is nietig."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 16, lid 3"@nl .


    id:UitvoeringsbesluitLoonbelasting1965-20250101-10-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002489&hoofdstuk=4&artikel=10&lid=1&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002471&artikel=18a"^^xsd:string ;
        def:hasText "In de premie, bedoeld in artikel 18a, eerste lid, van de wet, zijn kosten voor vermogensbeheer en voor het afdekken van beleggingsrisico’s begrepen. Het percentage van artikel 18a, eerste lid, van de wet wordt verhoogd met overige kosten ten behoeve van een aanspraak ingevolge een pensioenregeling. De overige kosten, bedoeld in de tweede zin, kunnen niet worden aangewend voor een ouderdomspensioen en partnerpensioen bij overlijden op of na pensioendatum."@nl ;
        def:referencesTo id:WetOpLoonbelasting1964-20250101-18a ;
        rdfs1:label "Uitvoeringsbesluit loonbelasting 1965, 2025-01-01, Artikel 10, lid 1"@nl .

    id:UitvoeringsbesluitLoonbelasting1965-20250101-10-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002489&hoofdstuk=4&artikel=10&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002471&artikel=38r"^^xsd:string ;
        def:hasText "Het eerste lid is van overeenkomstige toepassing op de maximale premie, bedoeld in artikel 38r, eerste lid, van de wet."@nl ;
        def:referencesTo id:WetOpLoonbelasting1964-20250101-38r ;
        rdfs1:label "Uitvoeringsbesluit loonbelasting 1965, 2025-01-01, Artikel 10, lid 2"@nl .


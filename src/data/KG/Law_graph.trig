    id:WetOpLoonbelasting1964-20250101-39f a def:LawArticle ;
        def:hasSection id:WetOpLoonbelasting1964-20250101-39f-1,
            id:WetOpLoonbelasting1964-20250101-39f-2,
            id:WetOpLoonbelasting1964-20250101-39f-3,
            id:WetOpLoonbelasting1964-20250101-39f-4,

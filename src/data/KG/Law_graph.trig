    id:Pensioenwet-20250301-163 a def:LawArticle ;
        def:hasDescription " Onze Minister houdt toezicht op:"@nl ;
        def:hasSection id:Pensioenwet-20250301-163-1,
            id:Pensioenwet-20250301-163-2 ;
        def:hasTitle "Uitvoeringstoezicht"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 163"@nl .

    id:Pensioenwet-20250301-163-1 a def:LawArticleSection ;
        def:hasText "a. de rechtmatigheid van de uitvoering van deze wet door de toezichthouder;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 163, Deel 1"@nl .

    id:Pensioenwet-20250301-163-2 a def:LawArticleSection ;
        def:hasText "b. de doeltreffendheid van de uitvoering van deze wet door de toezichthouder."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 163, Deel 2"@nl .

    id:Pensioenwet-20250301-164 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-164-1,
            id:Pensioenwet-20250301-164-2,
            id:Pensioenwet-20250301-164-3,
            id:Pensioenwet-20250301-164-4,
            id:Pensioenwet-20250301-164-5,
            id:Pensioenwet-20250301-164-6,
            id:Pensioenwet-20250301-164-7 ;
        def:hasTitle "Verstrekking informatie ten behoeve van toezicht door Onze Minister"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 164"@nl .

    id:Pensioenwet-20250301-164-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.3&artikel=164&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien Onze Minister de toezichthouder gegevens of inlichtingen vraagt die naar zijn oordeel nodig zijn voor een onderzoek naar de wijze waarop de toezichthouder de uit deze wet voortvloeiende taken en bevoegdheden uitvoert of heeft uitgevoerd, geschiedt de verstrekking daarvan met inachtneming van dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 164, lid 1"@nl .

    id:Pensioenwet-20250301-164-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.3&artikel=164&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder verstrekt kosteloos aan Onze Minister de in het eerste lid bedoelde gegevens of inlichtingen. Indien Onze Minister de toezichthouder vraagt bepaalde gegevens of inlichtingen te verstrekken die op grond van deze wet omtrent afzonderlijke pensioenuitvoerders, werkgevers of natuurlijke personen zijn verstrekt of zijn verkregen, is de toezichthouder niet verplicht deze gegevens of inlichtingen te verstrekken, indien deze betrekking hebben op of herleidbaar zijn tot een afzonderlijke pensioenuitvoerder, werkgever of natuurlijke persoon, met uitzondering van gegevens of inlichtingen die betrekking hebben op of herleidbaar zijn tot een pensioenuitvoerder of werkgever ten aanzien waarvan surséance van betaling is verleend, die in staat van faillissement is verklaard of op grond van een rechterlijke uitspraak is ontbonden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 164, lid 2"@nl .

    id:Pensioenwet-20250301-164-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.3&artikel=164&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Onze Minister kan een derde opdragen de gegevens of inlichtingen die hem ingevolge het tweede lid zijn verstrekt te onderzoeken en aan hem verslag uit te brengen. Tevens kan Onze Minister de derde die in zijn opdracht handelt, machtigen namens hem gegevens of inlichtingen in te winnen, in welk geval het eerste en tweede lid van overeenkomstige toepassing zijn."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 164, lid 3"@nl .

    id:Pensioenwet-20250301-164-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.3&artikel=164&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Onze Minister gebruikt de gegevens of inlichtingen die hij ingevolge het tweede of derde lid heeft verkregen uitsluitend voor het vormen van zijn oordeel over de toereikendheid van deze wet of de wijze waarop de toezichthouder de bij of krachtens deze wet opgedragen taken en toegekende bevoegdheden uitoefent of heeft uitgeoefend."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 164, lid 4"@nl .

    id:Pensioenwet-20250301-164-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.3&artikel=164&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Onze Minister en degenen die in zijn opdracht handelen zijn verplicht tot geheimhouding van de op grond van het tweede lid, tweede volzin, ontvangen gegevens of inlichtingen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 164, lid 5"@nl .

    id:Pensioenwet-20250301-164-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.3&artikel=164&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Niettegenstaande het vierde en vijfde lid kan Onze Minister de aan de gegevens of inlichtingen ontleende bevindingen en de daaruit getrokken conclusies aan de Staten-Generaal mededelen en de conclusies in algemene zin uit het onderzoek openbaar maken."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 164, lid 6"@nl .

    id:Pensioenwet-20250301-164-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.3&artikel=164&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0003372"^^xsd:string,
            "jci1.3:c:BWBR0045754"^^xsd:string ;
        def:hasText "De Wet open overheid en de Wet Nationale ombudsman zijn niet van toepassing met betrekking tot de in dit artikel bedoelde gegevens of inlichtingen die Onze Minister of de in zijn opdracht werkende derde onder zich heeft."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 164, lid 7"@nl .

    id:Pensioenwet-20250301-165 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-165-1,
            id:Pensioenwet-20250301-165-2 ;
        def:hasTitle "Aanwijzing door Onze Minister"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 165"@nl .

    id:Pensioenwet-20250301-165-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.3&artikel=165&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Onze Minister kan aan de toezichthouder een aanwijzing geven over de uitoefening van de aan de toezichthouder bij of krachtens deze wet opgedragen taken en toegekende bevoegdheden wanneer de toezichthouder hierin naar het oordeel van Onze Minister tekort schiet. Onze Minister treedt daarbij niet in individuele gevallen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 165, lid 1"@nl .

    id:Pensioenwet-20250301-165-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.3&artikel=165&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder is gehouden overeenkomstig de aanwijzing te handelen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 165, lid 2"@nl .

    id:Pensioenwet-20250301-166 a def:LawArticle ;
        def:hasDescription " De bevoegdheid om de noodzakelijke voorzieningen te treffen, genoemd in artikel 23, eerste lid, van de Kaderwet zelfstandige bestuursorganen, alsmede de verplichtingen, genoemd in artikel 23, tweede en derde lid, van de Kaderwet zelfstandige bestuursorganen, worden ten aanzien van ernstige verwaarlozing door de toezichthouder van de bij of krachtens deze wet opgedragen taken uitgeoefend dan wel uitgevoerd door Onze Minister."@nl ;
        def:hasTitle "Taakverwaarlozing"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 166"@nl .

    id:Pensioenwet-20250301-166a a def:LawArticle ;
        def:hasDescription " Onze Minister kan beleidsregels vaststellen ten aanzien van de toepassing door de toezichthouders van Hoofdstuk 7."@nl ;
        def:hasTitle "Beleidsregels door Onze Minister"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 166a"@nl .

    id:Pensioenwet-20250301-167 a def:LawArticle ;
        def:hasDescription " De pensioenuitvoerder, de werkgever, de accountant en de actuaris verstrekken aan de toezichthouder kosteloos de door deze gevorderde inlichtingen, gegevens en bescheiden."@nl ;
        def:hasTitle "Kosteloze informatieverstrekking"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 167"@nl .

    id:Pensioenwet-20250301-168 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-168-1,
            id:Pensioenwet-20250301-168-2,
            id:Pensioenwet-20250301-168-3,
            id:Pensioenwet-20250301-168-4,
            id:Pensioenwet-20250301-168-5 ;
        def:hasTitle "Inlichtingenbevoegdheid toezichthouder"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 168"@nl .

    id:Pensioenwet-20250301-168-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=168&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder kan ten behoeve van het toezicht op de naleving van deze wet van een ieder inlichtingen vorderen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 168, lid 1"@nl .

    id:Pensioenwet-20250301-168-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=168&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0005537&artikel=5:13"^^xsd:string,
            "jci1.3:c:BWBR0005537&artikel=5:20"^^xsd:string ;
        def:hasText "De artikelen 5:13 en 5:20, eerste en tweede lid, van de Algemene wet bestuursrecht zijn van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 168, lid 2"@nl .

    id:Pensioenwet-20250301-168-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=168&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0005537&artikel=5:20"^^xsd:string ;
        def:hasText "De toezichthouder is bevoegd tot toepassing van artikel 5:20, derde lid, van de Algemene wet bestuursrecht ten aanzien van de in het eerste lid bedoelde vordering."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 168, lid 3"@nl .

    id:Pensioenwet-20250301-168-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=168&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voorzover de toezichthouder voor het uitoefenen van het gedragstoezicht ten aanzien van pensioenfondsen waaraan de andere toezichthouder een vergunning heeft verleend of welke in het register is opgenomen, gegevens nodig heeft over aspecten van de bedrijfsvoering, bedoeld in artikel 143, tweede lid, onderdeel a en b, vordert de eerstgenoemde toezichthouder geen inlichtingen, dan nadat de andere toezichthouder is verzocht deze gegevens te verstrekken en is gebleken dat de andere toezichthouder niet aan dit verzoek tegemoet kan komen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 168, lid 4"@nl .

    id:Pensioenwet-20250301-168-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=168&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Van het vierde lid kan, na overleg met de andere toezichthouder, worden afgeweken indien sprake is van een redelijk vermoeden van een overtreding van de regels bij of krachtens deze wet gesteld en onverwijlde spoed, gelet op de betrokken belangen, dat vereist."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 168, lid 5"@nl .

    id:Pensioenwet-20250301-169 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-169-1,
            id:Pensioenwet-20250301-169-2 ;
        def:hasTitle "Bewaarplicht gegevens ten behoeve van toezichthouder"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 169"@nl .

    id:Pensioenwet-20250301-169-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=169&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De pensioenuitvoerder en de werkgever zijn verplicht de zakelijke gegevens en bescheiden die betrekking hebben op pensioenregelingen en andere bij of krachtens deze wet geregelde onderwerpen in Nederland beschikbaar te hebben en deze gedurende ten minste zeven jaren na het boekjaar waarop ze betrekking hebben beschikbaar te houden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 169, lid 1"@nl .

    id:Pensioenwet-20250301-169-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=169&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het eerste lid is niet van toepassing op een verzekeraar met zetel in een andere lidstaat van de Europese Unie."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 169, lid 2"@nl .

    id:Pensioenwet-20250301-17 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-17-1,
            id:Pensioenwet-20250301-17-2,
            id:Pensioenwet-20250301-17-3,
            id:Pensioenwet-20250301-17-4,
            id:Pensioenwet-20250301-17-5 ;
        def:hasTitle "Gelijke premie"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 17"@nl .

    id:Pensioenwet-20250301-17-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=17&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De door of voor een deelnemer verschuldigde premie voor pensioen op opbouwbasis bedraagt voor alle deelnemers een gelijk percentage van het loon dat voor de pensioenberekening in aanmerking wordt genomen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 17, lid 1"@nl .

    id:Pensioenwet-20250301-17-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=17&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De door of voor een deelnemer verschuldigde premie voor pensioen op risicobasis bedraagt bij een pensioenregeling uitgevoerd door een verplichtgesteld bedrijfstakpensioenfonds een gelijk percentage van het loon dat voor de pensioenberekening in aanmerking wordt genomen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 17, lid 2"@nl .

    id:Pensioenwet-20250301-17-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=17&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor verschillende vormen van pensioen en voor verschillende pensioenregelingen kunnen verschillende premies worden vastgesteld. Voor verschillende pensioenregelingen die worden uitgevoerd door hetzelfde verplichtgestelde bedrijfstakpensioenfonds kunnen geen verschillende premies worden vastgesteld indien die pensioenregelingen dezelfde of nagenoeg dezelfde inhoud hebben."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 17, lid 3"@nl .

    id:Pensioenwet-20250301-17-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=17&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het eerste en tweede lid zijn niet van toepassing op de premie voor vrijwillige pensioenregelingen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 17, lid 4"@nl .

    id:Pensioenwet-20250301-17-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=17&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De tweede zin van het derde lid is niet van toepassing indien bij een verplichtgesteld bedrijfstakpensioenfonds sprake is van een regeling die voor alle deelnemers geldt en de actuariële waarde van de uit die regeling voortvloeiende verplichtingen ten minste twee derde van de actuariële waarde van de uit het totaal van de pensioenregelingen van het bedrijfstakpensioenfonds anders dan regelingen voor nabestaanden- of arbeidsongeschiktheidspensioen, voortvloeiende verplichtingen met uitzondering van de uit de vrijwillige pensioenvoorzieningen voortvloeiende verplichtingen, betreft."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 17, lid 5"@nl .

    id:Pensioenwet-20250301-170 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-170-1,
            id:Pensioenwet-20250301-170-2,
            id:Pensioenwet-20250301-170-3,
            id:Pensioenwet-20250301-170-4,
            id:Pensioenwet-20250301-170-5 ;
        def:hasTitle "Informatie- en meldingsverplichting accountant en actuaris"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 170"@nl .

    id:Pensioenwet-20250301-170-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=170&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. in strijd is met deze wet;"@nl,
            "b. de nakoming van de door het pensioenfonds aangegane verplichtingen bedreigt; of"@nl,
            "c. leidt tot de weigering van het afgeven van de verklaring omtrent de getrouwheid of tot het maken van voorbehouden."@nl ;
        def:hasText "Een accountant die het onderzoek naar de staten, bedoeld in artikel 147, vijfde lid, uitvoert meldt de toezichthouder zo spoedig mogelijk elke omstandigheid waarvan hij bij de uitvoering van het onderzoek kennis heeft gekregen en die:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 170, lid 1"@nl .

    id:Pensioenwet-20250301-170-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=170&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het eerste lid, aanhef en onderdelen a en b, is van overeenkomstige toepassing op de actuaris die het onderzoek naar de staten, bedoeld in artikel 147, vierde lid, uitvoert."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 170, lid 2"@nl .

    id:Pensioenwet-20250301-170-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=170&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De accountant of actuaris verstrekt zo spoedig mogelijk kosteloos alle inlichtingen aan de toezichthouder die deze redelijkerwijs nodig heeft voor het toezicht op de naleving van deze wet. De toezichthouder stelt het betrokken pensioenfonds in de gelegenheid aanwezig te zijn bij het verstrekken van inlichtingen door de accountant of actuaris."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 170, lid 3"@nl .

    id:Pensioenwet-20250301-170-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=170&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De accountant of actuaris biedt desgevraagd de toezichthouder inzicht in zijn controlewerkzaamheden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 170, lid 4"@nl .

    id:Pensioenwet-20250301-170-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=170&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De accountant of actuaris die op grond van dit artikel tot een melding of het verstrekken van inlichtingen aan de toezichthouder is overgegaan, is niet aansprakelijk voor schade die een derde als gevolg daarvan lijdt, tenzij aannemelijk wordt gemaakt dat, gelet op alle feiten en omstandigheden, in redelijkheid niet tot melding of het verstrekken van inlichtingen had mogen worden overgegaan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 170, lid 5"@nl .

    id:Pensioenwet-20250301-171 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-171-1,
            id:Pensioenwet-20250301-171-2 ;
        def:hasTitle "Aanwijzing"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 171"@nl .

    id:Pensioenwet-20250301-171-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=171&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder kan een pensioenuitvoerder die niet voldoet aan hetgeen bij of krachtens deze wet is bepaald, door middel van het geven van een aanwijzing verplichten om binnen een door de toezichthouder gestelde redelijke termijn ten aanzien van in de aanwijzingsbeschikking aangegeven punten een bepaalde gedragslijn te volgen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 171, lid 1"@nl .

    id:Pensioenwet-20250301-171-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=171&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder kan een aanwijzing als bedoeld in het eerste lid eveneens aan een pensioenfonds geven indien hij tekenen ontwaart van een ontwikkeling die het eigen vermogen, de solvabiliteit, de liquiditeit of de bedrijfsvoering van het pensioenfonds in gevaar kunnen brengen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 171, lid 2"@nl .

    id:Pensioenwet-20250301-172 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-172-1,
            id:Pensioenwet-20250301-172-2,
            id:Pensioenwet-20250301-172-3,
            id:Pensioenwet-20250301-172-4,
            id:Pensioenwet-20250301-172-5,
            id:Pensioenwet-20250301-172-6,
            id:Pensioenwet-20250301-172-7,
            id:Pensioenwet-20250301-172-8 ;
        def:hasTitle "Benoeming curator"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 172"@nl .

    id:Pensioenwet-20250301-172-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=172&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder kan besluiten een of meer personen te benoemen als curator ten aanzien van alle of bepaalde organen van het pensioenfonds indien dat pensioenfonds niet voldoet aan hetgeen ingevolge deze wet is bepaald."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 172, lid 1"@nl .

    id:Pensioenwet-20250301-172-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=172&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. nadat door het pensioenfonds niet of niet volledig binnen de gestelde termijn aan een aanwijzing als bedoeld in artikel 171 gevolg is gegeven;"@nl,
            "b. indien de in het eerste lid bedoelde overtreding een adequate functionering van het pensioenfonds ernstig in gevaar brengt en dat pensioenfonds voorafgaand in de gelegenheid is gesteld zijn zienswijze naar voren te brengen over het voorgenomen besluit; of"@nl,
            "c. indien de in het eerste lid bedoelde overtreding de belangen van de aanspraak- en pensioengerechtigden ernstig in gevaar brengt en dat pensioenfonds voorafgaand in de gelegenheid is gesteld zijn zienswijze naar voren te brengen over het voorgenomen besluit."@nl ;
        def:hasText "Het besluit ingevolge het eerste lid wordt slechts genomen:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 172, lid 2"@nl .

    id:Pensioenwet-20250301-172-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=172&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Onverminderd het eerste en tweede lid kan de toezichthouder besluiten een of meer personen te benoemen als curator ten aanzien van alle of bepaalde organen of vertegenwoordigers van een pensioenfonds indien hij bij dat pensioenfonds tekenen ontwaart van een ontwikkeling die het eigen vermogen, de solvabiliteit, of de liquiditeit van dat pensioenfonds in gevaar kunnen brengen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 172, lid 3"@nl .

    id:Pensioenwet-20250301-172-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=172&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het benoemingsbesluit bevat onder meer een beschrijving van de belangen waardoor de curator zich dient te laten leiden. De toezichthouder benoemt de curator voor ten hoogste twee jaren, met de mogelijkheid om deze termijn telkens voor ten hoogste een jaar te verlengen; de verlenging wordt terstond van kracht. Met ingang van het tijdstip waarop het besluit tot benoeming van de curator aan het pensioenfonds is bekendgemaakt mogen de desbetreffende organen of vertegenwoordigers hun bevoegdheden slechts uitoefenen na goedkeuring door de curator en met inachtneming van de opdrachten van de curator."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 172, lid 4"@nl .

    id:Pensioenwet-20250301-172-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=172&lid=5&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. verlenen de organen en de vertegenwoordigers van het pensioenfonds de curator alle medewerking;"@nl,
            "b. kan de toezichthouder de betrokken organen of vertegenwoordigers van het pensioenfonds toestaan bepaalde rechtshandelingen zonder goedkeuring te verrichten;"@nl,
            "c. kan de toezichthouder te allen tijde de door hem aangewezen curator vervangen;"@nl,
            "d. is voor schade ten gevolge van handelingen, die zijn verricht in strijd met een besluit als bedoeld in het eerste of derde lid, elke persoon die deel uitmaakt van het orgaan van het pensioenfonds dat deze handelingen verrichtte, hoofdelijk aansprakelijk tegenover het pensioenfonds, tenzij het verrichten van deze handelingen niet aan hem is te verwijten en hij niet nalatig is geweest in het treffen van maatregelen om de gevolgen daarvan af te wenden;"@nl,
            "e. zijn de handelingen, bedoeld in onderdeel d, voorzover deze rechtshandelingen zijn, vernietigbaar, indien de wederpartij wist of behoorde te weten dat de vereiste goedkeuring ontbrak."@nl ;
        def:hasText "Na de benoeming van een curator:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 172, lid 5"@nl .

    id:Pensioenwet-20250301-172-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=172&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Zodra de omstandigheid, bedoeld in het eerste of derde lid niet langer aanwezig is, trekt de toezichthouder het besluit tot benoeming van de curator in. De toezichthouder maakt het besluit tot intrekking onverwijld bekend aan het pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 172, lid 6"@nl .

    id:Pensioenwet-20250301-172-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=172&lid=7&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het pensioenfonds of, wanneer de financiële omstandigheden van het pensioenfonds dit niet toestaan;"@nl,
            "b. de werkgever of, wanneer de financiële omstandigheden van de werkgever dit niet toestaan;"@nl,
            "c. de toezichthouder."@nl ;
        def:hasText "De toezichthouder kent aan een op grond van het eerste lid aangewezen persoon een bezoldiging toe. De bezoldiging komt ten laste van:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 172, lid 7"@nl .

    id:Pensioenwet-20250301-172-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=172&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De curator is niet aansprakelijk voor schade veroorzaakt door een handelen of nalaten in de uitoefening van de taak op grond van dit artikel, tenzij deze schade in belangrijke mate het gevolg is van een opzettelijk onbehoorlijke taakuitoefening of een opzettelijk onbehoorlijke uitoefening van bevoegdheden of in belangrijke mate te wijten is aan grove schuld."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 172, lid 8"@nl .

    id:Pensioenwet-20250301-173 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-173-1,
            id:Pensioenwet-20250301-173-2,
            id:Pensioenwet-20250301-173-3,
            id:Pensioenwet-20250301-173-4,
            id:Pensioenwet-20250301-173-5 ;
        def:hasTitle "Bewindvoerder over een pensioenfonds"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 173"@nl .

    id:Pensioenwet-20250301-173-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=173&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een pensioenfonds blijk geeft van een zodanig wanbeleid dat de belangen van de aanspraak- en pensioengerechtigden een onmiddellijke voorziening vereisen; of"@nl,
            "b. het bestuur is komen te ontbreken."@nl ;
        def:hasText "Op verzoek van de toezichthouder kan de ondernemingskamer van het gerechtshof Amsterdam over een pensioenfonds een bewindvoerder aanstellen, indien:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 173, lid 1"@nl .

    id:Pensioenwet-20250301-173-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=173&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder dient zijn verzoekschrift tot aanstelling van een bewindvoerder in tweevoud in. De griffier doet een exemplaar van het verzoekschrift onverwijld aan het pensioenfonds toekomen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 173, lid 2"@nl .

    id:Pensioenwet-20250301-173-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=173&lid=3&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het pensioenfonds of, wanneer de financiële omstandigheden van het pensioenfonds dit niet toestaan;"@nl,
            "b. de werkgever of, wanneer de financiële omstandigheden van de werkgever dit niet toestaan;"@nl,
            "c. de toezichthouder."@nl ;
        def:hasText "Indien de ondernemingskamer het verzoek toewijst, bepaalt zij de duur waarvoor de bewindvoerder wordt aangesteld. Zij kan deze duur op verzoek van de toezichthouder of van de bewindvoerder verlengen dan wel verkorten. De ondernemingskamer kent de bewindvoerder een bezoldiging toe ten laste van:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 173, lid 3"@nl .

    id:Pensioenwet-20250301-173-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=173&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De bewindvoerder treedt in de plaats van het bestuur of een of meerdere door de ondernemingskamer aangewezen leden van het bestuur van het pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 173, lid 4"@nl .

    id:Pensioenwet-20250301-173-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=173&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De voorlopige tenuitvoerlegging van de beschikking tot aanstelling van een bewindvoerder kan worden bevolen, indien het verzoek daartoe is gedaan op een van de gronden, genoemd in het eerste lid, onderdelen a en b."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 173, lid 5"@nl .

    id:Pensioenwet-20250301-174 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-174-1,
            id:Pensioenwet-20250301-174-2 ;
        def:hasTitle "Onbevoegd verklaring accountant of actuaris"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 174"@nl .

    id:Pensioenwet-20250301-174-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=174&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien een accountant of actuaris niet of niet meer de nodige waarborgen biedt dat deze zijn taak met betrekking tot het pensioenfonds naar behoren zal kunnen vervullen, kan de toezichthouder ten aanzien van deze accountant of actuaris bepalen dat hij niet langer bevoegd is de in deze wet bedoelde verklaringen met betrekking tot dat pensioenfonds af te leggen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 174, lid 1"@nl .

    id:Pensioenwet-20250301-174-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=174&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder doet mededeling van het besluit, bedoeld in het eerste lid, aan het pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 174, lid 2"@nl .

    id:Pensioenwet-20250301-175 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-175-1,
            id:Pensioenwet-20250301-175-2 ;
        def:hasTitle "Last onder dwangsom"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 175"@nl .

    id:Pensioenwet-20250301-175-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=175&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder kan een last onder dwangsom opleggen ter zake van een overtreding van de voorschriften, gesteld bij of krachtens deze wet."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 175, lid 1"@nl .

    id:Pensioenwet-20250301-175-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=175&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij regeling van Onze Minister kunnen regels worden gesteld ter zake van de uitoefening van de bevoegdheid tot het opleggen van een last onder dwangsom."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 175, lid 2"@nl .

    id:Pensioenwet-20250301-176 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-176-1,
            id:Pensioenwet-20250301-176-2 ;
        def:hasTitle "Bestuurlijke boete"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 176"@nl .

    id:Pensioenwet-20250301-176-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=176&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0005537&artikel=5:20"^^xsd:string ;
        def:hasText "De toezichthouder kan een bestuurlijke boete opleggen ter zake van een overtreding van voorschriften, gesteld bij of krachtens de artikelen 10a, 10b, 10c, 10d, 10e, 17, 21, eerste lid, tweede lid, tweede volzin en vierde lid, 23, 25, 26, 28, 29, eerste lid, 29, zevende lid, voor zover het betreft de overeenkomstige toepassing van artikel 29, eerste lid, 34 tot en met 48, 48a, 48b, 48c, 49, 50, tweede en vierde lid, 51, 51a, 52, 52a, 52b, 58, 60, 61, 61a, 62, 63, 63b, 66, vierde tot en met zesde, achtste en tiende lid, 67, tweede lid, 68, tweede lid, 69, vierde, vijfde en achtste lid, 70a, derde, vierde, vijfde en zevende lid, 71, eerste tot en met vijfde en zevende lid, 74, tweede en derde lid, 76, eerste tot en met vierde en zevende lid, 83, tweede en vierde lid, 84, tweede en vierde lid, 85, eerste lid, 86, eerste en tweede lid, 87, 91, 94, tweede lid, 96, 99 tot en met 107, 111, 112, 112a, 112b, 113, 115, 115a, 115b, 115c, 115e, 115f, 115g, 115h, 116, 117, 120, eerste en tweede lid, 125, 129, 134, 135, 136, 137, 138, 139, 140, 143, 145, 146, 147, eerste tot en met derde, vijfde en zesde lid, 150, 150a, eerste en vijfde lid, 150g, tweede lid, 150i, 150j, 150k, 150l, derde en vierde lid, 150m, tweede tot en met achtste en tiende lid, 150p, derde tot en met zesde lid, 150q, tweede en vierde lid, 167, 169, 170, eerste tot en met vierde lid, 171, eerste lid, 172, vijfde lid, 194, 197, 199, 203, derde en vierde lid, 204 en van artikel 5:20, eerste lid, van de Algemene wet bestuursrecht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 176, lid 1"@nl .

    id:Pensioenwet-20250301-176-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=176&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij algemene maatregel van bestuur worden regels gesteld met betrekking tot de uitoefening van de bevoegdheid, bedoeld in het eerste lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 176, lid 2"@nl .

    id:Pensioenwet-20250301-177 a def:LawArticle ;
        def:hasDescription " Vervallen"@nl ;
        def:hasTitle "Overtreder"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 177"@nl .


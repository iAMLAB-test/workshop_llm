            id:Pensioenwet-20250301-112a-7,
            id:Pensioenwet-20250301-112a-8,
            id:Pensioenwet-20250301-112a-9 ;
        def:hasTitle "Vergunning en weerstandsvermogen algemeen pensioenfonds"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a"@nl .

    id:Pensioenwet-20250301-112a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het is verboden zonder een daartoe door de toezichthouder verleende vergunning in Nederland het bedrijf van een algemeen pensioenfonds uit te oefenen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a, lid 1"@nl .

    id:Pensioenwet-20250301-112a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112a&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het achtste lid en de artikelen 33 en 34;"@nl,
            "b. een van de artikelen 100, 101 of 101a; en"@nl,
            "c. de artikelen 102a, 103, derde of vierde lid, 104, 105, eerste lid, 106, 106a, 111 en 143."@nl ;
        def:hasText "De toezichthouder verleent op aanvraag een vergunning als bedoeld in het eerste lid indien de aanvrager zetel heeft in Nederland en aantoont dat zal worden voldaan aan:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a, lid 2"@nl .

    id:Pensioenwet-20250301-112a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112a&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De aanvraag van de vergunning geschiedt onder opgave van bij of krachtens algemene maatregel van bestuur te bepalen gegevens."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a, lid 3"@nl .

    id:Pensioenwet-20250301-112a-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112a&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Aan de vergunning kunnen voorschriften worden verbonden en beperkingen worden gesteld met het oog op de belangen die deze wet beoogt te beschermen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a, lid 4"@nl .

    id:Pensioenwet-20250301-112a-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112a&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder kan de door hem verleende vergunning wijzigen of intrekken."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a, lid 5"@nl .

    id:Pensioenwet-20250301-112a-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112a&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De vergunning is persoonlijk en niet overdraagbaar."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a, lid 6"@nl .

    id:Pensioenwet-20250301-112a-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112a&lid=7&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. dat gestart is met het gebruikmaken van de vergunning; en"@nl,
            "b. het aanhouden van een nieuw afgescheiden vermogen."@nl ;
        def:hasText "Een algemeen pensioenfonds meldt binnen drie maanden aan de toezichthouder:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a, lid 7"@nl .

    id:Pensioenwet-20250301-112a-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112a&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een algemeen pensioenfonds beschikt over voldoende weerstandsvermogen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a, lid 8"@nl .

    id:Pensioenwet-20250301-112a-9 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112a&lid=9&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel die onder meer betrekking hebben op de aanvraag, de procedure, de omstandigheden die kunnen leiden tot het wijzigen of intrekken van de vergunning en het weerstandsvermogen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112a, lid 9"@nl .

    id:Pensioenwet-20250301-112b a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-112b-1,
            id:Pensioenwet-20250301-112b-2 ;
        def:hasTitle "Fusie of splitsing pensioenfonds"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112b"@nl .

    id:Pensioenwet-20250301-112b-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112b&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een pensioenfonds meldt een voorgenomen fusie of splitsing van het pensioenfonds uiterlijk drie maanden voor de beoogde datum van de fusie of splitsing aan de toezichthouder."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112b, lid 1"@nl .

    id:Pensioenwet-20250301-112b-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112b&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toezichthouder kan tot die beoogde datum een verbod tot fusie of splitsing opleggen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112b, lid 2"@nl .

    id:Pensioenwet-20250301-113 a def:LawArticle ;
        def:hasDescription " Het pensioenfonds zendt: binnen twee weken na totstandkoming van die wijziging aan de toezichthouder."@nl ;
        def:hasSection id:Pensioenwet-20250301-113-1,
            id:Pensioenwet-20250301-113-2,
            id:Pensioenwet-20250301-113-3,
            id:Pensioenwet-20250301-113-4,
            id:Pensioenwet-20250301-113-5 ;
        def:hasTitle "Toezending wijziging officiële stukken"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 113"@nl .

    id:Pensioenwet-20250301-113-1 a def:LawArticleSection ;
        def:hasText "a. een authentiek afschrift van de akte houdende wijziging van de statuten;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 113, Deel 1"@nl .

    id:Pensioenwet-20250301-113-2 a def:LawArticleSection ;
        def:hasText "b. een door het bestuur gewaarmerkt exemplaar van wijziging van de reglementen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 113, Deel 2"@nl .

    id:Pensioenwet-20250301-113-3 a def:LawArticleSection ;
        def:hasText "c. een door het bestuur gewaarmerkt afschrift van de wijzigingen in de uitvoeringsovereenkomst;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 113, Deel 3"@nl .

    id:Pensioenwet-20250301-113-4 a def:LawArticleSection ;
        def:hasText "d. een door het bestuur gewaarmerkt afschrift van de wijzigingen in de actuariële en bedrijfstechnische nota, bedoeld in artikel 145; en"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 113, Deel 4"@nl .

    id:Pensioenwet-20250301-113-5 a def:LawArticleSection ;
        def:hasText "e. een door het bestuur gewaarmerkt afschrift van de wijzigingen in de eventuele overeenkomst tot verzekering, overdracht of onderbrenging;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 113, Deel 5"@nl .

    id:Pensioenwet-20250301-114 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-114-1,
            id:Pensioenwet-20250301-114-2 ;
        def:hasTitle "Omzetting pensioenfonds"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 114"@nl .

    id:Pensioenwet-20250301-114-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=114&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0003045&artikel=18"^^xsd:string ;
        def:hasText "Voor omzetting van een pensioenfonds in een andere rechtsvorm als bedoeld in artikel 18 van Boek 2 van het Burgerlijk Wetboek is een verklaring van geen bezwaar van de toezichthouder vereist. De toezichthouder verleent de verklaring indien hij van oordeel is dat de belangen van deelnemers, gewezen deelnemers, andere aanspraakgerechtigden, de pensioengerechtigden en de werkgever voldoende gewaarborgd zijn."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 114, lid 1"@nl .

    id:Pensioenwet-20250301-114-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=114&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen nadere regels worden gesteld ten aanzien van het eerste lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 114, lid 2"@nl .

    id:Pensioenwet-20250301-115 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-115-1,

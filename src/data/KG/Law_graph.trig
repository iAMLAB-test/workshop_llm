    id:Pensioenwet-20250301-179 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-179-1,
            id:Pensioenwet-20250301-179-2,
            id:Pensioenwet-20250301-179-3 ;
        def:hasTitle "Hoogte bestuurlijke boete"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 179"@nl .

    id:Pensioenwet-20250301-179-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=179&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bedrag van de bestuurlijke boete wordt bepaald bij algemene maatregel van bestuur, met dien verstande dat de bestuurlijke boete voor een afzonderlijke overtreding ten hoogste € 4 000 000 bedraagt. Indien tijdens het plegen van de overtreding nog geen vijf jaren zijn verlopen sedert het opleggen van een bestuurlijke boete aan de overtreder ter zake van eenzelfde overtreding, wordt het bedrag van de bestuurlijke boete, bedoeld in de eerste volzin, voor een afzonderlijke overtreding verdubbeld."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 179, lid 1"@nl .

    id:Pensioenwet-20250301-179-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=179&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De algemene maatregel van bestuur, bedoeld in het eerste lid, bepaalt bij elke daarin omschreven overtreding het bedrag van de deswege op te leggen bestuurlijke boete. De overtredingen worden gerangschikt in categorieën naar zwaarte van de overtreding met de daarbij behorende basisbedragen, minimumbedragen en maximumbedragen. Daarbij wordt de volgende indeling gebruikt:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 179, lid 2"@nl .

    id:Pensioenwet-20250301-179-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=7&paragraaf=7.4&sub-paragraaf=7.4.1&artikel=179&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In afwijking van het eerste en tweede lid kan de toezichthouder de hoogte van de bestuurlijke boete vaststellen op ten hoogste twee keer het bedrag van het voordeel dat de overtreder door de overtreding heeft verkregen indien diens voordeel groter is dan € 2 000 000."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 179, lid 3"@nl .

    id:Pensioenwet-20250301-17a a def:LawArticle ;
        def:hasDescription " Het doorberekenen van kosten in het kader van een premieovereenkomst vindt evenredig in de tijd plaats."@nl ;
        def:hasTitle "Evenredig doorberekenen van kosten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 17a"@nl .

    id:Pensioenwet-20250301-18 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-18-1,
            id:Pensioenwet-20250301-18-2,
            id:Pensioenwet-20250301-18-3 ;
        def:hasTitle "Behoud aanspraken bij verlaging pensioengevend salaris"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 18"@nl .

    id:Pensioenwet-20250301-18-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=18&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In geval van verlaging van de pensioengrondslag van een werknemer worden de op grond van de pensioenovereenkomst tot het tijdstip van verlaging opgebouwde pensioenaanspraken niet gewijzigd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 18, lid 1"@nl .

    id:Pensioenwet-20250301-18-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=18&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In geval van verlaging van de pensioengrondslag blijven de opgebouwde pensioenaanspraken behouden en worden de pensioenaanspraken vastgesteld overeenkomstig artikel 55."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 18, lid 2"@nl .

    id:Pensioenwet-20250301-18-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=18&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Elk beding in strijd met het eerste of tweede lid is nietig."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 18, lid 3"@nl .


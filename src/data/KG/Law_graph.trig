        def:hasDescription " Vervallen"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 74"@nl .

    id:AlgemeneOuderdomswet-20250101-75 a def:LawArticle ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-75-1,
            id:AlgemeneOuderdomswet-20250101-75-2 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 75"@nl .

    id:AlgemeneOuderdomswet-20250101-75-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=X&artikel=75&lid=1&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Deze wet kan worden aangehaald onder de titel \"Algemene Ouderdomswet\". "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 75, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-75-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=X&artikel=75&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText " De artikelen van deze wet treden in werking met ingang van een bij koninklijk besluit te bepalen tijdstip, dat voor de verschillende artikelen of onderdelen daarvan verschillend kan worden gesteld. "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 75, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-7a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=7a&lid=1&o=a&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasCases "a. vóór 1 januari 2013: 65, respectievelijk 15 jaar;"@nl,
            "b. in 2013: 65 jaar en één maand, respectievelijk 15 jaar en één maand;"@nl,
            "c. in 2014: 65 jaar en twee maanden, respectievelijk 15 jaar en twee maanden;"@nl,
            "d. in 2015: 65 jaar en drie maanden, respectievelijk 15 jaar en drie maanden;"@nl,
            "e. in 2016: 65 jaar en zes maanden, respectievelijk 15 jaar en zes maanden;"@nl,
            "f. in 2017: 65 jaar en negen maanden, respectievelijk 15 jaar en negen maanden;"@nl,
            "g. in 2018: 66 jaar, respectievelijk 16 jaar;"@nl,
            "h. in 2019: 66 jaar en vier maanden, respectievelijk 16 jaar en vier maanden;"@nl,
            "i. in 2020: 66 jaar en vier maanden, respectievelijk 16 jaar en vier maanden;"@nl,
            "j. in 2021: 66 jaar en vier maanden, respectievelijk 16 jaar en vier maanden;"@nl,
            "k. in 2022: 66 jaar en zeven maanden, respectievelijk 16 jaar en zeven maanden;"@nl,
            "l. in 2023: 66 jaar en tien maanden, respectievelijk 16 jaar en tien maanden;"@nl,
            "m. in 2024: 67 jaar, respectievelijk 17 jaar;"@nl,
            "n. in 2025: 67 jaar, respectievelijk 17 jaar;"@nl,
            "o. in 2026 en de kalenderjaren daarna: de pensioengerechtigde leeftijd en de aanvangsleeftijd, die jaarlijks op basis van de verhoging van de pensioengerechtigde leeftijd en de aanvangsleeftijd op grond van het tweede lid worden vastgesteld."@nl ;
        def:hasText "De pensioengerechtigde leeftijd en de aanvangsleeftijd zijn:"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 7a, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-7a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=7a&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De verhoging van de pensioengerechtigde leeftijd en de aanvangsleeftijd in 2026 en de kalenderjaren daarna wordt jaarlijks, voor de eerste maal uiterlijk op 1 januari 2021 voor het jaar 2026, vastgesteld volgens de formule:"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 7a, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-7a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=7a&lid=3&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De verhoging, bedoeld in het tweede lid, treedt telkens in werking vijf jaar na de uiterste datum van vaststelling, bedoeld in het tweede lid, voor de eerste maal met ingang van 1 januari 2026."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 7a, lid 3"@nl .

    id:AlgemeneOuderdomswet-20250101-7a-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=7a&lid=4&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De ramingen van de macro gemiddelde resterende levensverwachting, bedoeld in het tweede lid, worden uitgevoerd en bekendgemaakt door het CBS."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 7a, lid 4"@nl .

    id:AlgemeneOuderdomswet-20250101-8 a def:LawArticle ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-8-1,
            id:AlgemeneOuderdomswet-20250101-8-2,
            id:AlgemeneOuderdomswet-20250101-8-3,
            id:AlgemeneOuderdomswet-20250101-8-4 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8"@nl .

    id:AlgemeneOuderdomswet-20250101-8-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8&lid=1&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De pensioengerechtigde die voor 1 januari 2015 is gehuwd en voor die datum recht heeft op ouderdomspensioen en van wie de echtgenoot jonger is dan de pensioengerechtigde leeftijd, heeft overeenkomstig de bepalingen van deze wet recht op een toeslag, tenzij, met inachtneming van artikel 11, het inkomen uit arbeid of overig inkomen van die echtgenoot meer bedraagt dan de volledige bruto-toeslag. "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-8-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8&lid=2&o=a&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasCases "a. wijziging van het inkomen, bedoeld in het eerste lid;"@nl,
            "b. wijziging van de leefvorm, waardoor niet langer sprake is van een ongehuwde als bedoeld in artikel 1, tweede en derde lid;"@nl,
            "c. het gaan wonen in Nederland of in een land waarmee een verdrag in werking is getreden dan wel een besluit van een volkenrechtelijke organisatie van kracht is geworden, op grond waarvan recht op toeslag kan bestaan;"@nl,
            "d. een invrijheidstelling als bedoeld in artikel 8b, derde lid; of"@nl,
            "e. het zich niet langer onttrekken aan de tenuitvoerlegging van een vrijheidsstraf of vrijheidsbenemende maatregel als bedoeld in artikel 8c, derde lid;"@nl,
            "f. het niet langer bestaan van het gegronde vermoeden dat de pensioengerechtigde dan wel diens partner zich buiten Nederland bevindt met het doel zich aan te sluiten bij een organisatie als bedoeld in artikel 1, eerste lid, onderdeel l."@nl ;
        def:hasText "In afwijking van het eerste lid ontstaat op of na 1 januari 2015, geen recht meer op toeslag als gevolg van:"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-8-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8&lid=3&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Indien het recht op toeslag is geëindigd uitsluitend als gevolg van een incidentele stijging van het inkomen, bedoeld in het eerste lid, kan, in afwijking van het tweede lid, het recht op toeslag herleven."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8, lid 3"@nl .

    id:AlgemeneOuderdomswet-20250101-8-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8&lid=4&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText " Waar in deze wet en in de tot haar uitvoering genomen besluiten wordt gesproken van ouderdomspensioen wordt daaronder mede verstaan de in het eerste lid bedoelde toeslag, voor zover niet anders is bepaald. "@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8, lid 4"@nl .

    id:AlgemeneOuderdomswet-20250101-8a a def:LawArticle ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-8a-1,
            id:AlgemeneOuderdomswet-20250101-8a-2,
            id:AlgemeneOuderdomswet-20250101-8a-3 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8a"@nl .

    id:AlgemeneOuderdomswet-20250101-8a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8a&lid=1&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Geen recht op toeslag heeft de pensioengerechtigde, bedoeld in artikel 8, eerste lid, die niet in Nederland woont."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8a, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-8a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8a&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Het eerste lid is niet van toepassing indien de pensioengerechtigde woont in een land waarin op grond van een verdrag of een besluit van een volkenrechtelijke organisatie recht op toeslag kan bestaan."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8a, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-8a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8a&lid=3&o=a&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasCases "a. de pensioengerechtigde, bedoeld in artikel 8, eerste lid, die werkzaamheden verricht in het algemeen belang en niet in Nederland woont;"@nl,
            "b. de pensioengerechtigde, bedoeld in artikel 8, eerste lid, die in Aruba, Curaçao, Sint Maarten of in de openbare lichamen Bonaire, Sint Eustatius en Saba woont; of"@nl,
            "c. de gezinsleden van de in de onderdelen a of b bedoelde pensioengerechtigde."@nl ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kan worden bepaald dat recht heeft op toeslag:"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8a, lid 3"@nl .

    id:AlgemeneOuderdomswet-20250101-8b a def:LawArticle ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-8b-1,
            id:AlgemeneOuderdomswet-20250101-8b-2,
            id:AlgemeneOuderdomswet-20250101-8b-3,
            id:AlgemeneOuderdomswet-20250101-8b-4,
            id:AlgemeneOuderdomswet-20250101-8b-5,
            id:AlgemeneOuderdomswet-20250101-8b-6 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8b"@nl .

    id:AlgemeneOuderdomswet-20250101-8b-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8b&lid=1&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Geen recht op ouderdomspensioen ontstaat voor de pensioengerechtigde, aan wie rechtens zijn vrijheid is ontnomen indien de dag waarop het ouderdomspensioen zou ingaan dan wel de dag na afloop van de toepassing van artikel 8c, eerste lid, met betrekking tot dat recht op ouderdomspensioen, is gelegen in de periode dat hem rechtens zijn vrijheid is ontnomen."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8b, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-8b-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8b&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Het recht op ouderdomspensioen eindigt, indien de pensioengerechtigde rechtens zijn vrijheid is ontnomen gedurende ten minste een maand."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8b, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-8b-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8b&lid=3&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De persoon die op grond van het eerste of tweede lid geen recht op ouderdomspensioen heeft, heeft met ingang van de dag waarop hij in vrijheid is gesteld met inachtneming van de bepalingen van deze wet recht op ouderdomspensioen."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8b, lid 3"@nl .

    id:AlgemeneOuderdomswet-20250101-8b-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8b&lid=4&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Voor de toepassing van het tweede lid worden perioden van vrijheidsontneming samengeteld, indien zij elkaar met een onderbreking van minder dan vier weken opvolgen."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8b, lid 4"@nl .

    id:AlgemeneOuderdomswet-20250101-8b-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8b&lid=5&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Het eerste en tweede lid zijn niet van toepassing en het derde lid is van overeenkomstige toepassing op bij algemene maatregel van bestuur aan te wijzen categorieën personen waarbij tenuitvoerlegging van een vrijheidsstraf of vrijheidsbenemende maatregel buiten een justitiële inrichting plaatsvindt."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8b, lid 5"@nl .

    id:AlgemeneOuderdomswet-20250101-8b-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8b&lid=6&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Voor de pensioengerechtigde die op de dag voorafgaande aan de vrijheidsontneming geen recht heeft op ouderdomspensioen op grond van artikel 8c, tweede lid, eindigt het recht op ouderdomspensioen, in afwijking van het tweede lid, vanaf de dag dat de vrijheidsontneming ingaat."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8b, lid 6"@nl .

    id:AlgemeneOuderdomswet-20250101-8c a def:LawArticle ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-8c-1,
            id:AlgemeneOuderdomswet-20250101-8c-2,
            id:AlgemeneOuderdomswet-20250101-8c-3 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8c"@nl .

    id:AlgemeneOuderdomswet-20250101-8c-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8c&lid=1&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Voor de pensioengerechtigde ontstaat geen recht op ouderdomspensioen indien en voor zolang hij zich op de dag waarop het ouderdomspensioen zou ingaan en daarna onttrekt aan de tenuitvoerlegging van een vrijheidsstraf of vrijheidsbenemende maatregel."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8c, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-8c-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8c&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Het recht op ouderdomspensioen eindigt, indien de pensioengerechtigde zich, nadat het recht op ouderdomspensioen is ingegaan, onttrekt aan de tenuitvoerlegging van een vrijheidsstraf of vrijheidsbenemende maatregel."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8c, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-8c-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8c&lid=3&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De persoon die op grond van het eerste of tweede lid geen recht op ouderdomspensioen heeft, heeft met ingang van de dag dat hij zich niet langer onttrekt aan de tenuitvoerlegging van een vrijheidsstraf of vrijheidsbenemende maatregel met inachtneming van de bepalingen van deze wet recht op ouderdomspensioen."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8c, lid 3"@nl .

    id:AlgemeneOuderdomswet-20250101-8d a def:LawArticle ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-8d-1,
            id:AlgemeneOuderdomswet-20250101-8d-2,
            id:AlgemeneOuderdomswet-20250101-8d-3,
            id:AlgemeneOuderdomswet-20250101-8d-4,
            id:AlgemeneOuderdomswet-20250101-8d-5 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8d"@nl .

    id:AlgemeneOuderdomswet-20250101-8d-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8d&lid=1&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Voor de pensioengerechtigde ontstaat geen recht op ouderdomspensioen indien en voor zolang hij een uitreiziger is."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8d, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-8d-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8d&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Het recht op ouderdomspensioen eindigt, indien de pensioengerechtigde, nadat het recht op ouderdomspensioen is ingegaan, een uitreiziger is."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8d, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-8d-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8d&lid=3&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De persoon die op grond van het eerste of tweede lid geen recht op ouderdomspensioen heeft, heeft met ingang van de dag dat niet langer het gegronde vermoeden bestaat dat hij zich buiten Nederland bevindt met het doel zich aan te sluiten bij een organisatie als bedoeld in artikel 1, eerste lid, onderdeel l, met inachtneming van de bepalingen van deze wet recht op ouderdomspensioen."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8d, lid 3"@nl .

    id:AlgemeneOuderdomswet-20250101-8d-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8d&lid=4&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Geen recht op toeslag heeft de pensioengerechtigde, bedoeld in artikel 8, eerste lid, van wie de partner een uitreiziger is."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8d, lid 4"@nl .

    id:AlgemeneOuderdomswet-20250101-8d-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=8d&lid=5&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Voor het derde lid is artikel 14, eerste lid, van overeenkomstige toepassing."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 8d, lid 5"@nl .

    id:AlgemeneOuderdomswet-20250101-9-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9&lid=1&o=a&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasCases "a. de ongehuwde pensioengerechtigde;"@nl,
            "b. de gehuwde pensioengerechtigde."@nl ;
        def:hasText "Deze wet kent een bruto-ouderdomspensioen voor:"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-9-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De in het eerste lid bedoelde ouderdomspensioenen worden afgeleid van het netto-minimumloon per maand."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-9-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9&lid=3&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0017745"^^xsd:string ;
        def:hasText "Onder het netto-minimumloon wordt verstaan het bruto-minimumloon, na aftrek van premies op grond van de Wet financiering sociale verzekeringen en loonbelasting."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9, lid 3"@nl .

    id:AlgemeneOuderdomswet-20250101-9-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9&lid=4&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002471&artikel=22"^^xsd:string,
            "jci1.3:c:BWBR0017745&artikel=2"^^xsd:string ;
        def:hasText "De loonbelasting en premie voor de volksverzekeringen, bedoeld in artikel 2 van de Wet financiering sociale verzekeringen, worden berekend voor een werknemer, jonger dan de pensioengerechtigde leeftijd, rekening houdend met uitsluitend tweemaal de algemene heffingskorting, bedoeld in artikel 22 van de Wet op de loonbelasting 1964, over het bruto-minimumloon."@nl ;
        def:referencesTo id:WetOpLoonbelasting1964-20250101-22 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9, lid 4"@nl .

    id:AlgemeneOuderdomswet-20250101-9-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9&lid=5&o=a&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasCases "a. het netto-ouderdomspensioen per maand van een pensioengerechtigde als bedoeld in het eerste lid, onderdeel a, gelijk is aan 70% van het netto-minimumloon per maand;"@nl,
            "b. het netto-ouderdomspensioen per maand van een pensioengerechtigde als bedoeld in het eerste lid, onderdeel b, gelijk is aan 50% van het netto-minimumloon per maand."@nl ;
        def:hasExtref "jci1.3:c:BWBR0018450&artikel=41"^^xsd:string ;
        def:hasText "De bruto-ouderdomspensioenen worden zodanig vastgesteld, dat na aftrek van de in te houden loonbelasting en premie voor de volksverzekeringen, rekening houdend met de toepasselijke heffingskortingen voor een persoon van de pensioengerechtigde leeftijd en ouder, en van de inkomensafhankelijke bijdrage, bedoeld in artikel 41 van de Zorgverzekeringswet:"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9, lid 5"@nl .

    id:AlgemeneOuderdomswet-20250101-9-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9&lid=6&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De volledige bruto-toeslag, bedoeld in artikel 8, is gelijk aan het bruto-ouderdomspensioen voor de pensioengerechtigde, bedoeld in het eerste lid, onderdeel b."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9, lid 6"@nl .

    id:AlgemeneOuderdomswet-20250101-9-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9&lid=7&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Een wijziging van het bruto-ouderdomspensioen in verband met een wijziging van het netto-minimumloon wordt samen met de dag waarop deze wijziging ingaat, door of namens Onze Minister medegedeeld in de Staatscourant."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9, lid 7"@nl .

    id:AlgemeneOuderdomswet-20250101-9-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9&lid=8&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "Een wijziging van het bruto-ouderdomspensioen in verband met een wijziging van het netto-minimumloon vindt plaats zonder dat dit bij beschikking is vastgesteld."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9, lid 8"@nl .

    id:AlgemeneOuderdomswet-20250101-9-9 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9&lid=9&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "De Sociale verzekeringsbank betaalt het gewijzigde ouderdomspensioen, bedoeld in het achtste lid, bij de eerstvolgende betaling van het ouderdomspensioen nadat de wijziging, bedoeld in het achtste lid, heeft plaatsgevonden."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9, lid 9"@nl .

    id:AlgemeneOuderdomswet-20250101-9a a def:LawArticle ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-9a-1,
            id:AlgemeneOuderdomswet-20250101-9a-2,
            id:AlgemeneOuderdomswet-20250101-9a-3,
            id:AlgemeneOuderdomswet-20250101-9a-4 ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9a"@nl .

    id:AlgemeneOuderdomswet-20250101-9a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9a&lid=1&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText "In afwijking van artikel 9 is voor de pensioengerechtigde, die niet in Nederland woont, het bruto-ouderdomspensioen gelijk aan het bruto-ouderdomspensioen voor de pensioengerechtigde, bedoeld in artikel 9, eerste lid, onderdeel b, onverminderd artikel 13, eerste lid."@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9a, lid 1"@nl .

    id:AlgemeneOuderdomswet-20250101-9a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9a&lid=2&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasText """
      Artikel 8a, tweede lid, is van overeenkomstige toepassing."""@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9a, lid 2"@nl .

    id:AlgemeneOuderdomswet-20250101-9a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9a&lid=3&o=a&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasCases "a. de pensioengerechtigde, bedoeld in artikel 9, eerste lid, onderdeel a, die werkzaamheden verricht in het algemeen belang en niet in Nederland woont;"@nl,
            "b. de pensioengerechtigde, bedoeld in artikel 9, eerste lid, onderdeel a, die in Aruba, Curaçao, Sint Maarten of in de openbare lichamen Bonaire, Sint Eustatius en Saba woont; of"@nl,
            "c. de gezinsleden van de in de onderdelen a of b bedoelde pensioengerechtigde."@nl ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kan worden bepaald dat recht heeft op ouderdomspensioen alsof hij in Nederland woont:"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9a, lid 3"@nl .

    id:AlgemeneOuderdomswet-20250101-9a-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0002221&hoofdstuk=III&paragraaf=1&artikel=9a&lid=4&o=a&z=2025-01-01&g=2025-01-01"^^xsd:anyURI ;
        def:hasCases "a. een buitengewoon pensioen, garantietoeslag, uitkering, periodieke uitkering, garantie-uitkering, vergoeding of tegemoetkoming als bedoeld in de Wet buitengewoon pensioen 1940–1945, de Wet buitengewoon pensioen zeelieden-oorlogsslachtoffers, de Wet buitengewoon pensioen Indisch verzet, de Wet uitkeringen vervolgingsslachtoffers 1940–1945, de Wet uitkeringen burger-oorlogsslachtoffers 1940–1945 of de Algemene Oorlogsongevallenregeling Indonesië met inbegrip van het besluit van de Luitenant-Gouverneur-Generaal van Nederlands-Indië van 5 november 1946 (Indisch Staatsblad, 1946, 118); of"@nl,
            "b. een pensioen, toeslag, uitkering, vergoeding of tegemoetkoming, vergelijkbaar met een pensioen, toeslag, uitkering, vergoeding of tegemoetkoming als bedoeld in onderdeel a, op grond van een buitenlandse wettelijke regeling die vergelijkbaar is met de wettelijke regelingen, genoemd in onderdeel a."@nl ;
        def:hasExtref "jci1.3:c:BWBR0002032"^^xsd:string,
            "jci1.3:c:BWBR0002035"^^xsd:string,
            "jci1.3:c:BWBR0002844"^^xsd:string,
            "jci1.3:c:BWBR0003664"^^xsd:string,
            "jci1.3:c:BWBR0003968"^^xsd:string ;
        def:hasText "Recht op ouderdomspensioen alsof hij in Nederland woont, heeft de pensioengerechtigde die niet in Nederland woont en recht heeft op:"@nl ;
        rdfs1:label "Algemene ouderdomswet, 2025-01-01, Artikel 9a, lid 4"@nl .

    id:Pensioenwet-20250301-1-1 a def:LawArticleSection ;
        def:hasText "– aanspraakgerechtigde: persoon die begunstigde is voor een nog niet ingegaan pensioen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 1"@nl .

    id:Pensioenwet-20250301-1-10 a def:LawArticleSection ;
        def:hasText "– bevoegde autoriteiten: nationale autoriteiten van andere lidstaten dan Nederland als bedoeld in artikel 6, onderdeel 8, van richtlijn 2016/2341/EU;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 10"@nl .

    id:Pensioenwet-20250301-1-11 a def:LawArticleSection ;
        def:hasText "– bijdrage: iedere geldsom die wordt voldaan aan een pensioenuitvoerder in het kader van de uitvoering van pensioenovereenkomsten en uitvoeringsovereenkomsten;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 11"@nl .

    id:Pensioenwet-20250301-1-12 a def:LawArticleSection ;
        def:hasText "– bijdragende onderneming: een onderneming of ander lichaam, ongeacht of deze een of meer natuurlijke personen of rechtspersonen die optreden als werkgever of zelfstandige, dan wel een combinatie daarvan, omvat of hieruit bestaat, en die een pensioenregeling aanbiedt of aan een pensioenfonds, premiepensioeninstelling of pensioeninstelling uit een andere lidstaat bijdragen betaalt;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 12"@nl .

    id:Pensioenwet-20250301-1-13 a def:LawArticleSection ;
        def:hasText "– bijzonder partnerpensioen: de aanspraak op partnerpensioen die op grond van artikel 57, eerste, tweede of derde lid, verkregen wordt door de gewezen partner;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 13"@nl .

    id:Pensioenwet-20250301-1-14 a def:LawArticleSection ;
        def:hasText "– buitenlandse instelling: een instelling met zetel buiten Nederland, niet zijnde een pensioeninstelling uit een andere lidstaat, een verzekeraar met een zetel buiten Nederland, een lidstaat van de Europese Unie of een instelling als bedoeld in artikel 70, tweede lid;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 14"@nl .

    id:Pensioenwet-20250301-1-15 a def:LawArticleSection ;
        def:hasText "– collectief toedelingsmechanisme: wijze waarop financiële mee- en tegenvallers collectief worden verwerkt in variabele uitkeringen van flexibele premieovereenkomsten;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 15"@nl .

    id:Pensioenwet-20250301-1-16 a def:LawArticleSection ;
        def:hasText "– collectiviteitkring: een of meerdere pensioenregelingen of beroepspensioenregelingen als bedoeld in artikel 1 van de Wet verplichte beroepspensioenregeling waarvoor een algemeen pensioenfonds een afgescheiden vermogen aanhoudt;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 16"@nl .

    id:Pensioenwet-20250301-1-17 a def:LawArticleSection ;
        def:hasText "– deelnemer: de werknemer of gewezen werknemer die op grond van een pensioenovereenkomst pensioenaanspraken verwerft jegens een pensioenuitvoerder;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 17"@nl .

    id:Pensioenwet-20250301-1-18 a def:LawArticleSection ;
        def:hasText "– dekkingsgraad: de verhouding tussen het vermogen inzake de bij een pensioenfonds ondergebrachte pensioenregeling of pensioenregelingen en de technische voorzieningen van een pensioenfonds;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 18"@nl .

    id:Pensioenwet-20250301-1-19 a def:LawArticleSection ;
        def:hasText "– dienstbetrekking: de rechtsbetrekking tussen werkgever en werknemer;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 19"@nl .

    id:Pensioenwet-20250301-1-2 a def:LawArticleSection ;
        def:hasText "– accountant: een accountant als bedoeld in artikel 393, eerste lid, van Boek 2 van het Burgerlijk Wetboek;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 2"@nl .

    id:Pensioenwet-20250301-1-20 a def:LawArticleSection ;
        def:hasCases "a. persoonlijk houder van aandelen welke ten minste een tiende deel van het geplaatste kapitaal van de vennootschap van de werkgever vertegenwoordigen en waaraan stemrecht in de algemene vergadering is verbonden;"@nl,
            "b. indirect persoonlijk houder van aandelen welke ten minste een tiende deel van het geplaatste kapitaal van de vennootschap van de werkgever vertegenwoordigen en waaraan stemrecht in de algemene vergadering is verbonden; of"@nl,
            "c. houder van certificaten van aandelen, uitgegeven door tussenkomst van een administratiekantoor waarvan hij voor ten minste een tiende deel in het bestuur vertegenwoordigd is, welke ten minste een tiende deel van het geplaatste kapitaal van de vennootschap vertegenwoordigen en aan welke aandelen stemrecht in de algemene vergadering is verbonden;"@nl ;
        def:hasText "– directeur-grootaandeelhouder:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 20"@nl .

    id:Pensioenwet-20250301-1-21 a def:LawArticleSection ;
        def:hasText "– elektronisch: door middel van een elektronische informatiedrager die de ontvanger in staat stelt de verstrekte informatie duurzaam te bewaren;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 21"@nl .

    id:Pensioenwet-20250301-1-22 a def:LawArticleSection ;
        def:hasText "– flexibele premieovereenkomst: premieovereenkomst waarbij de premie individueel wordt belegd en waarbij het kapitaal voortvloeiend uit de premie vanaf de pensioendatum wordt aangewend voor financiering van een variabele uitkering of voor de aankoop van een vastgestelde uitkering;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 22"@nl .

    id:Pensioenwet-20250301-1-23 a def:LawArticleSection ;
        def:hasText "– gedetacheerde werknemer: een werknemer die in een andere lidstaat wordt gedetacheerd om daar te werken en die krachtens titel II van verordening (EEG) nr. 1408/71 van de Raad van de Europese Gemeenschappen van 14 juni 1971 betreffende de toepassing van de socialezekerheidsregelingen op loontrekkenden en hun gezinnen, die zich binnen de gemeenschap verplaatsen (PbEG L 149), onderworpen blijft aan de wetgeving van de lidstaat van oorsprong;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 23"@nl .

    id:Pensioenwet-20250301-1-24 a def:LawArticleSection ;
        def:hasText "– gepensioneerde: pensioengerechtigde voor wie het ouderdomspensioen is ingegaan;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 24"@nl .

    id:Pensioenwet-20250301-1-25 a def:LawArticleSection ;
        def:hasText "– gewezen deelnemer: de werknemer of gewezen werknemer door wie op grond van een pensioenovereenkomst geen pensioen meer wordt verworven en die bij beëindiging van de deelneming een pensioenaanspraak heeft behouden jegens een pensioenuitvoerder;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 25"@nl .

    id:Pensioenwet-20250301-1-26 a def:LawArticleSection ;
        def:hasText "– groep: een groep als bedoeld in artikel 24b van Boek 2 van het Burgerlijk Wetboek;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 26"@nl .

    id:Pensioenwet-20250301-1-27 a def:LawArticleSection ;
        def:hasText "– lidstaat: een lidstaat van de Europese Unie alsmede een staat, niet zijnde een lidstaat van de Europese Unie, die partij is bij de Overeenkomst betreffende Europese Economische Ruimte;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 27"@nl .

    id:Pensioenwet-20250301-1-28 a def:LawArticleSection ;
        def:hasText "– nabestaandenpensioen: partnerpensioen of wezenpensioen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 28"@nl .

    id:Pensioenwet-20250301-1-29 a def:LawArticleSection ;
        def:hasText "– nettopensioen: ouderdomspensioen of nabestaandenpensioen in de vorm van een nettopensioen als bedoeld in afdeling 5.3B van de Wet inkomstenbelasting 2001;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 29"@nl .

    id:Pensioenwet-20250301-1-3 a def:LawArticleSection ;
        def:hasText "– afkoop: iedere handeling waardoor pensioenaanspraken en pensioenrechten hun pensioenbestemming verliezen, behoudens in geval van toepassing van de artikelen 55, vijfde lid, 134 of 220b, tweede lid, of van artikel 3A:85 van de Wet op het financieel toezicht;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 3"@nl .

    id:Pensioenwet-20250301-1-30 a def:LawArticleSection ;
        def:hasText "– ondernemingspensioenfonds: een pensioenfonds verbonden aan een onderneming of een groep;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 30"@nl .

    id:Pensioenwet-20250301-1-31 a def:LawArticleSection ;
        def:hasText "– ondernemingsraad: de ondernemingsraad, bedoeld in de Wet op de ondernemingsraden;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 31"@nl .

    id:Pensioenwet-20250301-1-32 a def:LawArticleSection ;
        def:hasText "– ontvangende pensioenuitvoerder: de pensioenuitvoerder aan wie in het kader van waardeoverdracht waarde wordt overgedragen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 32"@nl .

    id:Pensioenwet-20250301-1-33 a def:LawArticleSection ;
        def:hasText "– Onze Minister: Onze Minister voor Armoedebeleid, Participatie en Pensioenen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 33"@nl .

    id:Pensioenwet-20250301-1-34 a def:LawArticleSection ;
        def:hasText "– ouderdomspensioen: een geldelijke uitkering, die vastgesteld of variabel is, voor de werknemer of de gewezen werknemer bij wijze van inkomensvoorziening bij ouderdom;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 34"@nl .

    id:Pensioenwet-20250301-1-35 a def:LawArticleSection ;
        def:hasText "– overdrachtswaarde: de ten behoeve van de waardeoverdracht vastgestelde waarde van de over te dragen pensioenaanspraken of pensioenrechten;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 35"@nl .

    id:Pensioenwet-20250301-1-36 a def:LawArticleSection ;
        def:hasText "– overdragende pensioenuitvoerder: de pensioenuitvoerder die in het kader van waardeoverdracht waarde overdraagt aan een andere pensioenuitvoerder;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 36"@nl .

    id:Pensioenwet-20250301-1-37 a def:LawArticleSection ;
        def:hasText "– overrendement: het saldo van het totaal behaalde rendement op de beleggingen, de ontwikkeling van de levensverwachting en het sterfteresultaat en het toebedeelde beschermingsrendement op basis van de toedelingsregels;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 37"@nl .

    id:Pensioenwet-20250301-1-38 a def:LawArticleSection ;
        def:hasCases "a. echtgenoot;"@nl,
            "b. geregistreerd partner; of"@nl,
            "c. partner in de zin van de pensioenovereenkomst zijnde de meerderjarige persoon die met de werknemer of de gewezen werknemer een gezamenlijke huishouding voert, tenzij het betreft een bloedverwant in de eerste graad, een bloedverwant in de tweede graad in de rechte lijn, een meerderjarig stiefkind of meerderjarig voormalig pleegkind;"@nl ;
        def:hasText "– partner: "@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 38"@nl .

    id:Pensioenwet-20250301-1-39 a def:LawArticleSection ;
        def:hasText "– partnerpensioen: een geldelijke uitkering, die vastgesteld of variabel is, voor de partner of gewezen partner wegens het overlijden van de werknemer of gewezen werknemer;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 39"@nl .

    id:Pensioenwet-20250301-1-4 a def:LawArticleSection ;
        def:hasText "– algemeen pensioenfonds: een pensioenfonds dat een of meerdere pensioenregelingen of beroepspensioenregelingen als bedoeld in artikel 1 van de Wet verplichte beroepspensioenregeling uitvoert en daarvoor een afgescheiden vermogen aanhoudt per collectiviteitkring;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 4"@nl .

    id:Pensioenwet-20250301-1-40 a def:LawArticleSection ;
        def:hasText "– partnerrelatie: huwelijk, geregistreerd partnerschap of partnerrelatie in de zin van de pensioenovereenkomst;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 40"@nl .

    id:Pensioenwet-20250301-1-41 a def:LawArticleSection ;
        def:hasText "– pensioen: ouderdomspensioen, arbeidsongeschiktheidspensioen of nabestaandenpensioen, zoals tussen werkgever en werknemer overeengekomen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 41"@nl .

    id:Pensioenwet-20250301-1-42 a def:LawArticleSection ;
        def:hasText "– pensioenaanspraak: het recht op een nog niet ingegaan pensioen, uitgezonderd overeengekomen voorwaardelijke toeslagverlening;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 42"@nl .

    id:Pensioenwet-20250301-1-43 a def:LawArticleSection ;
        def:hasText "– pensioenbewaarder: de pensioenbewaarder, bedoeld in artikel 1:1 van de Wet op het financieel toezicht;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 43"@nl .

    id:Pensioenwet-20250301-1-44 a def:LawArticleSection ;
        def:hasText "– pensioeneenheid: eenheid van deelname in een beleggingsportefeuille, waarbij de waarde fluctueert met de koersontwikkeling van deze portefeuille en eventueel met de ontwikkeling van de levensverwachting;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 44"@nl .

    id:Pensioenwet-20250301-1-45 a def:LawArticleSection ;
        def:hasText "– pensioenfonds: een stichting die niet een premiepensioeninstelling is, waarin ten behoeve van ten minste twee deelnemers, gewezen deelnemers of hun nabestaanden gelden worden of werden bijeengebracht en worden beheerd ter uitvoering van ten minste een basispensioenregeling;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 45"@nl .

    id:Pensioenwet-20250301-1-46 a def:LawArticleSection ;
        def:hasText "– pensioengerechtigde: persoon voor wie op grond van een pensioenovereenkomst het pensioen is ingegaan;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 46"@nl .

    id:Pensioenwet-20250301-1-47 a def:LawArticleSection ;
        def:hasCases "a. individueel of collectief tussen een of meerdere werkgevers en een of meerdere werknemers of hun respectievelijke vertegenwoordigers; of"@nl,
            "b. individueel of collectief met zelfstandigen,"@nl ;
        def:hasText "– pensioeninstelling uit een andere lidstaat: een op basis van kapitaaldekking gefinancierde instelling, ongeacht de rechtsvorm, die zetel heeft in een andere lidstaat dan Nederland en die onafhankelijk van enige bijdragende onderneming of bedrijfstak is opgericht met als doel het verstrekken van arbeidsgerelateerde pensioenuitkeringen op basis van een als volgt gesloten overeenkomst:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 47"@nl .

    id:Pensioenwet-20250301-1-48 a def:LawArticleSection ;
        def:hasText "– pensioenovereenkomst: hetgeen tussen een werkgever en een werknemer is overeengekomen betreffende pensioen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 48"@nl .

    id:Pensioenwet-20250301-1-49 a def:LawArticleSection ;
        def:hasText "– pensioenrecht: het recht op een ingegaan pensioen, uitgezonderd overeengekomen voorwaardelijke toeslagverlening;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 49"@nl .

    id:Pensioenwet-20250301-1-5 a def:LawArticleSection ;
        def:hasText "– arbeidsongeschiktheidspensioen: een geldelijke uitkering, die vastgesteld of variabel is, wegens arbeidsongeschiktheid van de werknemer of gewezen werknemer, waarop recht bestaat na afloop van de periode bedoeld in artikel 29, eerste lid, van de Ziektewet of, indien de werknemer of gewezen werknemer Ziektewetuitkering ontvangt, na afloop van de periode bedoeld in artikel 29, vijfde en tiende lid, van de Ziektewet;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 5"@nl .

    id:Pensioenwet-20250301-1-50 a def:LawArticleSection ;
        def:hasCases "a. een pensioenregeling op grond van een pensioenovereenkomst; of"@nl,
            "b. indien de bijdragende onderneming zetel heeft in een andere lidstaat dan Nederland, een overeenkomst, een trustakte of voorschriften waarin is bepaald welke pensioenuitkeringen worden toegezegd en onder welke voorwaarden;"@nl ;
        def:hasText "– pensioenregeling:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 50"@nl .

    id:Pensioenwet-20250301-1-51 a def:LawArticleSection ;
        def:hasText "– pensioenreglement: de door de pensioenuitvoerder opgestelde regeling met betrekking tot de verhouding tussen pensioenuitvoerder en deelnemer, gewezen deelnemer, andere aanspraakgerechtigde of pensioengerechtigde;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 51"@nl .

    id:Pensioenwet-20250301-1-52 a def:LawArticleSection ;
        def:hasText "– pensioenuitvoerder: een ondernemingspensioenfonds, een bedrijfstakpensioenfonds, een algemeen pensioenfonds, of een premiepensioeninstelling of verzekeraar die zetel heeft in Nederland;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 52"@nl .

    id:Pensioenwet-20250301-1-53 a def:LawArticleSection ;
        def:hasText "– pensioenverplichtingen: verplichtingen van de pensioenuitvoerder uit hoofde van pensioenaanspraken en pensioenrechten;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 53"@nl .

    id:Pensioenwet-20250301-1-54 a def:LawArticleSection ;
        def:hasText "– premie: de in geld uitgedrukte periodiek vastgestelde structurele prestatie die verschuldigd is aan de pensioenuitvoerder en die bestemd is voor pensioen en de daaraan verbonden kosten;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 54"@nl .

    id:Pensioenwet-20250301-1-55 a def:LawArticleSection ;
        def:hasText "– premie-uitkeringsovereenkomst: premieovereenkomst uitgevoerd door een verzekeraar of door een premiepensioeninstelling waarbij de premie individueel wordt belegd, waarbij de premie of het kapitaal voortvloeiend uit de premie in de laatste 15 jaar voor de pensioengerechtigde leeftijd, bedoeld in artikel 7a, eerste lid, van de Algemene Ouderdomswet, kan worden aangewend voor aankoop van een vastgestelde uitkering vanaf de pensioendatum en waarbij het resterend kapitaal vanaf de pensioendatum wordt aangewend voor financiering van een variabele uitkering of voor de aankoop van een vastgestelde uitkering;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 55"@nl .

    id:Pensioenwet-20250301-1-56 a def:LawArticleSection ;
        def:hasText "– premieovereenkomst: een pensioenovereenkomst inzake een vastgestelde premie die uiterlijk op de pensioendatum wordt omgezet in een vastgestelde of variabele pensioenuitkering;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 56"@nl .

    id:Pensioenwet-20250301-1-57 a def:LawArticleSection ;
        def:hasText "– premiepensioeninstelling: een premiepensioeninstelling die op grond van de Wet op het financieel toezicht in Nederland het bedrijf van premiepensioeninstelling mag uitoefenen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 57"@nl .

    id:Pensioenwet-20250301-1-58 a def:LawArticleSection ;
        def:hasText "– projectierendement: het ingerekende toekomstig rendement voor de vaststelling van variabele uitkeringen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 58"@nl .

    id:Pensioenwet-20250301-1-59 a def:LawArticleSection ;
        def:hasText "– richtlijn 2016/2341/EU: richtlijn 2016/2341/EU van het Europees Parlement en de Raad van 14 december 2016 betreffende de werkzaamheden van en het toezicht op instellingen voor bedrijfspensioenvoorziening (IBPV’s) (PbEU L 2016, 354);"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 59"@nl .

    id:Pensioenwet-20250301-1-6 a def:LawArticleSection ;
        def:hasText "– basispensioenregeling: de collectieve pensioenregeling of het deel van de pensioenregeling waaraan de werknemer op basis van de pensioenovereenkomst gehouden is om deel te nemen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 6"@nl .

    id:Pensioenwet-20250301-1-60 a def:LawArticleSection ;
        def:hasText "– risicodelingsreserve: een collectieve vermogensreserve waarmee in een flexibele premieovereenkomst financiële mee- of tegenvallers kunnen worden gedeeld;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 60"@nl .

    id:Pensioenwet-20250301-1-61 a def:LawArticleSection ;
        def:hasText "– risicohouding: de vastgestelde mate waarin een groep deelnemers, gewezen deelnemers of pensioengerechtigden bereid is beleggingsrisico’s te lopen met oog op hun doelstellingen en de mate waarin deze groep beleggingsrisico’s kan dragen gegeven de kenmerken van deze groep;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 61"@nl .

    id:Pensioenwet-20250301-1-62 a def:LawArticleSection ;
        def:hasText "–  risicovrije rente: de door De Nederlandsche Bank N.V. gepubliceerde actuele rentetermijnstructuur;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 62"@nl .

    id:Pensioenwet-20250301-1-63 a def:LawArticleSection ;
        def:hasText "– scheiding: echtscheiding, ontbinding van het huwelijk na scheiding van tafel en bed, beëindiging van een geregistreerd partnerschap anders dan door dood, vermissing of omzetting van een geregistreerd partnerschap in een huwelijk of beëindiging van een partnerrelatie in de zin van de pensioenovereenkomst;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 63"@nl .

    id:Pensioenwet-20250301-1-64 a def:LawArticleSection ;
        def:hasText "– schriftelijk: in schrifttekens op papier;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 64"@nl .

    id:Pensioenwet-20250301-1-65 a def:LawArticleSection ;
        def:hasText "– solidaire premieovereenkomst: premieovereenkomst waarbij de premie collectief wordt belegd, de resultaten in ieder geval naar leeftijdscohorten worden toebedeeld en waarbij het voor pensioenuitkering bestemd vermogen gedurende de uitkeringsfase wordt aangewend voor financiering van een variabele uitkering;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 65"@nl .

    id:Pensioenwet-20250301-1-66 a def:LawArticleSection ;
        def:hasText "– solidariteitsreserve: een collectieve vermogensreserve waarmee in een solidaire premieovereenkomst financiële mee- of tegenvallers kunnen worden gedeeld;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 66"@nl .

    id:Pensioenwet-20250301-1-67 a def:LawArticleSection ;
        def:hasText "– toedelingskring: groep personen waarop een collectief toedelingsmechanisme wordt toegepast;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 67"@nl .

    id:Pensioenwet-20250301-1-68 a def:LawArticleSection ;
        def:hasText "– toedelingsregels: de wijze waarop bij de solidaire premieovereenkomst financiële mee- of tegenvallers als gevolg van het collectief gevoerde beleggingsbeleid, de ontwikkeling van de levensverwachting en het sterfteresultaat via beschermingsrendementen en overrendementen in de voor pensioenuitkering bestemde vermogens of in de solidariteitsreserve worden verwerkt;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 68"@nl .

    id:Pensioenwet-20250301-1-69 a def:LawArticleSection ;
        def:hasCases "a. een pensioenrecht in de vorm van een vastgestelde uitkering; of"@nl,
            "b. een pensioenaanspraak in de vorm van een aanspraak op een vastgestelde uitkering, mits die verhoging geen verband houdt met een verhoging van de pensioengrondslag, de toename van het in aanmerking te nemen aantal jaren of een wijziging van de pensioenovereenkomst;"@nl ;
        def:hasText "– toeslag: een verhoging van:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 69"@nl .

    id:Pensioenwet-20250301-1-7 a def:LawArticleSection ;
        def:hasText "– bedrijfstakpensioenfonds: een pensioenfonds ten behoeve van een of meer bedrijfstakken of delen van een bedrijfstak;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 7"@nl .

    id:Pensioenwet-20250301-1-70 a def:LawArticleSection ;
        def:hasText "– toezichthouder: de Stichting Autoriteit Financiële Markten of De Nederlandsche Bank N.V., ieder voor zover belast met de uitoefening van het toezicht bij of krachtens artikel 151;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 70"@nl .

    id:Pensioenwet-20250301-1-71 a def:LawArticleSection ;
        def:hasText "– uitvoeringsovereenkomst: de overeenkomst tussen een werkgever en een pensioenuitvoerder over de uitvoering van een of meer pensioenovereenkomsten;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 71"@nl .

    id:Pensioenwet-20250301-1-72 a def:LawArticleSection ;
        def:hasCases "a. de door een bedrijftakpensioenfonds opgestelde regeling met betrekking tot de verhouding tussen pensioenuitvoerder en werkgever;"@nl,
            "b. de door een pensioenuitvoerder opgestelde regeling inzake de uitvoering van de pensioenovereenkomsten met zijn werknemers;"@nl,
            "c. de door een algemeen pensioenfonds opgestelde regeling inzake de uitvoering van een beëindigde pensioenregeling of beroepspensioenregeling als bedoeld in artikel 23a of artikel 4a, derde lid, van de Wet verplichte beroepspensioenregeling;"@nl ;
        def:hasText "– uitvoeringsreglement:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 72"@nl .

    id:Pensioenwet-20250301-1-73 a def:LawArticleSection ;
        def:hasText "– verplichtgesteld bedrijfstakpensioenfonds: een bedrijfstakpensioenfonds waarin de deelneming verplicht is gesteld als bedoeld in artikel 2, eerste lid, van de Wet verplichte deelneming in een bedrijfstakpensioenfonds 2000, artikel 21, eerste lid, van de Wet privatisering ABP en artikel 113a, eerste lid, van de Wet op het notarisambt;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 73"@nl .

    id:Pensioenwet-20250301-1-74 a def:LawArticleSection ;
        def:hasText "– verzekeraar: een verzekeraar die op grond van de Wet op het financieel toezicht in Nederland het bedrijf van levensverzekeraar of schadeverzekeraar mag uitoefenen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 74"@nl .

    id:Pensioenwet-20250301-1-75 a def:LawArticleSection ;
        def:hasText "– voorwaarden in verband met de partnerrelatie: huwelijkse voorwaarden, voorwaarden van een geregistreerd partnerschap of voorwaarden in verband met een partnerrelatie in de zin van de pensioenovereenkomst;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 75"@nl .

    id:Pensioenwet-20250301-1-76 a def:LawArticleSection ;
        def:hasText "– vrijwillige pensioenregeling: het deel van de pensioenregeling waaraan de werknemer op basis van de pensioenovereenkomst de mogelijkheid heeft om deel te nemen;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 76"@nl .

    id:Pensioenwet-20250301-1-77 a def:LawArticleSection ;
        def:hasCases "1°. andere pensioenaanspraken of pensioenrechten bij dezelfde of een andere pensioenuitvoerder; of"@nl,
            "2°. dezelfde pensioenaanspraken of pensioenrechten bij een andere pensioenuitvoerder;"@nl ;
        def:hasText "– waardeoverdracht: iedere handeling waarbij de waarde van opgebouwde pensioenaanspraken of pensioenrechten wordt aangewend ten behoeve van:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 77"@nl .

    id:Pensioenwet-20250301-1-78 a def:LawArticleSection ;
        def:hasText "– werkgever: degene die een werknemer krachtens arbeidsovereenkomst naar burgerlijk recht of publiekrechtelijke aanstelling arbeid laat verrichten;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 78"@nl .

    id:Pensioenwet-20250301-1-79 a def:LawArticleSection ;
        def:hasText "– werkgeverspremie: het deel van de premie dat voor rekening komt van de werkgever;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 79"@nl .

    id:Pensioenwet-20250301-1-8 a def:LawArticleSection ;
        def:hasCases "a. het overlijden van de deelnemer; of"@nl,
            "b. het ingaan van het ouderdomspensioen;"@nl ;
        def:hasText "– beëindiging van de deelneming: het beëindigen van de pensioenverwerving op basis van een pensioenovereenkomst anders dan door:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 8"@nl .

    id:Pensioenwet-20250301-1-80 a def:LawArticleSection ;
        def:hasText "– werknemer: degene die krachtens arbeidsovereenkomst naar burgerlijk recht of publiekrechtelijke aanstelling arbeid verricht voor een werkgever, met uitzondering van de directeur-grootaandeelhouder en de werknemer die onder de werkingsfeer van een verplichtgestelde beroepspensioenregeling als bedoeld in de Wet verplichte beroepspensioenregeling valt;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 80"@nl .

    id:Pensioenwet-20250301-1-81 a def:LawArticleSection ;
        def:hasText "– werknemerspremie: het deel van de premie dat voor rekening komt van de werknemer;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 81"@nl .

    id:Pensioenwet-20250301-1-82 a def:LawArticleSection ;
        def:hasText "– wezenpensioen: een geldelijke uitkering, die vastgesteld of variabel is, voor een kind tot wie de overleden werknemer of gewezen werknemer als ouder in familierechtelijke betrekking stond of voor diens stief- of pleegkind, wegens het overlijden van de werknemer of gewezen werknemer;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 82"@nl .

    id:Pensioenwet-20250301-1-83 a def:LawArticleSection ;
        def:hasText "– zetel: de plaats waar een rechtspersoon volgens zijn statuten of reglementen is gevestigd of, indien het een pensioenfonds of pensioeninstelling uit een andere lidstaat betreft, de plaats waar deze volgens zijn statuten of reglementen is gevestigd en zijn hoofdbestuur heeft of, indien het een pensioeninstelling uit een andere lidstaat betreft die geen rechtspersoon is of een natuurlijke persoon betreft, de plaats waar die pensioeninstelling of persoon zijn hoofdbestuur heeft."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 83"@nl .

    id:Pensioenwet-20250301-1-9 a def:LawArticleSection ;
        def:hasText "– beschermingsrendement: de vermogensbijschrijving die ervoor zorgt dat de, uit het opgebouwde pensioenvermogen te financieren, toekomstige pensioenuitkeringen en lopende pensioenuitkeringen nominaal stabiel blijven, dat wordt gefinancierd vanuit het totaal behaalde rendement en looptijdafhankelijk wordt toebedeeld aan de vermogens op basis van marktwaardering, waarbij toedelingsregels worden gehanteerd;"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 1, Deel 9"@nl .

    id:Pensioenwet-20250301-10-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De pensioenovereenkomst houdt een premieovereenkomst in."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10, lid 1"@nl .

    id:Pensioenwet-20250301-10-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een solidaire premieovereenkomst;"@nl,
            "b. een flexibele premieovereenkomst; of"@nl,
            "c. een premie-uitkeringsovereenkomst."@nl ;
        def:hasText "Een premieovereenkomst voor pensioen op opbouwbasis houdt in:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10, lid 2"@nl .

    id:Pensioenwet-20250301-10-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien een pensioenovereenkomst voorziet in een nabestaandenpensioen op risicobasis of een arbeidsongeschiktheidspensioen op risicobasis, wordt in de pensioenovereenkomst opgenomen of aan de nabestaanden of de arbeidsongeschikte een vastgestelde of een variabele uitkering wordt verstrekt. Indien een variabele uitkering wordt verstrekt wordt in de pensioenovereenkomst opgenomen bij welk karakter als bedoeld in het tweede lid voor het nabestaandenpensioen of het arbeidsongeschiktheidspensioen wordt aangesloten. De regels die bij of krachtens deze wet gelden voor een variabele pensioenuitkering bij een pensioenovereenkomst met dat karakter zijn van overeenkomstige toepassing bij de uitvoering van het nabestaandenpensioen of arbeidsongeschiktheidspensioen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10, lid 3"@nl .

    id:Pensioenwet-20250301-100 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-100-1,
            id:Pensioenwet-20250301-100-2,
            id:Pensioenwet-20250301-100-3,
            id:Pensioenwet-20250301-100-4,
            id:Pensioenwet-20250301-100-5,
            id:Pensioenwet-20250301-100-6,
            id:Pensioenwet-20250301-100-7 ;
        def:hasTitle "Samenstelling paritair bestuur"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 100"@nl .

    id:Pensioenwet-20250301-100-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=100&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In het paritaire bestuur van een bedrijfstakpensioenfonds zijn de belanghebbenden op een zo evenwichtig mogelijke wijze vertegenwoordigd met dien verstande dat de vertegenwoordigers van werknemersverenigingen in de betrokken bedrijfstak of bedrijfstakken en de vertegenwoordigers van pensioengerechtigden tezamen ten minste evenveel zetels bezetten als de vertegenwoordigers van werkgeversverenigingen in de betrokken bedrijfstak of bedrijfstakken."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 100, lid 1"@nl .

    id:Pensioenwet-20250301-100-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=100&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In het paritaire bestuur van een ondernemingspensioenfonds zijn de belanghebbenden op een zo evenwichtig mogelijke wijze vertegenwoordigd met dien verstande dat de werknemersvertegenwoordigers en vertegenwoordigers van pensioengerechtigden tezamen ten minste evenveel zetels bezetten als de werkgeversvertegenwoordigers."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 100, lid 2"@nl .

    id:Pensioenwet-20250301-100-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=100&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0018831&artikel=1"^^xsd:string,
            "jci1.3:c:BWBR0018831&artikel=109"^^xsd:string ;
        def:hasText "In het paritaire bestuur van een algemeen pensioenfonds is het eerste lid van overeenkomstige toepassing voor zover het algemeen pensioenfonds een pensioenregeling uitvoert voor een of meer bedrijfstakken of delen van een bedrijfstak; is het tweede lid van overeenkomstige toepassing voor zover het algemeen pensioenfonds een pensioenregeling uitvoert voor een onderneming of groep en is artikel 109 van de Wet verplichte beroepspensioenregeling van overeenkomstige toepassing voor zover het algemeen pensioenfonds een beroepspensioenregeling uitvoert als bedoeld in artikel 1 van die wet."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 100, lid 3"@nl .

    id:Pensioenwet-20250301-100-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=100&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien de statuten van een pensioenfonds voorzien in stemgerechtigde vertegenwoordigers in het paritaire bestuur van anderen dan werknemersverenigingen dan wel werknemers, werkgeversverenigingen dan wel de werkgever of pensioengerechtigden, worden die vertegenwoordigers voor de toepassing van het eerste, tweede en derde lid gelijkgesteld met vertegenwoordigers van pensioengerechtigden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 100, lid 4"@nl .

    id:Pensioenwet-20250301-100-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=100&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het tweede tot en met vierde lid is niet van toepassing voor zover een onderneming of groep waaraan het ondernemingspensioenfonds was verbonden heeft opgehouden te bestaan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 100, lid 5"@nl .

    id:Pensioenwet-20250301-100-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=100&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Aan het paritaire bestuur van een pensioenfonds kunnen maximaal twee bestuurders worden toegevoegd die niet directe vertegenwoordigers zijn van de belanghebbenden bij het pensioenfonds. Het vierde lid is niet van toepassing op deze bestuurders."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 100, lid 6"@nl .

    id:Pensioenwet-20250301-100-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=100&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur stelt een profielschets op voor leden van het bestuur. Het bestuur kan een kandidaat bestuurder afwijzen indien deze niet aan de profielschets voldoet."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 100, lid 7"@nl .

    id:Pensioenwet-20250301-101 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-101-1,
            id:Pensioenwet-20250301-101-2 ;
        def:hasTitle "Samenstelling onafhankelijk bestuur"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101"@nl .

    id:Pensioenwet-20250301-101-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het onafhankelijke bestuur van een pensioenfonds bestaat uit ten minste twee bestuurders. De bestuurders zijn niet directe vertegenwoordigers van de belanghebbenden bij het pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101, lid 1"@nl .

    id:Pensioenwet-20250301-101-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur stelt een profielschets op voor leden van het bestuur. Het bestuur kan een kandidaat bestuurder afwijzen indien deze niet aan de profielschets voldoet."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101, lid 2"@nl .

    id:Pensioenwet-20250301-101a a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-101a-1,
            id:Pensioenwet-20250301-101a-2,
            id:Pensioenwet-20250301-101a-3,
            id:Pensioenwet-20250301-101a-4,
            id:Pensioenwet-20250301-101a-5,
            id:Pensioenwet-20250301-101a-6,
            id:Pensioenwet-20250301-101a-7,
            id:Pensioenwet-20250301-101a-8,
            id:Pensioenwet-20250301-101a-9 ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a"@nl .

    id:Pensioenwet-20250301-101a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het gemengd bestuur van een pensioenfonds bestaat uit uitvoerende bestuurders en niet uitvoerende bestuurders."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a, lid 1"@nl .

    id:Pensioenwet-20250301-101a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101a&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Voor de samenstelling, zetelverdeling en benoeming van de uitvoerende bestuurders bij een paritair gemengd bestuur zijn de artikelen 100 en 102 van overeenkomstige toepassing. Voor de samenstelling van de uitvoerende bestuurders bij een onafhankelijk gemengd bestuur is artikel 101 van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a, lid 2"@nl .

    id:Pensioenwet-20250301-101a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101a&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het gemengd bestuur heeft ten minste drie natuurlijke personen als niet uitvoerende bestuurders. De niet uitvoerende bestuurders zijn niet directe vertegenwoordigers van de belanghebbenden bij het pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a, lid 3"@nl .

    id:Pensioenwet-20250301-101a-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101a&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij de statuten van een pensioenfonds met een gemengd bestuur wordt bepaald dat de bestuurstaken worden verdeeld over niet uitvoerende bestuurders en uitvoerende bestuurders. Artikel 104, tweede lid, is van overeenkomstige toepassing op de taken van de niet uitvoerende bestuurders. De taak om toezicht te houden op de taakuitoefening door bestuurders kan niet door een taakverdeling worden ontnomen aan niet uitvoerende bestuurders. Het voorzitterschap van het bestuur, het doen van voordrachten voor benoeming van een bestuurder en het vaststellen van de bezoldiging van uitvoerende bestuurders kan niet aan een uitvoerende bestuurder worden toebedeeld."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a, lid 4"@nl .

    id:Pensioenwet-20250301-101a-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101a&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De uitvoerende bestuurders nemen niet deel aan de besluitvorming over het vaststellen van de bezoldiging van uitvoerende bestuurders."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a, lid 5"@nl .

    id:Pensioenwet-20250301-101a-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101a&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens de statuten kan worden bepaald dat een of meer bestuurders rechtsgeldig kunnen besluiten over zaken die tot zijn respectievelijk hun taak behoren. Bepaling krachtens de statuten geschiedt schriftelijk."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a, lid 6"@nl .

    id:Pensioenwet-20250301-101a-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101a&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In afwijking van het tweede en derde lid kan een omgekeerd gemengd bestuur worden gevormd waarbij voor de samenstelling van de uitvoerende bestuurders artikel 101 van overeenkomstige toepassing is en waarbij voor de samenstelling, zetelverdeling en benoeming van de niet uitvoerende bestuurders de artikelen 100 en 102 van overeenkomstige toepassing zijn."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a, lid 7"@nl .

    id:Pensioenwet-20250301-101a-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101a&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een omgekeerd gemengd bestuur heeft een onafhankelijke voorzitter die geen vertegenwoordiger is van de belanghebbenden bij het pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a, lid 8"@nl .

    id:Pensioenwet-20250301-101a-9 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=101a&lid=9&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen nadere regels worden gesteld over de bevoegdheden van de onafhankelijk voorzitter, bedoeld in het achtste lid, de samenstelling van een omgekeerd gemengd bestuur en over de taakverdeling in en organisatie van dit bestuur."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 101a, lid 9"@nl .

    id:Pensioenwet-20250301-102 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-102-1,
            id:Pensioenwet-20250301-102-2,
            id:Pensioenwet-20250301-102-3,
            id:Pensioenwet-20250301-102-4,
            id:Pensioenwet-20250301-102-5 ;
        def:hasTitle "Zetelverdeling en benoeming pensioengerechtigden en werknemers in paritair bestuur"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102"@nl .

    id:Pensioenwet-20250301-102-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=102&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De verdeling van de zetels van vertegenwoordigers van werknemersverenigingen of werknemersvertegenwoordigers en vertegenwoordigers van pensioengerechtigden in het paritaire bestuur van een pensioenfonds vindt plaats op basis van de onderlinge getalsverhoudingen, met dien verstande dat de vertegenwoordigers van pensioengerechtigden ten hoogste de helft van het aantal zetels in het paritaire bestuur van een pensioenfonds bezetten dat vertegenwoordigers van werknemersverenigingen of werknemersvertegenwoordigers en vertegenwoordigers van pensioengerechtigden gezamenlijk bezetten. Van deze verdeling kan worden afgeweken indien de betrokken partijen daarmee akkoord zijn."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102, lid 1"@nl .

    id:Pensioenwet-20250301-102-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=102&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "In het paritaire bestuur van een ondernemingspensioenfonds kunnen, in afwijking van het eerste lid, vertegenwoordigers van pensioengerechtigden meer zetels bezetten dan werknemersvertegenwoordigers, indien het aantal deelnemers minder bedraagt dan 10% van de som van het aantal deelnemers en pensioengerechtigden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102, lid 2"@nl .

    id:Pensioenwet-20250301-102-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=102&lid=3&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. na verkiezing van de vertegenwoordigers door de deelnemers;"@nl,
            "b. op voordracht van de vertegenwoordigers van de deelnemers in het verantwoordingsorgaan, bedoeld in artikel 115;"@nl,
            "c. op voordracht van de ondernemingsraad; of"@nl,
            "d. op een andere wijze, mits de ondernemingsraad heeft ingestemd met deze benoemingswijze."@nl ;
        def:hasText "De benoeming van de werknemersvertegenwoordigers in het paritaire bestuur van een ondernemingspensioenfonds vindt plaats:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102, lid 3"@nl .

    id:Pensioenwet-20250301-102-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=102&lid=4&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. na verkiezing van de vertegenwoordigers door de pensioengerechtigden; of"@nl,
            "b. op voordracht van de vertegenwoordigers van de pensioengerechtigden in het verantwoordingsorgaan, bedoeld in artikel 115, mits deze vertegenwoordigers na verkiezing zijn benoemd."@nl ;
        def:hasText "De benoeming van de vertegenwoordigers van pensioengerechtigden in het paritaire bestuur van een pensioenfonds vindt plaats:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102, lid 4"@nl .

    id:Pensioenwet-20250301-102-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=102&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het tweede en derde lid is van overeenkomstige toepassing voor zover een algemeen pensioenfonds met een paritair bestuur een pensioenregeling uitvoert voor een onderneming of groep."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102, lid 5"@nl .

    id:Pensioenwet-20250301-102a a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-102a-1,
            id:Pensioenwet-20250301-102a-2,
            id:Pensioenwet-20250301-102a-3,
            id:Pensioenwet-20250301-102a-4 ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102a"@nl .

    id:Pensioenwet-20250301-102a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=102a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur van een pensioenfonds draagt na overleg met de overige organen van het pensioenfonds zorg voor de vastlegging van de doelstellingen en beleidsuitgangspunten, waaronder de risicohouding, van het pensioenfonds. De organen van het pensioenfonds gebruiken deze doelstellingen en uitgangspunten bij de toetsing van de opdrachtaanvaarding van de door vertegenwoordigers van werkgevers of werkgeversverenigingen en werknemers of werknemersverenigingen overeengekomen pensioenregelingen, en bij de besluitvorming, de verantwoording, de advisering, en het toezicht binnen het pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102a, lid 1"@nl .

    id:Pensioenwet-20250301-102a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=102a&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur van een pensioenfonds streeft er naar van de vertegenwoordigers, bedoeld in het eerste lid, zo veel mogelijk duidelijkheid te verkrijgen over de doelstellingen die ten grondslag liggen aan de pensioenregelingen die de vertegenwoordigers als opdracht in uitvoering aan het pensioenfonds geven."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102a, lid 2"@nl .

    id:Pensioenwet-20250301-102a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=102a&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur van een pensioenfonds draagt zorg voor de formele opdrachtaanvaarding van de door de vertegenwoordigers, bedoeld in het eerste lid, aan het pensioenfonds opgedragen pensioenregelingen. Het bestuur toetst bij de opdrachtaanvaarding voor het pensioenfonds als geheel en voor de relevante beleidsgebieden aan de doelstellingen en uitgangspunten, bedoeld in het eerste lid."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102a, lid 3"@nl .

    id:Pensioenwet-20250301-102a-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=102a&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen regels worden gesteld met betrekking tot dit artikel over onder meer de vastlegging van de doelstellingen en beleidsuitgangspunten en het proces van opdrachtaanvaarding."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 102a, lid 4"@nl .

    id:Pensioenwet-20250301-103 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-103-1,
            id:Pensioenwet-20250301-103-2,
            id:Pensioenwet-20250301-103-3,
            id:Pensioenwet-20250301-103-4,
            id:Pensioenwet-20250301-103-5 ;
        def:hasTitle "Intern toezicht pensioenfondsen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 103"@nl .

    id:Pensioenwet-20250301-103-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=103&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het intern toezicht bij een bedrijfstakpensioenfonds met een paritair of onafhankelijk bestuur wordt uitgeoefend door een raad van toezicht, tenzij het bedrijfstakpensioenfonds volledig is verzekerd bij een verzekeraar. In dat geval kan het intern toezicht ook worden uitgeoefend door jaarlijkse visitatie door een visitatiecommissie."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 103, lid 1"@nl .

    id:Pensioenwet-20250301-103-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=103&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een raad van toezicht of jaarlijkse visitatie door een visitatiecommissie indien het ondernemingspensioenfonds volledig is verzekerd bij een verzekeraar of een beheerd vermogen heeft van niet meer dan € 1 miljard; of"@nl,
            "b. een raad van toezicht indien het ondernemingspensioenfonds een beheerd vermogen heeft van meer dan € 1 miljard, tenzij het ondernemingspensioenfonds volledig verzekerd is bij een verzekeraar in welk geval het intern toezicht ook kan worden uitgeoefend door jaarlijkse visitatie als bedoeld in onderdeel a."@nl ;
        def:hasText "Het intern toezicht bij een ondernemingspensioenfonds met een paritair of onafhankelijk bestuur wordt uitgeoefend door:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 103, lid 2"@nl .

    id:Pensioenwet-20250301-103-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=103&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het intern toezicht bij een algemeen pensioenfonds met een paritair of onafhankelijk bestuur wordt uitgeoefend door een raad van toezicht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 103, lid 3"@nl .

    id:Pensioenwet-20250301-103-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=103&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het intern toezicht bij een pensioenfonds met een gemengd bestuur wordt uitgeoefend door de niet uitvoerende bestuurders."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 103, lid 4"@nl .

    id:Pensioenwet-20250301-103-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=103&lid=5&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. heeft een ondernemingspensioenfonds een beheerd vermogen van niet meer dan € 1 miljard indien het ondernemingspensioenfonds op twee opeenvolgende balansdata, zonder onderbreking nadien op twee opeenvolgende balansdata, een vermogen van niet meer dan € 1 miljard heeft; en"@nl,
            "b. wordt het intern toezicht bij een ondernemingspensioenfonds dat een raad van toezicht heeft ingesteld omdat het ondernemingspensioenfonds een beheerd vermogen van meer dan € 1 miljard heeft, ten minste vier jaar uitgeoefend door een raad van toezicht ook indien het beheerd vermogen in deze periode € 1 miljard of minder wordt."@nl ;
        def:hasText "Voor de toepassing van het tweede lid:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 103, lid 5"@nl .

    id:Pensioenwet-20250301-104 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-104-1,
            id:Pensioenwet-20250301-104-10,
            id:Pensioenwet-20250301-104-2,
            id:Pensioenwet-20250301-104-3,
            id:Pensioenwet-20250301-104-4,
            id:Pensioenwet-20250301-104-5,
            id:Pensioenwet-20250301-104-6,
            id:Pensioenwet-20250301-104-7,
            id:Pensioenwet-20250301-104-8,
            id:Pensioenwet-20250301-104-9 ;
        def:hasTitle "Samenstelling en taken raad van toezicht en visitatiecommissie"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104"@nl .

    id:Pensioenwet-20250301-104-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De raad van toezicht of de visitatiecommissie bestaat uit ten minste drie natuurlijke personen. De leden van de raad van toezicht of de visitatiecommissie zijn onafhankelijk en laten dit tot uiting komen in het toezicht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 1"@nl .

    id:Pensioenwet-20250301-104-10 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=10&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen nadere regels worden gesteld inzake dit artikel die onder meer betrekking hebben op de bevoegdheden van de raad van toezicht ten aanzien van benoeming en ontslag van bestuurders."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 10"@nl .

    id:Pensioenwet-20250301-104-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0003045&artikel=9"^^xsd:string ;
        def:hasText "De raad van toezicht heeft tot taak toezicht te houden op het beleid van het bestuur en op de algemene gang van zaken in het pensioenfonds. De raad van toezicht is ten minste belast met het toezien op adequate risicobeheersing en evenwichtige belangenafweging door het bestuur en legt verantwoording af over de uitvoering van de taken en de uitoefening van de bevoegdheden aan het verantwoordingsorgaan en de werkgever, het belanghebbendenorgaan en in het bestuursverslag. De raad van toezicht staat het bestuur met raad ter zijde. Artikel 9 van Boek 2 van het Burgerlijk Wetboek is van overeenkomstige toepassing op de leden van de raad van toezicht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 2"@nl .

    id:Pensioenwet-20250301-104-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=3&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het bestuursverslag en de jaarrekening;"@nl,
            "b. de profielschets voor bestuurders;"@nl,
            "c. het beleid inzake beloningen, met uitzondering van de beloning van de raad van toezicht;"@nl,
            "d. gehele of gedeeltelijke overdracht van de verplichtingen van het pensioenfonds of de overname van verplichtingen door het pensioenfonds;"@nl,
            "e. liquidatie, fusie of splitsing van het pensioenfonds;"@nl,
            "f. het beleid inzake het aangaan en beëindigen van uitvoeringsovereenkomsten en het opstellen en beëindigen van uitvoeringsreglementen; en"@nl,
            "g. het omzetten van het pensioenfonds in een andere rechtsvorm, bedoeld in artikel 18 van Boek 2 van het Burgerlijk Wetboek."@nl ;
        def:hasExtref "jci1.3:c:BWBR0003045&artikel=18"^^xsd:string ;
        def:hasText "Aan de goedkeuring van de raad van toezicht zijn onderworpen de besluiten van het bestuur tot vaststelling van:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 3"@nl .

    id:Pensioenwet-20250301-104-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De statuten voorzien in een regeling voor geschillen over goedkeuring van besluiten door de raad van toezicht. Het ontbreken van de goedkeuring van de raad van toezicht op een besluit als bedoeld in het derde lid tast de vertegenwoordigingsbevoegdheid van bestuur of bestuurders niet aan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 4"@nl .

    id:Pensioenwet-20250301-104-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De raad van toezicht meldt disfunctioneren van het bestuur aan het verantwoordingsorgaan, het belanghebbendenorgaan en aan de partij die bevoegd is tot benoeming van het bestuur. Indien de partij die bevoegd is tot benoeming van het bestuur naar aanleiding van de melding niet binnen een redelijke termijn, naar tevredenheid van de raad van toezicht, handelt, meldt de raad van toezicht het disfunctioneren van het bestuur aan De Nederlandsche Bank N.V."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 5"@nl .

    id:Pensioenwet-20250301-104-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De raad van toezicht stelt een profielschets op voor leden van de raad van toezicht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 6"@nl .

    id:Pensioenwet-20250301-104-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur van het pensioenfonds en de raad van toezicht komen ten minste tweemaal per kalenderjaar in vergadering bijeen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 7"@nl .

    id:Pensioenwet-20250301-104-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De visitatiecommissie heeft tot taak toezicht te houden op het beleid van het bestuur en op de algemene gang van zaken in het pensioenfonds. De visitatiecommissie is ten minste belast met het toezien op adequate risicobeheersing en evenwichtige belangenafweging door het bestuur en legt verantwoording af over de uitvoering van de taken aan het verantwoordingsorgaan en de werkgever, het belanghebbendenorgaan en in het bestuursverslag."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 8"@nl .

    id:Pensioenwet-20250301-104-9 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=104&lid=9&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het pensioenfonds verstrekt desgevraagd aan de raad van toezicht of de visitatiecommissie tijdig alle inlichtingen en gegevens, die deze voor de vervulling van zijn taak redelijkerwijs nodig heeft. De inlichtingen worden desgevraagd schriftelijk verstrekt."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 104, lid 9"@nl .

    id:Pensioenwet-20250301-105 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-105-1,
            id:Pensioenwet-20250301-105-2,
            id:Pensioenwet-20250301-105-3 ;
        def:hasTitle "Eisen ten aanzien van beleid"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 105"@nl .

    id:Pensioenwet-20250301-105-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=105&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Ten minste twee natuurlijke personen bepalen het dagelijks beleid van een pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 105, lid 1"@nl .

    id:Pensioenwet-20250301-105-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=105&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De personen die het beleid van een pensioenfonds bepalen of mede bepalen richten zich bij de vervulling van hun taak naar de belangen van de bij het pensioenfonds betrokken deelnemers, gewezen deelnemers, andere aanspraakgerechtigden, de pensioengerechtigden en de werkgever en zorgen ervoor dat dezen zich door hen op evenwichtige wijze vertegenwoordigd kunnen voelen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 105, lid 2"@nl .

    id:Pensioenwet-20250301-105-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=105&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Iedere bestuurder van een pensioenfonds is bevoegd een deskundige te raadplegen, of zich krachtens een bestuursbesluit, waarbij ten minste één vierde van de bestuurders zich daarvoor heeft uitgesproken, ter vergadering door een deskundige te laten bijstaan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 105, lid 3"@nl .

    id:Pensioenwet-20250301-106 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-106-1,
            id:Pensioenwet-20250301-106-2,
            id:Pensioenwet-20250301-106-3,
            id:Pensioenwet-20250301-106-4,
            id:Pensioenwet-20250301-106-5,
            id:Pensioenwet-20250301-106-6,
            id:Pensioenwet-20250301-106-7,
            id:Pensioenwet-20250301-106-8,
            id:Pensioenwet-20250301-106-9 ;
        def:hasTitle "Geschiktheid en betrouwbaarheid"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106"@nl .

    id:Pensioenwet-20250301-106-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=106&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het beleid van een pensioenfonds wordt bepaald of mede bepaald door personen die geschikt zijn in verband met de uitoefening van het bedrijf van het pensioenfonds."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106, lid 1"@nl .

    id:Pensioenwet-20250301-106-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=106&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het intern toezicht van een pensioenfonds door een visitatiecommissie wordt uitgeoefend door personen die geschikt zijn voor de uitoefening van dit toezicht."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106, lid 2"@nl .

    id:Pensioenwet-20250301-106-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=106&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De risicobeheerfunctie, interne auditfunctie en actuariële functie bij een pensioenfonds worden uitgeoefend door personen die geschikt zijn voor de uitoefening van deze functies."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106, lid 3"@nl .

    id:Pensioenwet-20250301-106-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=106&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het beleid van een pensioenfonds wordt bepaald of mede bepaald door personen wier betrouwbaarheid buiten twijfel staat. Het intern toezicht van een pensioenfonds door een visitatiecommissie en de risicobeheerfunctie, interne auditfunctie en actuariële functie bij een pensioenfonds worden uitgeoefend door personen wier betrouwbaarheid buiten twijfel staat."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106, lid 4"@nl .

    id:Pensioenwet-20250301-106-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=106&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het bestuur van het pensioenfonds meldt elke wijziging in de samenstelling van de personen die het beleid van het pensioenfonds bepalen of mede bepalen vooraf aan de toezichthouder."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106, lid 5"@nl .

    id:Pensioenwet-20250301-106-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=106&lid=6&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. de toezichthouder binnen zes weken na ontvangst van de melding van de wijziging aan het pensioenfonds bekend maakt dat hij niet met de voorgenomen wijziging instemt; of"@nl,
            "b. de toezichthouder om nadere gegevens of inlichtingen heeft verzocht en binnen zes weken na ontvangst van die gegevens of inlichtingen aan het pensioenfonds bekend maakt dat hij niet met de voorgenomen wijziging instemt."@nl ;
        def:hasText "Een wijziging als bedoeld in het vijfde lid wordt niet doorgevoerd indien:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106, lid 6"@nl .

    id:Pensioenwet-20250301-106-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=106&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien zich een wijziging voordoet van de antecedenten die van invloed is op de betrouwbaarheid van de personen die het beleid van het pensioenfonds bepalen of mede bepalen stelt het pensioenfonds de toezichthouder daarvan onverwijld schriftelijk in kennis."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106, lid 7"@nl .

    id:Pensioenwet-20250301-106-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=106&lid=8&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De betrouwbaarheid van een persoon staat buiten twijfel wanneer dat eenmaal door de toezichthouder voor de toepassing van deze wet is vastgesteld, zolang niet een wijziging in de relevante feiten of omstandigheden een redelijke aanleiding geeft tot een nieuwe beoordeling."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106, lid 8"@nl .

    id:Pensioenwet-20250301-106-9 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=106&lid=9&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel. Die regels hebben onder meer betrekking op de misdrijven die, indien begaan door een persoon als bedoeld in het vierde lid, met het oog op de belangen die de wet beoogt te beschermen, tot de vaststelling leiden dat de betrouwbaarheid van die persoon niet buiten twijfel staat."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106, lid 9"@nl .

    id:Pensioenwet-20250301-106a a def:LawArticle ;
        def:hasDescription " Bestuurders en leden van de raad van toezicht van een pensioenfonds hebben voldoende tijd beschikbaar om hun functie naar behoren uit te oefenen. Bij algemene maatregel van bestuur worden nadere regels gesteld. De artikelen 132a, 142a, 242a, 252a, 297a en 297b van Boek 2 van het Burgerlijk Wetboek zijn niet van toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 106a"@nl .

    id:Pensioenwet-20250301-107 a def:LawArticle ;
        def:hasDescription " In het bestuursverslag van een pensioenfonds wordt gerapporteerd over de samenstelling naar leeftijd en geslacht van het verantwoordingsorgaan, het belanghebbendenorgaan en het bestuur en over de inspanningen die zijn verricht om diversiteit in de organen van het pensioenfonds te bevorderen."@nl ;
        def:hasTitle "Diversiteit"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 107"@nl .

    id:Pensioenwet-20250301-108 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-108-1,
            id:Pensioenwet-20250301-108-2,
            id:Pensioenwet-20250301-108-3,
            id:Pensioenwet-20250301-108-4,
            id:Pensioenwet-20250301-108-5,
            id:Pensioenwet-20250301-108-6 ;
        def:hasTitle "Bescherming leden pensioenfondsbestuur, verantwoordingsorgaan en belanghebbendenorgaan"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 108"@nl .

    id:Pensioenwet-20250301-108-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=108&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De werkgever draagt er zorg voor dat de werknemers die staan of gestaan hebben op een kandidatenlijst voor het verantwoordingsorgaan of het belanghebbendenorgaan alsmede de leden en de gewezen leden van het pensioenfondsbestuur, het verantwoordingsorgaan of het belanghebbendenorgaan niet uit hoofde van hun kandidaatstelling voor of hun lidmaatschap van het pensioenfondsbestuur, het verantwoordingsorgaan of het belanghebbendenorgaan worden benadeeld in hun positie als werknemer."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 108, lid 1"@nl .

    id:Pensioenwet-20250301-108-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=108&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien het pensioenfonds in zijn hoedanigheid als werkgever aan het verantwoordingsorgaan of het belanghebbendenorgaan een secretaris heeft toegevoegd is het eerste lid op die secretaris van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 108, lid 2"@nl .

    id:Pensioenwet-20250301-108-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=108&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het verantwoordingsorgaan, het belanghebbendenorgaan en een werknemer als bedoeld in het eerste en tweede lid kunnen de kantonrechter verzoeken te bepalen dat de werkgever gevolg dient te geven aan hetgeen in deze leden is bepaald."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 108, lid 3"@nl .

    id:Pensioenwet-20250301-108-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=108&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De werkgever kan de arbeidsovereenkomst niet opzeggen met de werknemer die lid is van het bestuur van een pensioenfonds, een verantwoordingsorgaan of een belanghebbendenorgaan. Indien de werkgever aan het bestuur van een pensioenfonds, een verantwoordingsorgaan of een belanghebbendenorgaan een secretaris heeft toegevoegd, is de eerste zin op die secretaris van overeenkomstige toepassing."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 108, lid 4"@nl .

    id:Pensioenwet-20250301-108-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=108&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De werkgever kan de arbeidsovereenkomst niet opzeggen met een werknemer die geplaatst is op een kandidatenlijst voor het bestuur van een pensioenfonds, een verantwoordingsorgaan of een belanghebbendenorgaan of die korter dan twee jaar geleden lid is geweest van het bestuur van een pensioenfonds, een verantwoordingsorgaan of een belanghebbendenorgaan."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 108, lid 5"@nl .

    id:Pensioenwet-20250301-108-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=108&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het vierde en vijfde lid zijn niet van toepassing bij een opzegging gedurende de proeftijd, wegens een dringende reden, indien de werknemer schriftelijk met de opzegging instemt of indien de opzegging geschiedt wegens de beëindiging van de werkzaamheden van de onderneming of van het onderdeel van de onderneming, waarin de werknemer uitsluitend of in hoofdzaak werkzaam is."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 108, lid 6"@nl .

    id:Pensioenwet-20250301-109 a def:LawArticle ;
        def:hasDescription " Iedere bepaling die het lidmaatschap van het bestuur, de raad van toezicht, de visitatiecommissie, het verantwoordingsorgaan of het belanghebbendenorgaan onmogelijk maakt op grond van het bereikt hebben van een bepaalde leeftijd is nietig."@nl ;
        def:hasTitle "Verbod leeftijdgrens"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 109"@nl .

    id:Pensioenwet-20250301-10a a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-10a-1,
            id:Pensioenwet-20250301-10a-2,
            id:Pensioenwet-20250301-10a-3,
            id:Pensioenwet-20250301-10a-4,
            id:Pensioenwet-20250301-10a-5,
            id:Pensioenwet-20250301-10a-6,
            id:Pensioenwet-20250301-10a-7 ;
        def:hasTitle "Solidaire premieovereenkomst"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10a"@nl .

    id:Pensioenwet-20250301-10a-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10a&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij een solidaire premieovereenkomst berekent de pensioenuitvoerder de kans dat met de premie de beoogde pensioendoelstelling wordt behaald. Deze berekening wordt door pensioenfondsen bij de opdrachtaanvaarding, door verzekeraars en premiepensioeninstellingen bij het sluiten van de uitvoeringsovereenkomst en door alle pensioenuitvoerders periodiek, uiterlijk iedere vijf jaar, gedaan aan de hand van een uniforme scenario-analyse. De pensioenuitvoerder informeert de werkgever of de partijen die de pensioenregeling zijn overeengekomen hierover."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10a, lid 1"@nl .

    id:Pensioenwet-20250301-10a-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10a&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het voor de pensioenuitkering bestemd vermogen wordt gedurende de uitkeringsfase gebruikt voor financiering van een variabele uitkering."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10a, lid 2"@nl .

    id:Pensioenwet-20250301-10a-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10a&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De solidariteitsreserve is onderdeel van de solidaire premieovereenkomst."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10a, lid 3"@nl .

    id:Pensioenwet-20250301-10a-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10a&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De financiële mee- of tegenvallers als gevolg van het collectieve beleggingsbeleid worden in de voor pensioenuitkering bestemde vermogens en de solidariteitsreserve verwerkt door middel van vastgelegde toedelingsregels die in ieder geval aansluiten bij de risicohouding per leeftijdscohort, waarbij in de toedelingsregels bepaald kan worden dat het beschermingsrendement voor het renterisico wordt toebedeeld ofwel op basis van wijziging van de rentetermijnstructuur die de toezichthouder beschikbaar stelt ofwel rechtstreeks uit het rendement van de daarvoor bestemde beleggingen. De financiële mee- of tegenvallers als gevolg van de ontwikkeling van de levensverwachting en het sterfteresultaat worden verwerkt door middel van aparte toedelingsregels of de solidariteitsreserve."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10a, lid 4"@nl .

    id:Pensioenwet-20250301-10a-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10a&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De toedelingsregels voor de beschermingsrendementen en overrendementen voor het beleggingsrisico zijn zodanig dat er op voorhand geen herverdelingseffecten plaatsvinden. Van de eerste zin kan afgeweken worden voor zover dat nodig is om gelijke aanpassingen van de ingegane pensioenuitkeringen en van de opgebouwde aanspraak op nabestaandenpensioen van pensioengerechtigden te realiseren en alleen herverdelingseffecten optreden tussen de pensioengerechtigden onderling."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10a, lid 5"@nl .

    id:Pensioenwet-20250301-10a-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10a&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het voor de pensioenuitkering bestemd vermogen is niet negatief."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10a, lid 6"@nl .

    id:Pensioenwet-20250301-10a-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10a&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel over onder meer de scenario-analyse en de toedelingsregels."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10a, lid 7"@nl .

    id:Pensioenwet-20250301-10b a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-10b-1,
            id:Pensioenwet-20250301-10b-2,
            id:Pensioenwet-20250301-10b-3,
            id:Pensioenwet-20250301-10b-4,
            id:Pensioenwet-20250301-10b-5,
            id:Pensioenwet-20250301-10b-6,
            id:Pensioenwet-20250301-10b-7,
            id:Pensioenwet-20250301-10b-8,
            id:Pensioenwet-20250301-10b-9 ;
        def:hasTitle "Flexibele premieovereenkomst"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b"@nl .

    id:Pensioenwet-20250301-10b-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10b&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij een flexibele premieovereenkomst wordt de premie belegd tot de pensioendatum. Het kapitaal voortvloeiend uit de premies wordt vanaf de pensioendatum gebruikt voor financiering van een vastgestelde of variabele uitkering."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b, lid 1"@nl .

    id:Pensioenwet-20250301-10b-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10b&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een risicodelingsreserve kan onderdeel zijn van een flexibele premieovereenkomst."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b, lid 2"@nl .

    id:Pensioenwet-20250301-10b-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10b&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij de variabele uitkering worden financiële mee- of tegenvallers die het gevolg zijn van het beleggingsrisico verwerkt door middel van individuele toedeling of een collectief toedelingsmechanisme. De verwerking van financiële mee- of tegenvallers die het gevolg zijn van de ontwikkeling van de levensverwachting gebeurt door middel van individuele toedeling, een collectief toedelingsmechanisme of een risicodelingsreserve. De verwerking van financiële mee- of tegenvallers die het gevolg zijn van het sterfteresultaat gebeurt door middel van een collectief toedelingsmechanisme of een risicodelingsreserve."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b, lid 3"@nl .

    id:Pensioenwet-20250301-10b-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10b&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De vormgeving van het collectief toedelingsmechanisme voor het beleggingsrisico is zodanig dat er op voorhand geen herverdelingseffecten plaatsvinden. Van de eerste zin kan afgeweken worden voor zover dat nodig is om gelijke aanpassingen van de ingegane pensioenuitkeringen en van de opgebouwde aanspraak op nabestaandenpensioen van pensioengerechtigden te realiseren en alleen herverdelingseffecten optreden tussen de pensioengerechtigden onderling."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b, lid 4"@nl .

    id:Pensioenwet-20250301-10b-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10b&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het kapitaal is niet negatief."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b, lid 5"@nl .

    id:Pensioenwet-20250301-10b-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10b&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De pensioenuitvoerder past een collectief toedelingsmechanisme toe op een toedelingskring die bestaat uit pensioengerechtigden."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b, lid 6"@nl .

    id:Pensioenwet-20250301-10b-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10b&lid=7&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. deelnemers of gewezen deelnemers deel uitmaken van de toedelingskring in de laatste tien jaar voorafgaand aan de reglementaire pensioenleeftijd, waarbij de deelname van deelnemers of gewezen deelnemers aan de toedelingskring plaatsvindt door tijdsevenredige toetreding tot het collectief toedelingsmechanisme in deze periode; en"@nl,
            "b. alle deelnemers, gewezen deelnemers of andere aanspraakgerechtigden deel uitmaken van de toedelingskring voor het collectief toedelingsmechanisme voor verwerking van het sterfteresultaat."@nl ;
        def:hasText "In afwijking van het zesde lid kunnen:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b, lid 7"@nl .

    id:Pensioenwet-20250301-10b-8 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10b&lid=8&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een risicodelingsreserve is onderdeel van de flexibele premieovereenkomst; en"@nl,
            "b. de verwerking van het sterfteresultaat vindt plaats over alle deelnemers, gewezen deelnemers, andere aanspraakgerechtigden en pensioengerechtigden."@nl ;
        def:hasText "Bij uitvoering van een flexibele premieovereenkomst door een verplichtgesteld bedrijfstakpensioenfonds geldt, voor zover het basispensioenregelingen betreft die onder de verplichtstelling vallen, in afwijking van het tweede lid en het zevende lid, het volgende:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b, lid 8"@nl .

    id:Pensioenwet-20250301-10b-9 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10b&lid=9&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel over onder meer de tarieven voor een vastgestelde uitkering."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10b, lid 9"@nl .

    id:Pensioenwet-20250301-10c a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-10c-1,
            id:Pensioenwet-20250301-10c-2,
            id:Pensioenwet-20250301-10c-3,
            id:Pensioenwet-20250301-10c-4,
            id:Pensioenwet-20250301-10c-5,
            id:Pensioenwet-20250301-10c-6,
            id:Pensioenwet-20250301-10c-7 ;
        def:hasTitle "Premie-uitkeringsovereenkomst"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10c"@nl .

    id:Pensioenwet-20250301-10c-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10c&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij een premie-uitkeringsovereenkomst wordt de premie belegd tot de pensioendatum. Het kapitaal voortvloeiend uit de premies wordt vanaf de pensioendatum gebruikt voor financiering van een vastgestelde of variabele uitkering."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10c, lid 1"@nl .

    id:Pensioenwet-20250301-10c-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10c&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002221&artikel=7a"^^xsd:string ;
        def:hasText "In afwijking van het eerste lid is de verzekeraar verplicht om op verzoek van de deelnemer of gewezen deelnemer in de laatste 15 jaar voor de pensioengerechtigde leeftijd, bedoeld in artikel 7a, eerste lid, van de Algemene Ouderdomswet, het tot op dat moment opgebouwde kapitaal of de vanaf dat moment beschikbaar gestelde premie geheel of gedeeltelijk aan te wenden voor een aanspraak op een vastgestelde uitkering vanaf de pensioendatum."@nl ;
        def:referencesTo id:AlgemeneOuderdomswet-20250101-7a ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10c, lid 2"@nl .

    id:Pensioenwet-20250301-10c-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10c&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasExtref "jci1.3:c:BWBR0002221&artikel=7a"^^xsd:string ;
        def:hasText "In afwijking van het eerste lid is de premiepensioeninstelling verplicht om op verzoek van de deelnemer of gewezen deelnemer in de laatste vijftien jaar voor de pensioengerechtigde leeftijd, bedoeld in artikel 7a, eerste lid, van de Algemene Ouderdomswet, het tot op dat moment opgebouwde kapitaal geheel of gedeeltelijk over te dragen aan een door de deelnemer of gewezen deelnemer aangewezen verzekeraar voor een aanspraak op een vastgestelde uitkering vanaf de pensioendatum."@nl ;
        def:referencesTo id:AlgemeneOuderdomswet-20250101-7a ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10c, lid 3"@nl .

    id:Pensioenwet-20250301-10c-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10c&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De verzekeraar of de premiepensioeninstelling informeert degene die het kapitaal of de premie wenst aan te wenden als bedoeld in het tweede lid of over te laten dragen als bedoeld in het derde lid over de voor hem relevante gevolgen en risico’s hierbij, waaronder een opgave van de hoogte van de vastgestelde uitkeringen indien het kapitaal of de premie daarvoor zou worden aangewend of overgedragen en de hoogte van de variabele uitkeringen als wordt belegd tot de pensioendatum. Deze opgaven worden tevens weergegeven op basis van een pessimistisch scenario, een verwacht scenario en een optimistisch scenario."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10c, lid 4"@nl .

    id:Pensioenwet-20250301-10c-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10c&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij de variabele uitkering worden financiële mee- of tegenvallers die het gevolg zijn van het beleggingsrisico of van de ontwikkeling van de levensverwachting verwerkt door middel van individuele toedeling."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10c, lid 5"@nl .

    id:Pensioenwet-20250301-10c-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10c&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het kapitaal is niet negatief."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10c, lid 6"@nl .

    id:Pensioenwet-20250301-10c-7 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10c&lid=7&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur worden nadere regels gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10c, lid 7"@nl .

    id:Pensioenwet-20250301-10d a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-10d-1,
            id:Pensioenwet-20250301-10d-2,
            id:Pensioenwet-20250301-10d-3,
            id:Pensioenwet-20250301-10d-4,
            id:Pensioenwet-20250301-10d-5 ;
        def:hasTitle "Solidariteitsreserve"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10d"@nl .

    id:Pensioenwet-20250301-10d-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10d&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een solidariteitsreserve heeft een maximale omvang van 15% van het geheel voor pensioen gereserveerde vermogen inclusief de solidariteitsreserve. De solidariteitsreserve is niet negatief."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10d, lid 1"@nl .

    id:Pensioenwet-20250301-10d-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10d&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien een solidariteitsreserve wordt gevuld uit premies of overrendement, bedraagt de inleg uit premie niet meer dan 10% van de premiesom per deelnemer per jaar en bedraagt de inleg uit overrendement niet meer dan 10% van het positieve collectieve overrendement per jaar."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10d, lid 2"@nl .

    id:Pensioenwet-20250301-10d-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10d&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Door middel van de solidariteitsreserve worden financiële mee- of tegenvallers collectief gedeeld op een wijze die op voorhand leidt tot gemiddeld stabielere of hogere toekomstige en al ingegane pensioenuitkeringen voor alle generaties, ten opzichte van de aanname dat de solidariteitsreserve geen onderdeel van de pensioenregeling zou zijn. De solidariteitsreserve wordt niet gebruikt voor deling van operationele kosten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10d, lid 3"@nl .

    id:Pensioenwet-20250301-10d-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10d&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De pensioenuitvoerder stelt regels vast voor de doelstellingen van de solidariteitsreserve en het vullen en uitdelen uit de solidariteitsreserve. Deze regels zijn evenwichtig, transparant, onderling consistent en worden voor langere tijd vastgesteld."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10d, lid 4"@nl .

    id:Pensioenwet-20250301-10d-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10d&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen nadere regels worden gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10d, lid 5"@nl .

    id:Pensioenwet-20250301-10e a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-10e-1,
            id:Pensioenwet-20250301-10e-2,
            id:Pensioenwet-20250301-10e-3,
            id:Pensioenwet-20250301-10e-4,
            id:Pensioenwet-20250301-10e-5,
            id:Pensioenwet-20250301-10e-6 ;
        def:hasTitle "Risicodelingsreserve"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10e"@nl .

    id:Pensioenwet-20250301-10e-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10e&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een risicodelingsreserve heeft een maximale omvang van 15% van het geheel voor pensioen gereserveerde kapitaal inclusief de risicodelingsreserve. De risicodelingsreserve is niet negatief."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10e, lid 1"@nl .

    id:Pensioenwet-20250301-10e-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10e&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien een risicodelingsreserve wordt gevuld uit premies of uit kapitaal bij toetreding tot het collectief toedelingsmechanisme voor de collectieve uitkeringsfase, bedraagt de inleg uit premie en de inleg uit kapitaal in totaal niet meer dan 10%."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10e, lid 2"@nl .

    id:Pensioenwet-20250301-10e-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10e&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Door middel van de risicodelingsreserve worden financiële mee- of tegenvallers collectief gedeeld op een wijze die op voorhand leidt tot gemiddeld stabielere of hogere toekomstige en al ingegane pensioenuitkeringen voor alle generaties, ten opzichte van de situatie dat de risicodelingsreserve geen onderdeel van de pensioenregeling is. De risicodelingsreserve wordt niet gebruikt voor deling van operationele kosten."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10e, lid 3"@nl .

    id:Pensioenwet-20250301-10e-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10e&lid=4&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij een flexibele premieovereenkomst met beleggingsvrijheid worden financiële mee- of tegenvallers als gevolg van het beleggingsrisico niet gecompenseerd door de risicodelingsreserve."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10e, lid 4"@nl .

    id:Pensioenwet-20250301-10e-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10e&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De pensioenuitvoerder stelt regels vast voor de doelstellingen van de risicodelingsreserve en het vullen en uitdelen uit de risicodelingsreserve. Deze regels zijn evenwichtig, transparant, onderling consistent en worden voor langere tijd vastgesteld."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10e, lid 5"@nl .

    id:Pensioenwet-20250301-10e-6 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=2&paragraaf=2.2&artikel=10e&lid=6&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij of krachtens algemene maatregel van bestuur kunnen nadere regels worden gesteld met betrekking tot dit artikel."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 10e, lid 6"@nl .

    id:Pensioenwet-20250301-11 a def:LawArticle ;
        def:hasDescription " De uitkering, het kapitaal, voor pensioenuitkering bestemd vermogen en de premie in het kader van een pensioenovereenkomst luiden in een Nederlands wettig betaalmiddel."@nl ;
        def:hasTitle "Uitkering, kapitaal, voor pensioenuitkering bestemd vermogen of premie"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 11"@nl .

    id:Pensioenwet-20250301-110 a def:LawArticle ;
        def:hasDescription " Iedere bepaling die een goedkeuringsrecht inhoudt van een partij, die geen orgaan is van het pensioenfonds, inzake een besluit of voorgenomen besluit van het pensioenfonds is nietig, tenzij in deze wet anders is bepaald."@nl ;
        def:hasTitle "Goedkeuringsrecht"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 110"@nl .

    id:Pensioenwet-20250301-111 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-111-1,
            id:Pensioenwet-20250301-111-2,
            id:Pensioenwet-20250301-111-3 ;
        def:hasTitle "Statuten"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 111"@nl .

    id:Pensioenwet-20250301-111-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=111&lid=1&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. het doel van het pensioenfonds, waaronder een omschrijving van de werkingssfeer;"@nl,
            "b. de bestemming van de middelen van het pensioenfonds;"@nl,
            "c. het beheer van het pensioenfonds;"@nl,
            "d. de inkomsten van het pensioenfonds;"@nl,
            "e. de belegging van de gelden;"@nl,
            "f. het bestuursmodel van het pensioenfonds, bedoeld in artikel 99;"@nl,
            "g. de wijze waarop de bestuurders worden benoemd en ontslagen;"@nl,
            "h. de wijze waarop het intern toezicht is georganiseerd;"@nl,
            "i. de wijze waarop de leden van de raad van toezicht dan wel de visitatiecommissie worden benoemd en ontslagen;"@nl,
            "j. de wijze waarop de leden van het verantwoordingsorgaan dan wel het belanghebbendenorgaan worden benoemd en ontslagen;"@nl,
            "k. de wijziging van de statuten;"@nl,
            "l. de liquidatie van het pensioenfonds, waaronder begrepen de verplichtingen van de liquidateuren en de bestemming van de bezittingen van het pensioenfonds; en"@nl,
            "m. de toepassing van artikel 105, derde lid."@nl ;
        def:hasText "In de statuten van een pensioenfonds worden in ieder geval bepalingen opgenomen betreffende:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 111, lid 1"@nl .

    id:Pensioenwet-20250301-111-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=111&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De omschrijving van de werkingssfeer, bedoeld in het eerste lid, onderdeel a, vindt ten aanzien van een bedrijfstakpensioenfonds plaats door het omschrijven van de bedrijfsactiviteiten van de bedrijfstak."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 111, lid 2"@nl .

    id:Pensioenwet-20250301-111-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=111&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Een bedrijfstakpensioenfonds dat aan werkgevers de mogelijkheid biedt om zich vrijwillig aan te sluiten bepaalt in zijn statuten onder welke voorwaarden deze vrijwillige aansluiting mogelijk is."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 111, lid 3"@nl .

    id:Pensioenwet-20250301-112 a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-112-1,
            id:Pensioenwet-20250301-112-2,
            id:Pensioenwet-20250301-112-3 ;
        def:hasTitle "Melding oprichting van een ondernemingspensioenfonds of bedrijfstakpensioenfonds"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112"@nl .

    id:Pensioenwet-20250301-112-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "De oprichters melden de voorgenomen oprichting van een ondernemingspensioenfonds of bedrijfstakpensioenfonds uiterlijk zes weken voor de beoogde datum van oprichting aan de toezichthouder."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112, lid 1"@nl .

    id:Pensioenwet-20250301-112-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112&lid=2&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Het ondernemingspensioenfonds of bedrijfstakpensioenfonds meldt binnen drie maanden na zijn oprichting deze oprichting aan de toezichthouder."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112, lid 2"@nl .

    id:Pensioenwet-20250301-112-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=5&paragraaf=5.1&artikel=112&lid=3&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. een authentiek afschrift van de akte van oprichting van het pensioenfonds;"@nl,
            "b. een door het bestuur gewaarmerkt exemplaar van het reglement of de reglementen van het pensioenfonds;"@nl,
            "c. een door het bestuur gewaarmerkt afschrift van de uitvoeringsovereenkomst;"@nl,
            "d. een actuariële en bedrijfstechnische nota als bedoeld in artikel 145; en"@nl,
            "e. een eventuele overeenkomst tot verzekering, overdracht of onderbrenging."@nl ;
        def:hasText "Bij de melding, bedoeld in het tweede lid, worden gevoegd:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 112, lid 3"@nl .

    id:Pensioenwet-20250301-112a a def:LawArticle ;
        def:hasSection id:Pensioenwet-20250301-112a-1,
            id:Pensioenwet-20250301-112a-2,
            id:Pensioenwet-20250301-112a-3,
            id:Pensioenwet-20250301-112a-4,
            id:Pensioenwet-20250301-112a-5,

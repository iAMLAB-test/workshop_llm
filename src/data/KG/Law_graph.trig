            id:Pensioenwet-20250301-58-4,
            id:Pensioenwet-20250301-58-5 ;
        def:hasTitle "Gelijke behandeling bij toeslagen"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 58"@nl .

    id:Pensioenwet-20250301-58-1 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=4&paragraaf=4.2&artikel=58&lid=1&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien een ouderdomspensioenrecht van een gepensioneerde die geen gewezen deelnemer is geweest wordt verhoogd door middel van een toeslag, wordt het ouderdomspensioenrecht van een gepensioneerde die wel gewezen deelnemer is geweest in dezelfde mate verhoogd indien zij in dezelfde pensioenregeling hebben deelgenomen."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 58, lid 1"@nl .

    id:Pensioenwet-20250301-58-2 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=4&paragraaf=4.2&artikel=58&lid=2&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. ten behoeve van de partners van overleden gepensioneerden die gewezen deelnemer zijn geweest;"@nl,
            "b. ten behoeve van de partners van overleden gewezen deelnemers;"@nl,
            "c. ten behoeve van de partners van overleden deelnemers; en"@nl,
            "d. van de gewezen partners met een bijzonder partnerpensioen;"@nl ;
        def:hasText "Indien een recht op partnerpensioen van de partner van een overleden gepensioneerde die geen gewezen deelnemer is geweest wordt verhoogd door middel van een toeslag, worden de partnerpensioenrechten:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 58, lid 2"@nl .

    id:Pensioenwet-20250301-58-3 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=4&paragraaf=4.2&artikel=58&lid=3&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Indien een ouderdomspensioenrecht wordt verhoogd door middel van een toeslag, wordt de aanspraak op ouderdomspensioen van een gewezen deelnemer die in dezelfde pensioenregeling heeft deelgenomen in dezelfde mate verhoogd."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 58, lid 3"@nl .

    id:Pensioenwet-20250301-58-4 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=4&paragraaf=4.2&artikel=58&lid=4&o=a&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasCases "a. ten behoeve van de partner van een gepensioneerde die wel gewezen deelnemer is geweest;"@nl,
            "b. ten behoeve van de partner van een gewezen deelnemer; en"@nl,
            "c. van de gewezen partner van de gewezen deelnemer met een bijzonder partnerpensioen;"@nl ;
        def:hasText "Indien een aanspraak op partnerpensioen van een gepensioneerde die geen gewezen deelnemer is geweest wordt verhoogd door middel van een toeslag, worden de partnerpensioenaanspraken:"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 58, lid 4"@nl .

    id:Pensioenwet-20250301-58-5 a def:LawArticleSection ;
        dcterms:source "https://wetten.overheid.nl/jci1.3:c:BWBR0020809&hoofdstuk=4&paragraaf=4.2&artikel=58&lid=5&z=2025-03-01&g=2025-03-01"^^xsd:anyURI ;
        def:hasText "Bij de verlening van toeslagen op partnerpensioen wordt geen onderscheid gemaakt tussen partners."@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 58, lid 5"@nl .

    id:Pensioenwet-20250301-59 a def:LawArticle ;
        def:hasDescription " Een rechtsvordering tegen een pensioenuitvoerder tot het doen van een uitkering verjaart niet bij leven van de pensioengerechtigde."@nl ;
        def:hasTitle "Geen verjaring ten gunste van de pensioenuitvoerder"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 59"@nl .

    id:Pensioenwet-20250301-6 a def:LawArticle ;
        def:hasDescription " De Wet op het financieel toezicht is niet van toepassing op de verhouding tussen een verzekeraar of een premiepensioeninstelling en een aanspraak- of pensioengerechtigde, tenzij in deze wet anders is bepaald."@nl ;
        def:hasTitle "Relatie met"@nl ;
        rdfs1:label "Pensioenwet, 2025-03-01, Artikel 6"@nl .


    id:AlgemeneOuderdomswet-20250101-1 a def:LawArticle ;
        def:hasSection id:AlgemeneOuderdomswet-20250101-1-1,
            id:AlgemeneOuderdomswet-20250101-1-2,
            id:AlgemeneOuderdomswet-20250101-1-3,
